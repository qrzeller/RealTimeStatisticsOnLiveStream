TableFilterTemplate.filter = function(opt_data, opt_ignored) {
  return '' + ((opt_data.type == 'selectfilter') ? '<div class="filter-container selectfilter-element sortable"><label class="hideble"><span class="tf-label-name" title="' + soy.$$escapeHtml("Double-click to edit the filter label") + '">' + soy.$$escapeHtml(opt_data.column) + '</span><span class="eq" title="' + soy.$$escapeHtml("Click to invert the filter") + '">&nbsp;=</span><span class="not-eq" title="' + soy.$$escapeHtml("Click to invert the filter") + '">&nbsp;&ne;</span></label><select class="select" name="' + soy.$$escapeHtml(opt_data.column) + '" style="width: 150px;" multiple data-width="150px"></select></div>' : (opt_data.type == 'userfilter') ? '<div class="filter-container userfilter-element sortable input"><label class="hideble"><span class="tf-label-name" title="' + soy.$$escapeHtml("Double-click to edit the filter label") + '">' + soy.$$escapeHtml(opt_data.column) + '</span><span class="eq" title="' + soy.$$escapeHtml("Click to invert the filter") + '">&nbsp;=</span><span class="not-eq" title="' + soy.$$escapeHtml("Click to invert the filter") + '">&nbsp;&ne;</span></label><div class="tf-resizeable" style="width: 150px;"><input class="user-filter text" type="text" name="' + soy.$$escapeHtml(opt_data.column) + '" style="width: 100%; max-width: none;" placeholder="' + soy.$$escapeHtml("Start typing...\u200b") + '" data-width="150px"/></div></div>' : (opt_data.type == 'numberfilter') ? '<div class="filter-container numberfilter-element sortable"><label class="hideble"><span class="tf-label-name" title="' + soy.$$escapeHtml("Double-click to edit the filter label") + '">' + soy.$$escapeHtml(opt_data.column) + '</span><span class="eq" title="' + soy.$$escapeHtml("Click to invert the filter") + '">&nbsp;=</span><span class="not-eq" title="' + soy.$$escapeHtml("Click to invert the filter") + '">&nbsp;&ne;</span></label><div class="range numbers" name="' + soy.$$escapeHtml(opt_data.column) + '">' + soy.$$escapeHtml("from") + '<input class="number from text" type="text" name="' + soy.$$escapeHtml(opt_data.column) + '" style="width: 90px;" placeholder="' + soy.$$escapeHtml("Type value") + '"/>' + soy.$$escapeHtml("to") + '<input class="number to text" type="text" name="' + soy.$$escapeHtml(opt_data.column) + '" style="width: 90px;" placeholder="' + soy.$$escapeHtml("Type value") + '"/></div></div>' : (opt_data.type == 'datefilter') ? '<div class="filter-container datefilter-element sortable"><label class="hideble"><span class="tf-label-name" title="' + soy.$$escapeHtml("Double-click to edit the filter label") + '">' + soy.$$escapeHtml(opt_data.column) + '</span><span class="eq" title="' + soy.$$escapeHtml("Click to invert the filter") + '">&nbsp;=</span><span class="not-eq" title="' + soy.$$escapeHtml("Click to invert the filter") + '">&nbsp;&ne;</span></label><div class="range dates" name="' + soy.$$escapeHtml(opt_data.column) + '">' + soy.$$escapeHtml("from") + '<input class="date from text" type="text" name="' + soy.$$escapeHtml(opt_data.column) + '" style="width: 93px;" placeholder="' + soy.$$escapeHtml("Pick date") + '"/>' + soy.$$escapeHtml("to") + '<input class="date to text" type="text" name="' + soy.$$escapeHtml(opt_data.column) + '" style="width: 93px;" placeholder="' + soy.$$escapeHtml("Pick date") + '"/></div></div>' : (opt_data.type == 'globalfilter') ? '<div class="filter-container globalfilter-element sortable input"><label class="hideble"><span class="tf-label-name" title="' + soy.$$escapeHtml("Double-click to edit the filter label") + '">' + soy.$$escapeHtml("Global filter") + '</span><span class="eq" title="' + soy.$$escapeHtml("Click to invert the filter") + '">&nbsp;=</span><span class="not-eq" title="' + soy.$$escapeHtml("Click to invert the filter") + '">&nbsp;&ne;</span></label><div class="tf-resizeable" style="width: 150px;"><input class="global-filter text" type="text" style="width: 100%; max-width: none;" placeholder="' + soy.$$escapeHtml("Start typing...\u200b") + '" data-width="150px"/></div></div>' : (opt_data.type == 'columnhider') ? '<div class="filter-container columnhider-element sortable"><label class="hideble"><span class="tf-label-name" title="' + soy.$$escapeHtml("Double-click to edit the filter label") + '">' + soy.$$escapeHtml("Hide columns") + '</span><span class="eq" title="' + soy.$$escapeHtml("Click to invert the filter") + '">&nbsp;=</span><span class="not-eq" title="' + soy.$$escapeHtml("Click to invert the filter") + '">&nbsp;&ne;</span></label><select class="column-select" style="width: 150px;" multiple data-width="150px"></select></div>' : (opt_data.type == 'iconfilter') ? '<div class="filter-container iconfilter-element sortable"><label class="hideble"><span class="tf-label-name" title="' + soy.$$escapeHtml("Double-click to edit the filter label") + '">' + soy.$$escapeHtml(opt_data.column) + '</span><span class="eq" title="' + soy.$$escapeHtml("Click to invert the filter") + '">&nbsp;=</span><span class="not-eq" title="' + soy.$$escapeHtml("Click to invert the filter") + '">&nbsp;&ne;</span></label><select class="icon-select" name="' + soy.$$escapeHtml(opt_data.column) + '" style="width: 150px;" multiple data-width="150px" data-html="true"></select></div>' : '');
};
if (goog.DEBUG) {
  TableFilterTemplate.filter.soyTemplateName = 'TableFilterTemplate.filter';
}


TableFilterTemplate.panel = function(opt_data, opt_ignored) {
  var output = '' + ((opt_data.hideLabels) ? '<style type="text/css">#comboBoxDiv-' + soy.$$escapeHtml(opt_data.macroId) + ' label.hideble {display: none;}#comboBoxDiv-' + soy.$$escapeHtml(opt_data.macroId) + ' .filter-container.sortable {padding-left: 5px;}</style>' : '') + ((opt_data.hidePane) ? '<style type="text/css">#comboBoxDiv-' + soy.$$escapeHtml(opt_data.macroId) + ', #tf-shower-' + soy.$$escapeHtml(opt_data.macroId) + ' {display: none !important;}</style>' : '') + ((opt_data.limitHeight) ? '<style type="text/css">#tf-table-' + soy.$$escapeHtml(opt_data.macroId) + ' thead.tableFloatingHeaderOriginal {position: static !important;}#tf-table-' + soy.$$escapeHtml(opt_data.macroId) + ' thead.tableFloatingHeaderOriginal th {min-width: 0 !important; max-width: 100% !important;}#tf-table-' + soy.$$escapeHtml(opt_data.macroId) + ' thead.tableFloatingHeader {display: none !important;}</style>' : '') + ((opt_data.hideControls) ? '<style type="text/css">#comboBoxDiv-' + soy.$$escapeHtml(opt_data.macroId) + ' .lockDisabled.not-empty {min-width: 0 !important;}#comboBoxDiv-' + soy.$$escapeHtml(opt_data.macroId) + ' .tf-form .filter-logo {display: none;}#comboBoxDiv-' + soy.$$escapeHtml(opt_data.macroId) + ' .tf-form .filter-buttons-container {display: none !important;}#comboBoxDiv-' + soy.$$escapeHtml(opt_data.macroId) + ' form.aui.tf-form {padding-right: 0 !important;}#comboBoxDiv-' + soy.$$escapeHtml(opt_data.macroId) + ' .tf-hider {display: none !important;}</style>' : '') + '<div class="tablefilter-outer-wrapper" data-id="' + soy.$$escapeHtml(opt_data.macroId) + '" data-pageid="' + soy.$$escapeHtml(opt_data.pageId) + '">';
  if (! opt_data.isEmail) {
    output += '<div id="comboBoxDiv-' + soy.$$escapeHtml(opt_data.macroId) + '" class="tableFilterCbStyle"><div class="lockDisabled not-empty"><form class="aui tf-form" name="inlinecommentform" onsubmit="return false;" onkeydown="return event.keyCode != 13;"><div class="filter-container filter-icon filter-logo default ' + ((opt_data.fd) ? 'tf-fd' : '') + '" title="' + soy.$$escapeHtml(AJS.format("Nothing filtered. Showing {0} rows",0)) + '"></div><div class="filter-buttons-container"><div class="tf-menu-buttons aui-toolbar2" role="toolbar"><div class="aui-toolbar2-inner"><div class="aui-toolbar2-primary"><div class="tf-btn-container"><div class="tf-btn-row"><button class="filter-reset aui-button aui-button-subtle" title="' + soy.$$escapeHtml("Reset all filters") + '"></button><button class="disableLock aui-button aui-button-subtle" title="' + soy.$$escapeHtml("Disable all filters") + '"></button></div><div id="tf-settings-row-' + soy.$$escapeHtml(opt_data.macroId) + '" class="tf-btn-row"><div class="tf-settings-wrapper"><button id=\'filter-settings-' + soy.$$escapeHtml(opt_data.macroId) + '\' class="filter-settings aui-button aui-button-subtle aui-dropdown2-trigger" aria-owns="filter-menu-view-' + soy.$$escapeHtml(opt_data.macroId) + '" aria-haspopup="true" data-container="" resolved="" aria-expanded="false"><span class="aui-icon aui-icon-small aui-iconfont-configure"></span></button><div id="filter-menu-view-' + soy.$$escapeHtml(opt_data.macroId) + '" class="aui-style-default aui-dropdown2" style="display: none; z-index: 3001;"><div class="aui-dropdown2-section"><ul class="aui-list-truncate first">' + ((opt_data.editable && ! opt_data.disableSave) ? '<li><a class="filter-save cursor-pointer tf-disabled-link">' + soy.$$escapeHtml("Save changes") + '</a></li>' : '') + '<li><a class="filter-add cursor-pointer">' + soy.$$escapeHtml("Add filter") + '</a></li><li><a class="filter-sort-reset cursor-pointer tf-disabled-link">' + soy.$$escapeHtml("Reset column sorting") + '</a></li><li><a class="export-file-pdf cursor-pointer">' + soy.$$escapeHtml("Export to PDF") + '</a></li><li><a class="export-file-csv cursor-pointer">' + soy.$$escapeHtml("Export to CSV") + '</a></li><li><a class="export-file-doc cursor-pointer">' + soy.$$escapeHtml("Export to Word") + '</a></li><li><a class="get-tf-url cursor-pointer" ' + ((opt_data.isCloud) ? 'style="display: none;"' : '') + '>' + soy.$$escapeHtml("Copy the filter URL") + '</a></li><li><a class="tf-change-settings cursor-pointer">' + soy.$$escapeHtml("Modify Settings") + '</a></li></ul></div><div class="aui-dropdown2-section"><ul class="aui-list-truncate"><li><a target="_blank" href="https://docs.stiltsoft.com/display/TableFilter/How+to+use+Table+filter+macro?from=tf-view">' + soy.$$escapeHtml("Documentation") + '</a></li><li><a target="_blank" href="https://docs.stiltsoft.com/display/TableFilter/Table+Filter+and+Charts+3.9.0?from=tf-view">' + soy.$$escapeHtml("What\x27s new") + '</a></li></ul></div></div></div></div></div></div></div></div></div><div class="sortable-container">';
    var comboList286 = opt_data.comboBoxes;
    var comboListLen286 = comboList286.length;
    for (var comboIndex286 = 0; comboIndex286 < comboListLen286; comboIndex286++) {
      var comboData286 = comboList286[comboIndex286];
      output += '<div class="filter-container selectfilter-element sortable ' + ((comboData286.inverse) ? 'inverse' : '') + '"><label class="hideble"><span class="tf-label-name" title="' + soy.$$escapeHtml("Double-click to edit the filter label") + '">' + soy.$$escapeHtml(comboData286.label) + '</span><span class="eq" title="' + soy.$$escapeHtml("Click to invert the filter") + '">&nbsp;=</span><span class="not-eq" title="' + soy.$$escapeHtml("Click to invert the filter") + '">&nbsp;&ne;</span></label><select class="select ' + ((comboData286.inverse) ? 'inverse' : '') + '" name="' + soy.$$escapeHtml(comboData286.name) + '" style="width: ' + soy.$$escapeHtml(comboData286.columnWidth) + 'px;" multiple data-width="' + soy.$$escapeHtml(comboData286.columnWidth) + 'px">';
      var optList310 = comboData286.options;
      var optListLen310 = optList310.length;
      for (var optIndex310 = 0; optIndex310 < optListLen310; optIndex310++) {
        var optData310 = optList310[optIndex310];
        output += '<option selected="selected"' + ((optData310 == '') ? 'class="tf-empty-option">' + soy.$$escapeHtml("-Empty-") : '>' + soy.$$escapeHtml(optData310)) + '</option>';
      }
      output += '</select></div>';
    }
    var editList323 = opt_data.edits;
    var editListLen323 = editList323.length;
    for (var editIndex323 = 0; editIndex323 < editListLen323; editIndex323++) {
      var editData323 = editList323[editIndex323];
      output += '<div class="filter-container userfilter-element sortable input ' + ((editData323.inverse) ? 'inverse' : '') + '"><label class="hideble"><span class="tf-label-name" title="' + soy.$$escapeHtml("Double-click to edit the filter label") + '">' + soy.$$escapeHtml(editData323.label) + '</span><span class="eq" title="' + soy.$$escapeHtml("Click to invert the filter") + '">&nbsp;=</span><span class="not-eq" title="' + soy.$$escapeHtml("Click to invert the filter") + '">&nbsp;&ne;</span></label><div class="tf-resizeable" style="width: ' + soy.$$escapeHtml(editData323.columnWidth) + 'px;"><input class="user-filter text ' + ((editData323.inverse) ? 'inverse' : '') + '" type="text" name="' + soy.$$escapeHtml(editData323.name) + '" style="width: 100%; max-width: none;" value="' + soy.$$escapeHtml(editData323.defaultValue) + '" placeholder="' + soy.$$escapeHtml("Start typing...\u200b") + '" data-width="' + soy.$$escapeHtml(editData323.columnWidth) + 'px"/></div></div>';
    }
    var editList352 = opt_data.numberFilters;
    var editListLen352 = editList352.length;
    for (var editIndex352 = 0; editIndex352 < editListLen352; editIndex352++) {
      var editData352 = editList352[editIndex352];
      output += '<div class="filter-container numberfilter-element sortable ' + ((editData352.inverse) ? 'inverse' : '') + '"><label class="hideble"><span class="tf-label-name" title="' + soy.$$escapeHtml("Double-click to edit the filter label") + '">' + soy.$$escapeHtml(editData352.label) + '</span><span class="eq" title="' + soy.$$escapeHtml("Click to invert the filter") + '">&nbsp;=</span><span class="not-eq" title="' + soy.$$escapeHtml("Click to invert the filter") + '">&nbsp;&ne;</span></label><div class="range numbers" name="' + soy.$$escapeHtml(editData352.name) + '">' + soy.$$escapeHtml("from") + '<input class="number from text ' + ((editData352.inverse) ? 'inverse' : '') + '" type="text" name="' + soy.$$escapeHtml(editData352.name) + '" style="width: 90px;" value="' + soy.$$escapeHtml(editData352.defaultFromValue) + '" placeholder="' + soy.$$escapeHtml("Type value") + '"/>' + soy.$$escapeHtml("to") + '<input class="number to text ' + ((editData352.inverse) ? 'inverse' : '') + '" type="text" name="' + soy.$$escapeHtml(editData352.name) + '" style="width: 90px;" value="' + soy.$$escapeHtml(editData352.defaultToValue) + '" placeholder="' + soy.$$escapeHtml("Type value") + '"/></div></div>';
    }
    var editList393 = opt_data.dateFilters;
    var editListLen393 = editList393.length;
    for (var editIndex393 = 0; editIndex393 < editListLen393; editIndex393++) {
      var editData393 = editList393[editIndex393];
      output += '<div class="filter-container datefilter-element sortable ' + ((editData393.inverse) ? 'inverse' : '') + '"><label class="hideble"><span class="tf-label-name" title="' + soy.$$escapeHtml("Double-click to edit the filter label") + '">' + soy.$$escapeHtml(editData393.label) + '</span><span class="eq" title="' + soy.$$escapeHtml("Click to invert the filter") + '">&nbsp;=</span><span class="not-eq" title="' + soy.$$escapeHtml("Click to invert the filter") + '">&nbsp;&ne;</span></label><div class="range dates" name="' + soy.$$escapeHtml(editData393.name) + '">' + soy.$$escapeHtml("from") + '<input class="date from text ' + ((editData393.inverse) ? 'inverse' : '') + '" type="text" name="' + soy.$$escapeHtml(editData393.name) + '" style="width: 93px;" value="' + soy.$$escapeHtml(editData393.defaultFromValue) + '" placeholder="' + soy.$$escapeHtml("Pick date") + '"/>' + soy.$$escapeHtml("to") + '<input class="date to text ' + ((editData393.inverse) ? 'inverse' : '') + '" type="text" name="' + soy.$$escapeHtml(editData393.name) + '" style="width: 93px;" value="' + soy.$$escapeHtml(editData393.defaultToValue) + '" placeholder="' + soy.$$escapeHtml("Pick date") + '"/></div></div>';
    }
    var comboList434 = opt_data.iconFilters;
    var comboListLen434 = comboList434.length;
    for (var comboIndex434 = 0; comboIndex434 < comboListLen434; comboIndex434++) {
      var comboData434 = comboList434[comboIndex434];
      output += '<div class="filter-container iconfilter-element sortable ' + ((comboData434.inverse) ? 'inverse' : '') + '"><label class="hideble"><span class="tf-label-name" title="' + soy.$$escapeHtml("Double-click to edit the filter label") + '">' + soy.$$escapeHtml(comboData434.label) + '</span><span class="eq" title="' + soy.$$escapeHtml("Click to invert the filter") + '">&nbsp;=</span><span class="not-eq" title="' + soy.$$escapeHtml("Click to invert the filter") + '">&nbsp;&ne;</span></label><select class="icon-select ' + ((comboData434.inverse) ? 'inverse' : '') + '" name="' + soy.$$escapeHtml(comboData434.name) + '" style="width: ' + soy.$$escapeHtml(comboData434.columnWidth) + 'px;" multiple data-width="' + soy.$$escapeHtml(comboData434.columnWidth) + 'px" data-html="true">';
      var optList458 = comboData434.options;
      var optListLen458 = optList458.length;
      for (var optIndex458 = 0; optIndex458 < optListLen458; optIndex458++) {
        var optData458 = optList458[optIndex458];
        output += '<option selected="selected"' + ((optData458 == '') ? 'class="tf-empty-option">' + soy.$$escapeHtml("-Empty-") : '>' + soy.$$escapeHtml(optData458)) + '</option>';
      }
      output += '</select></div>';
    }
    output += (opt_data.globalFilter) ? '<div class="filter-container globalfilter-element sortable input ' + ((opt_data.globalFilter.inverse) ? 'inverse' : '') + '"><label class="hideble"><span class="tf-label-name" title="' + soy.$$escapeHtml("Double-click to edit the filter label") + '">' + soy.$$escapeHtml(opt_data.globalFilter.label) + '</span><span class="eq" title="' + soy.$$escapeHtml("Click to invert the filter") + '">&nbsp;=</span><span class="not-eq" title="' + soy.$$escapeHtml("Click to invert the filter") + '">&nbsp;&ne;</span></label><div class="tf-resizeable" style="width: ' + soy.$$escapeHtml(opt_data.globalFilter.columnWidth) + 'px;"><input class="global-filter text ' + ((opt_data.globalFilter.inverse) ? 'inverse' : '') + '" type="text" style="width: 100%; max-width: none;" value="' + soy.$$escapeHtml(opt_data.globalFilter.defaultValue) + '" placeholder="' + soy.$$escapeHtml("Start typing...\u200b") + '" data-width="' + soy.$$escapeHtml(opt_data.globalFilter.columnWidth) + 'px"/></div></div>' : '';
    if (opt_data.hideColumns) {
      output += '<div class="filter-container columnhider-element sortable ' + ((opt_data.hideColumns.inverse) ? 'inverse' : '') + '"><label class="hideble"><span class="tf-label-name" title="' + soy.$$escapeHtml("Double-click to edit the filter label") + '">' + soy.$$escapeHtml(opt_data.hideColumns.label) + '</span><span class="eq" title="' + soy.$$escapeHtml("Click to invert the filter") + '">&nbsp;=</span><span class="not-eq" title="' + soy.$$escapeHtml("Click to invert the filter") + '">&nbsp;&ne;</span></label><select class="column-select ' + ((opt_data.hideColumns.inverse) ? 'inverse' : '') + '" style="width: ' + soy.$$escapeHtml(opt_data.hideColumns.columnWidth) + 'px;" multiple data-width="' + soy.$$escapeHtml(opt_data.hideColumns.columnWidth) + 'px">';
      var optList521 = opt_data.hideColumns.options;
      var optListLen521 = optList521.length;
      for (var optIndex521 = 0; optIndex521 < optListLen521; optIndex521++) {
        var optData521 = optList521[optIndex521];
        output += '<option selected="selected"' + ((optData521 == '') ? 'class="tf-empty-option">' + soy.$$escapeHtml("-Empty-") : '>' + soy.$$escapeHtml(optData521)) + '</option>';
      }
      output += '</select></div>';
    }
    output += '</div><div class="filter-container show-n-rows-only-message"><label>' + soy.$$escapeHtml(AJS.format("Showing the first {0} rows",opt_data.showNRows)) + '</label></div><div class="filter-container empty-message"><div class="aui-message info"><p class="title"><span class="aui-icon icon-info"></span></p>' + soy.$$escapeHtml("Filtration is disabled as no filters are defined.") + '</div></div></form></div><div class="lockEnabled"><div class="filter-container filter-icon filter-logo default" title="' + soy.$$escapeHtml(AJS.format("Nothing filtered. Showing {0} rows",0)) + '"></div><div class="lockPosition"><div class="filter-container"><label class="hideble">' + soy.$$escapeHtml("The filter is disabled at the moment.") + '</label></div><div class="tf-menu-buttons aui-toolbar2" role="toolbar"><div class="aui-toolbar2-inner"><div class="aui-toolbar2-primary"><div class="tf-btn-container"><div class="tf-btn-row"><button class="filter-reset aui-button aui-button-subtle" disabled="disabled"></button><button class="enableLock aui-button aui-button-subtle" title="' + soy.$$escapeHtml("Enable all filters") + '"></button></div><div class="tf-btn-row settings-row"><div class="tf-settings-wrapper"><button class="filter-settings aui-button aui-button-subtle aui-dropdown2-trigger tf-disabled-from-editor" disabled="disabled" aria-owns="filter-menu-view-' + soy.$$escapeHtml(opt_data.macroId) + '"><span class="aui-icon aui-icon-small aui-iconfont-configure"></span></button></div></div></div></div></div></div></div></div><div class="filter-container no-table-message"><div class="aui-message warning"><p class="title"><span class="aui-icon icon-warning"></span></p>' + soy.$$escapeHtml("Probably, there is no table within the Table Filter macro. Please place the table or the macro generating a table within it. Once this is done, configure the filtration parameters.") + '</div></div><div class="tf-hider-wrapper" title="' + soy.$$escapeHtml("Hide filtration pane") + '"><div class="tf-hider"><span class="tf-hide-show-icon"></span></div></div></div><div id="tf-shower-' + soy.$$escapeHtml(opt_data.macroId) + '" class="tf-shower-wrapper" title="' + soy.$$escapeHtml("Show filtration pane") + '"><div class="tf-shower"><span class="tf-hide-show-icon"></span></div></div>';
  }
  output += ((! opt_data.isEditor) ? '<div id="tf-table-' + soy.$$escapeHtml(opt_data.macroId) + '" class="tablefilter-table-wrapper ' + ((opt_data.isCloud) ? 'tf-cloud-table' : '') + '">' + soy.$$filterNoAutoescape(opt_data.body) + '</div>' : '') + '</div>';
  return output;
};
if (goog.DEBUG) {
  TableFilterTemplate.panel.soyTemplateName = 'TableFilterTemplate.panel';
}


TableFilterTemplate.fail = function(opt_data, opt_ignored) {
  return '<div class="tablefilter-outer-wrapper error"><div class="tableFilterCbStyle"><div class="filter-container"><div class="aui-message warning"><p class="title"><span class="aui-icon icon-warning"></span></p>' + ((opt_data.failureCause == 'no_table') ? "Probably, there is no table within the Table Filter macro. Please place the table or the macro generating a table within it. Once this is done, configure the filtration parameters." : (opt_data.failureCause == 'license') ? AJS.format("Table Filter Plugin license is not active, please \x3ca href\x3d\x22{0}/plugins/servlet/upm#manage/com.stiltsoft.confluence.plugin.tablefilter.tablefilter\x22\x3egenerate a trial license or purchase\x3c/a\x3e the product.",opt_data.contextPath) : (opt_data.failureCause == 'bad_request') ? "Oops! We are sorry but the Table Filter macro cannot be previewed on Confluence page." : "Oops! We are sorry but the current complex table cannot be filtered.") + '</div></div></div><div class="tablefilter-table-wrapper">' + opt_data.body + '</div></div>';
};
if (goog.DEBUG) {
  TableFilterTemplate.fail.soyTemplateName = 'TableFilterTemplate.fail';
}


TableFilterTemplate.macrobrowserTabs = function(opt_data, opt_ignored) {
  return '<div class="aui-tabs horizontal-tabs tf-macrobrowser-tabs"><ul class="tabs-menu"><li id="tab-filters" class="menu-item"><a href="#">' + "Filters" + '</a></li><li id="tab-filter-options" class="menu-item"><a href="#">' + "Filtration" + '</a></li><li id="tab-table-options" class="menu-item"><a htef="#">' + "Table View" + '</a></li></ul><div class="tabs-pane active-pane"></div></div>';
};
if (goog.DEBUG) {
  TableFilterTemplate.macrobrowserTabs.soyTemplateName = 'TableFilterTemplate.macrobrowserTabs';
}


TableFilterTemplate.macro = function(opt_data, opt_ignored) {
  return '<div class="dialog-panel-body macro-input-panel"><div id="macro-insert-container" style="display: block;"><input class="macro-name" type="hidden" value="table-filter"><div class="macro-input-fields dialog-panel"><form action="#" class="aui"><div class="macro-desc"><p>' + "Macro adds filters for table data." + '</p>' + "Post \x3ca href\x3d\x22https://stiltsoft.customercase.com/forums/table-filter-for-confluence/ideas/create?from\x3dmacro\x22\x3eIdea\x3c/a\x3e or \x3ca href\x3d\x22https://stiltsoft.customercase.com/forums/support-requests/ideas/create?from\x3dmacro\x26category\x3dTable%2BFilter%2Band%2BCharts%2Bfor%2BConfluence\x22\x3eIssue\x3c/a\x3e." + ' ' + "\x3ca class\x3d\x22macro-doco-link\x22 href\x3d\x22https://docs.stiltsoft.com/display/TableFilter/How+to+use+Table+filter+macro?from\x3dmacro\x22\x3eDocumentation\x3c/a\x3e" + '</div><div class="macro-param-div" id="macro-param-div-column"><label for="macro-param-column">' + "Dropdown filter columns" + '</label><input type="text" class="text macro-param-input" id="macro-param-column"></div><div class="macro-param-div" id="macro-param-div-userfilter"><label for="macro-param-userfilter">' + "Free text filter columns" + '</label><input type="text" class="text macro-param-input" id="macro-param-userfilter"></div><div class="macro-param-div" id="macro-param-div-numberfilter"><label for="macro-param-numberfilter">' + "Number range filter columns" + '</label><input type="text" class="text macro-param-input" id="macro-param-numberfilter"></div><div class="macro-param-div" id="macro-param-div-datefilter"><label for="macro-param-datefilter">' + "Date range filter columns" + '</label><input type="text" class="text macro-param-input" id="macro-param-datefilter"></div><div class="macro-param-div" id="macro-param-div-iconfilter"><label for="macro-param-iconfilter">' + "Icon, checkbox or image filter columns" + '</label><input type="text" class="text macro-param-input" id="macro-param-iconfilter"></div><div class="c