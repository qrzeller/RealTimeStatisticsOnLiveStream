ChartTemplate.colorDialog = function(opt_data, opt_ignored) {
  var output = '<div class="tfc-upper-container"><div class="tfc-container">';
  if (opt_data.labels.length == 0) {
    output += '<div class="aui-message info"><span class="aui-icon icon-info"></span>' + "Labels are not available" + '</div>';
  } else {
    var labelList234 = opt_data.labels;
    var labelListLen234 = labelList234.length;
    for (var labelIndex234 = 0; labelIndex234 < labelListLen234; labelIndex234++) {
      var labelData234 = labelList234[labelIndex234];
      output += '<div class="tfc-row"><label>' + labelData234 + '</label><button class="aui-button aui-dropdown2-trigger" aria-owns="tf-chart-colors-' + labelIndex234 + '-' + opt_data.macroId + '" aria-haspopup="true"><a class="tfc-color selected" data-color="' + opt_data.colors[labelIndex234] + '" style="background: ' + opt_data.colors[labelIndex234] + ';"></a></button><div id="tf-chart-colors-' + labelIndex234 + '-' + opt_data.macroId + '" class="aui-style-default aui-dropdown2 tfc-color-dd"><ul>';
      var colorList250 = opt_data.palette;
      var colorListLen250 = colorList250.length;
      for (var colorIndex250 = 0; colorIndex250 < colorListLen250; colorIndex250++) {
        var colorData250 = colorList250[colorIndex250];
        output += '<li class="tfc-li-color"><a class="tfc-color" data-color="' + colorData250 + '" style="background: ' + colorData250 + ';" data-index="' + labelIndex234 + '"></a></li>';
      }
      output += '<li class="tfc-li-color"><a class="tfc-color" data-color="#000000" style="background: #000000;" data-index="' + labelIndex234 + '"></a></li><li class="tfc-li-color"><a class="tfc-color" data-color="#ffffff" style="background: #ffffff;" data-index="' + labelIndex234 + '"></a></li></ul></div></div>';
    }
  }
  output += '</div><div class="tfc-dialog-buttons">' + ((opt_data.labels.length > 0) ? '<input class="aui-button" type="button" value="' + "Apply" + '" id="apply"/>' : '') + '<a href="#" class="cancel" id="cancel">' + "Cancel" + '</a></div></div>';
  return output;
};
if (goog.DEBUG) {
  ChartTemplate.colorDialog.soyTemplateName = 'ChartTemplate.colorDialog';
}

}catch(e){WRMCB(e)};
;
