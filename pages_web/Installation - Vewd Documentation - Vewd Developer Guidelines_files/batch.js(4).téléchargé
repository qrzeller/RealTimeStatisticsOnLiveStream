try {
/* module-key = 'com.stiltsoft.confluence.plugin.tablefilter.tablefilter:web-resources', location = 'tf-libs/js/table-utils.js' */
(function ($) {

    window.TableUtils = {

        number: 0,
        worklog: 1,
        duration: 2,
        worklogPlaces: [[' weeks', ' week', 'w'], [' days', ' day', 'd'], [' hours', ' hour', 'h' ], [' minutes', ' minute', 'm']],

        unescapeStrings: function(strs) {
            var div = $('<div></div>');
            var unescaped = [];
            strs.forEach(function(str) {
                unescaped.push(div.html(str).text());
            });
            return unescaped;
        },

        unescapeString: function(str) {
            return TableUtils.unescapeStrings([str])[0];
        },

        addFloats: function(f1, f2) {
            var f1Str = "" + f1;
            var f1Parts = f1Str.split('.');
            var f2Str = "" + f2;
            var f2Parts = f2Str.split('.');
            var decimals = Math.max(f1Parts[f1Parts.length - 1].length, f2Parts[f2Parts.length - 1].length);
            return parseFloat(parseFloat(f1 + f2).toFixed(decimals));
        },

        normalizeTable: function(tables) {
            tables.each(function() {
                var table = $(this);
                table.prepend(table.children('tbody').nextAll('thead'));
            });
        },

        getRows: function(table) {
            var children = table.children('thead:not(.tableFloatingHeader), tbody');
            var rows = table.add(children).children('tr:has(td,th)').not('.pivot-header');
            if (table.is('.attachments')) {
                rows = rows.not('.attachment-summary');
            }
            return rows;
        },

        getCellPosition: function(table, columnName) {
            var position = null;
            if (table.length) {
                var name = TableUtils.clearStringLowerCase(columnName);
                table[0].forEach(function(cell, n) {
                    if (TableUtils.clearStringLowerCase(cell) == name) {
                        position = n;
                        return false;
                    }
                });
            }
            return position;
        },

        getColumnPosition: function(rows, columnName) {
            var position = null;
            var name = TableUtils.clearStringLowerCase(columnName);
            rows.eq(0).find("td, th").each(function(n) {
                if (TableUtils.clearStringLowerCase($(this).text()) == name) {
                    position = n;
                    return false;
                }
            });
            return position;
        },

        isRowVisible: function(tr) {
            return tr[0].style.display !== 'none';
        },

        clearString: function (originalString) {
            if (originalString == undefined) return '';
            return $.trim(originalString.replace(/\u00A0/g, ' ').replace(/\u200B/g, '').replace(/\uFEFF/g, '').replace(/\s{2,}/g, ' '));
        },

        clearStringLowerCase: function (originalString) {
            return TableUtils.clearString(originalString).toLocaleLowerCase();
        },

        getNthTd: function (tr, number) {
            return TableUtils.getNthTdElem(tr, number).text();
        },

        getNthTdIcons: function (tr, number) {
            return TableUtils.getNthTdElem(tr, number).find('img, ul.inline-task-list');
        },

        getTdIconsSrc: function (td) {
            var src = [];
            td.find('img').each(function() {
                src.push(this.src);
            });
            if (src.length == 0) {
                src.push('');
            }
            return src;
        },

        getNthTdElem: function (tr, number) {
            return $(tr).children().eq(number);
        },

        convertToDate: function(value, dateFormat, locale) {
            if ($.isNumeric(value)) {
                var date = new Date(parseInt(value) + (new Date()).getTimezoneOffset() * 60000);
                value = TableUtils.formatDate(date, dateFormat, TableUtils.localisations[locale]);
            }
            return value;
        },

        formatDate: function(date, dateFormat, locale) {
            return $.datepicker.formatDate(dateFormat, date, TableUtils.localisations[locale]);
        },

        parseDate: function(date, dateFormat, locale) {
            if (dateFormat.indexOf("d") == -1) {
                return $.datepicker.parseDate("dd/" + dateFormat, "01/" + date, TableUtils.localisations[locale]);
            } else {
                try {
                    return $.datepicker.parseDate(dateFormat, date, TableUtils.localisations[locale]);
                } catch (e) {
                    if (e.indexOf("Extra/unparsed characters found in date: ") != -1) {
                        return $.datepicker.parseDate(dateFormat, date.replace(e.split("Extra/unparsed characters found in date: ")[1], ''), TableUtils.localisations[locale]);
                    } else {
                        throw e;
                    }
                }
            }
        },

        dateFormatAutocomplete: function(input, parent) {
            input.autocomplete({source: [
                {label: "dd-mm-yy (28-04-2016)", value: "dd-mm-yy"},
                {label: "dd.mm.yy (28.04.2016)", value: "dd.mm.yy"},
                {label: "yy-mm-dd (2016-04-28)", value: "yy-mm-dd"},
                {label: "yy/mm/dd (2016/04/28)", value: "yy/mm/dd"},
                {label: "m/d/yy (4/28/2016)", value: "m/d/yy"},
                {label: "m/d/y (4/28/16)", value: "m/d/y"},
                {label: "d M yy (28 Apr 2016)", value: "d M yy"},
                {label: "d M, yy (28 Apr, 2016)", value: "d M, yy"},
                {label: "MM yy (April 2016)", value: "MM yy"}
            ], minLength: 0, appendTo: parent, change: function () {
                localStorage.setItem('tf-dateformat', input.val());
            }});
            input.focus(function() {
                input.autocomplete("search", "");
            }).change(function () {
                localStorage.setItem('tf-dateformat', input.val());
            });
        },

        getLocaleDateFormat: function (defaultDateFormat) {
            var localeDateFormat = (AJS.Meta.get("user-date-pattern") || window.parent.AJS.Meta.get("user-date-pattern"));
            localeDateFormat = localeDateFormat ? localeDateFormat.replace('yyyy', 'yy').replace('MMM', 'M').replace('MM', 'mm') : defaultDateFormat;
            return localeDateFormat;
        },

        getValueType: function(value) {
            var self = this;
            var type = self.number;

            if ((new RegExp('^\\d{1,2}:\\d{2}')).test(value)) {
                return self.duration;
            }

            var correctedValue = value.replace(/,/g, '');
            this.worklogPlaces.some(function(place) {
                var found = false;
                place.some(function(placeVariant) {
                    if ((new RegExp('^\\d+' + placeVariant)).test(correctedValue)) {
                        type = self.worklog;
                        found = true;
                        return true;
                    }
                });
                return found;
            });

            return type;
        },

        parseValueByType: function(type, value, separator) {
            var self = this;
            value = self.clearString(value);
            if (type == self.worklog) {
                return self.parseWorklog(value);
            }
            if (type == self.duration) {
                return self.parseDuration(value);
            }
            return self.parseNumber(value, separator);
        },

        parseDuration: function(value) {
            var duration = false;
            var parts = value.split(':');
            if (parts.length > 1) {
                duration = 0;
                var mult = [1, 60, 3600];
                for (var i = parts.length - 1, j = 0; i >= 0; i--, j++) {
                    duration += parts[i] * mult[j];
                }
            }

            return duration;
        },

        parseWorklog: function(value) {
            var result = false;
            var mult = [2400, 480, 60, 1];
            value = value.replace(/,/g, '');

            this.worklogPlaces.forEach(function(place, i) {
                place.some(function(placeVariant) {
                    var parts = value.split(placeVariant);
                    if (parts.length == 2) {
                        if (result === false) result = 0;
                        result += mult[i] * parts[0];
                        value = parts[1];
                    }
                });
            });

            return result;
        },

        parseNumber: function(value, separator) {
            if (separator == '.') {
                value = value.replace(/,/g, '')
            } else if (separator == ',') {
                value = value.replace(/\./g, '')
            }
            return parseFloat(value.replace(/\s/g, '').replace(/,/g, '.').replace(/^[^\d-]*/g, ''));
        },

        isNumber: function(number) {
            return !isNaN(parseFloat(number)) && isFinite(number);
        },

        formatWorklog: function(value) {
            var result = [];
            var mult = [2400, 480, 60, 1];

            this.worklogPlaces.forEach(function(place, i) {
                var div = Math.floor(value / mult[i]);
                if (div) {
                    result.push(div + place[2]);
                    value -= div * mult[i];
                }
            });

            return result.join(' ');
        },

        formatDuration: function(value) {
            var result = [];
            var mult = [3600, 60, 1];

            var isFirst = true;
            mult.forEach(function(m, i) {
                var div = Math.floor(value / m);
                if (div || i) {
                    result.push(!isFirst && div < 10 ? '0' + div : div);
                    value -= div * m;
                    isFirst = false;
                }
            });

            return result.join(':');
        },

        getTables: function(body, containerSelector, isIcon, columnFilter) {
            return TableUtils.getTablesAndRows(body, containerSelector, isIcon, columnFilter).tables;
        },

        getTablesAndRows: function(body, containerSelector, isIcon, columnFilter) {
            var tables = [];
            var rows = [];

            columnFilter = columnFilter ? columnFilter : '';

            body.find('table').not(containerSelector + ' table table').each(function () {
                var table = [];
                var $rows = TableUtils.getRows($(this));
                rows.push($rows);
                $rows.each(function () {
                    var row = [];
                    $(this).children('th' + columnFilter + ', td' + columnFilter).each(function () {
                        var td = $(this);
                        if (isIcon) {
                            var html = '';
                            td.find('img, ul.inline-task-list').each(function() {
                                html += this.outerHTML;
                            });
                            row.push(html);
                        } else {
                            row.push(TableUtils.clearString(td.text()));
                        }
                    });
                    if (row.length > 0) {
                        table.push(row);
                    }
                });

                tables.push(table);
            });

            return {tables: tables, rows: rows};
        },

        getRawColumns: function(tables) {
            var columns = [];

            $.each(tables, function(i, table) {
                if (table.length) {
                    $.each(table[0], function(i, cell) {
                        if ($.inArray(cell, columns) == -1) {
                            columns.push(cell);
                        }
                    });
                }
            });

            return columns;
        },

        getColumns: function(tables) {
            return TableUtils.getRawColumns(tables).sort();
        },

        hashCode: function(str){
            var hash = 0;
            if (str.length == 0) return hash;
            for (var i = 0; i < str.length; i++) {
                hash = ((hash << 5) - hash) + str.charCodeAt(i);
                hash = hash & hash; // Convert to 32bit integer
            }
            return hash;
        },

        getContentId: function() {
            return AJS.params.contentId || AJS.Meta.get("content-id") || AJS.params.pageId || AJS.Meta.get("page-id");
        },

        transpose: function(tables) {
            var ts = [];
            tables.forEach(function(a) {
                var w = a.length ? a.length : 0,
                    h = a[0] instanceof Array ? a[0].length : 0;

                if (h === 0 || w === 0) { return []; }

                var i, j, t = [];

                for (i = 0; i < h; i++) {
                    t[i] = [];
                    for(j = 0; j <w ; j++) {
                        t[i][j] = a[j][i];
                    }
                }

                ts.push(t);
            });

            return ts;
        },

        processMergedCells: function(tables) {
            tables.find('.fake').remove();

            var isVisible = function(t) {
                return t[0].style.display != 'none' && t.parent()[0].style.display != 'none';
            };

            var showChild = function(t) {
                if (isVisible(t) && $.data(t[0], 'childCell')) {
                    $.data(t[0], 'childCell')[0].style.display = '';
                }
            };

            var showParent = function(t) {
                showChild(t);

                while ($.data(t[0], 'parentCell')) {
                    t = $.data(t[0], 'parentCell');
                    if (isVisible(t)) {
                        t.attr('rowspan', t.attr('rowspan') - 1);
                        break;
                    }
                }
            };

            var showColChild = function(t) {
                if (isVisible(t) && $.data(t[0], 'childColCell')) {
                    $.data(t[0], 'childColCell')[0].style.display = '';
                }
            };

            var calculateCurrentIndex = function(row, parentIndex, cell) {
                var index = -1;
                var count = 0;
                row.find('td, th').each(function() {
                    if (cell && this == cell.get(0)) {
                        return false;
                    }
                    if (typeof $(this).attr('colspan') !== 'undefined') {
                        index += parseInt($(this).attr('colspan'));
                    } else {
                        index += 1;
                    }
                    if (index >= parentIndex) {
                        return false;
                    }
                    count += 1;
                });

                if (cell) {
                    return index + 1;
                }

                return count;
            };

            tables.each(function () {
                var rowspans = [];
                $(this).find('th[rowspan], td[rowspan]').each(function() {
                    rowspans.push($(this));
                });
                rowspans.sort(function(cell1, cell2) {
                    return calculateCurrentIndex(cell1.parent(), 1000000, cell1) - calculateCurrentIndex(cell2.parent(), 1000000, cell2);
                });

                $.each(rowspans, function() {
                    var cell = $(this);
                    var row = cell.parent();
                    var index = calculateCurrentIndex(row, 100000, cell);
                    var currentIndex = 0;
                    var count = parseInt(cell.attr('rowspan')) - 1;
                    $.data(cell[0], 'rowspan', count + 1);
                    cell.bind('hide', function() {
                        showChild($(this));
                    });

                    for (; count > 0; count--) {
                        row = row.next();
                        currentIndex = calculateCurrentIndex(row, index, false);
                        var parent = cell;
                        cell = cell.clone(true, true);
                        $.data(parent[0], 'childCell', cell);
                        $.data(cell[0], 'parentCell', parent);
                        cell.attr('rowspan', -1)  //для выделятора ячеек в редакторе
                            .hide()
                            .addClass('fake');
                        $.data(cell[0], 'rowspan', count);

                        if (index) {
                            row.find("td:nth-child(" + currentIndex + "), th:nth-child(" + currentIndex + ")").after(cell);
                        } else {
                            row.find("td:nth-child(" + (currentIndex + 1) + "), th:nth-child(" + (currentIndex + 1) + ")").before(cell);
                        }

                        cell.unbind('hide').bind('hide', function() {
                            showParent($(this));
                        });
                    }
                });

                $(this).find('th[colspan], td[colspan]').each(function () {
                    var cell = $(this);
                    var colspan = parseInt(cell.attr('colspan'));
                    $.data(cell[0], 'colspan', colspan);
                    cell.bind('hideCol', function() {
                        showColChild($(this));
                    });

                    for (var count = colspan - 1; count > 0; count--) {
                        var fakeCell = cell.clone(true, true);
                        fakeCell.attr('colspan', -1);  //для выделятора ячеек в редакторе
                        $.data(fakeCell[0], 'colspan', count);
                        $.data(fakeCell[0], 'counted', 'counted');
                        fakeCell.hide().addClass('fake');
                        $.data(fakeCell[0], 'parentColCell', cell);

                        cell.after(fakeCell);
                        $.data(cell[0], 'childColCell', fakeCell);

                        fakeCell.unbind('hideCol').bind('hideCol', function() {
                            var t = $(this);
                            showColChild(t);

                            while ($.data(t[0], 'parentColCell')) {
                                t = $.data(t[0], 'parentColCell');
                                if (isVisible(t)) {
                                    t.attr('colspan', t.attr('colspan') - 1);
                                    break;
                                }
                            }
                        });

                        cell = fakeCell;

                        if (cell.attr('rowspan')) {
                            cell.addClass('tf-rowspan-bind');
                        }
                    }
                });

                $(this).find('.tf-rowspan-bind').each(function() {
                    var cell = $(this);
                    if (!cell.hasClass('tf-rowspan-bind')) {
                        return;
                    }
                    var row = cell.parent();
                    var index = cell.get(0).cellIndex;
                    var count = parseInt(cell.attr('rowspan')) - 1;
                    $.data(cell[0], 'rowspan', count + 1);
                    cell.removeClass('tf-rowspan-bind');
                    cell.bind('hide', function() {
                        showChild($(this));
                    });

                    for (; count > 0; count--) {
                        row = row.next();
                        var parent = cell;
                        cell = row.find('td,th').eq(index);
                        $.data(parent[0], 'childCell', cell);
                        if (cell.length) {
                            $.data(cell[0], 'parentCell', parent);
                            $.data(cell[0], 'rowspan', count);
                            cell.removeClass('tf-rowspan-bind');

                            cell.unbind('hide').bind('hide', function() {
                                showParent($(this));
                            });
                        }
                    }
                });
            });
        },

        sortSelectedOptions: function(select, params) {
            var choices = select.parent().find('.chosen-choices');
            for (var i = params.length - 1; i >= 0 ; i--) {
                choices.prepend(choices.find('.search-choice').filter(function() {
                    return $(this).text() == params[i].replace(/\‚/g, ',');
                }));
            }
            return choices;
        },

        setCookie: function(name, value) {
            document.cookie = name + "=" + encodeURIComponent(value) + "; max-age=" + 60 * 60 * 24 * 3000;
        },

        getCookie: function(name) {
            var cookie = document.cookie;
            if (cookie.length != 0) {
                var re = new RegExp('[; ]' + name + '=([^\\s;]*)');
                var matcher = (' ' + document.cookie).match(re);
                if (matcher) {
                    return decodeURIComponent(matcher[1]);
                }
            }
            return '';
        },

        createDialog: function(container, rows, macroId) {
            var separator = '\t';
            var quote = '"';
            var storedSeparator = separator;
            var storedQuote = quote;
            var encode = true;
            var dialog = new AJS.Dialog({
                width: 440,
                height: 382,
                id: "export-csv-dialog-" + macroId,
                closeOnOutsideClick: false
            });

            dialog.addHeader("Export Options");
            dialog.addPanel("Export to CSV", TableUtilsTemplate.exportToCSV(), "export-csv-panel-body");

            var $currentDialog = $('#export-csv-dialog-' + macroId);
            var $separator = $currentDialog.find('.export-csv-separator');
            var $quote = $currentDialog.find('.export-csv-quote');
            var $headers = $currentDialog.find('.export-csv-headers');
            var typeSelect = $currentDialog.find('#tf-csv-export-type');
            var defaultMessage = $currentDialog.find('#tf-csv-export-default-message');
            var paramsContainer = $currentDialog.find('#tf-csv-export-params');
            var exportLinks = $currentDialog.find('.tf-csv-export-links');
            var linkSeparator = $currentDialog.find('#tf-csv-export-link-separator');
            $separator.val(separator);
            $quote.val(quote);
            $headers.val(decorateValues(getHeader()));

            $separator.on('keyup', function() {
                localStorage.setItem('tf-csvexport-sep', $separator.val().trim());
                updateHeader();
            });
            $quote.on('keyup', function() {
                localStorage.setItem('tf-csvexport-quote', $quote.val().trim());
                updateHeader();
            });

            typeSelect.change(function () {
                if (typeSelect.val() == 'custom') {
                    defaultMessage.hide();
                    paramsContainer.show();
                    $separator.val(localStorage.getItem('tf-csvexport-sep') || ',');
                    $quote.val(localStorage.getItem('tf-csvexport-quote') || '"');
                    updateHeader();
                    encode = false;
                } else {
                    defaultMessage.show();
                    paramsContainer.hide();
                    $separator.val('\t');
                    $quote.val('"');
                    updateHeader();
                    encode = true;
                }
            });

            exportLinks.change(function () {
                if (exportLinks.is(':checked')) {
                    linkSeparator.parent().show();
                    localStorage.setItem('tf-csvexport-link', true);
                } else {
                    linkSeparator.parent().hide();
                    localStorage.setItem('tf-csvexport-link', false);
                }
            });

            if (localStorage.getItem('tf-csvexport-link') == 'false') {
                exportLinks.prop('checked', false).trigger('change');
            }

            linkSeparator.change(function () {
                localStorage.setItem('tf-csvexport-link-sep', linkSeparator.val());
            });

            linkSeparator.find('option[value="' + (localStorage.getItem('tf-csvexport-link-sep') || ';') + '"]').prop('selected', true);

            dialog.addButton("Export", function (dialog) {
                separator = $separator.val();
                quote = $quote.val();
                TableUtils.exportToCsv(container, separator || ',', quote, $headers.val() ? revealValues($headers.val()) : [], encode, exportLinks.is(':checked'), linkSeparator.val());
                quote = $quote.val();
                separator = $separator.val();
                dialog.hide();
                dialog.remove();
            });
            dialog.addLink("Cancel", function (dialog) {
                dialog.hide();
                dialog.remove();
            }, "#");

            return dialog;

            function updateHeader() {
                var headers = revealValues($headers.val());
                storedSeparator = $separator.val();
                storedQuote = $quote.val();
                $headers.val(decorateValues(headers));
            }

            function getHeader() {
                var header = [];
                var columns = rows.eq(0).find('th, td');

                if (columns.length > 0) {
                    columns.each(function() {
                        var cell = $(this);
                        if (cell.css('display') != 'none') {
                            header.push(cell.text());
                        } else if (cell.hasClass('fake') && !cell.hasClass('tf-hidden-column') && !cell.hasClass('pivot-hidden-column')) {
                            header.push('');
                        }
                    });
                }

                return header;
            }

            function decorateValues(values) {
                if (storedQuote) {
                    return storedQuote + values.join(storedQuote + (storedSeparator || ',') + storedQuote) + storedQuote;
                } else {
                    return values.join(storedSeparator || ',');
                }

            }

            function revealValues(values) {
                if (storedQuote) {
                    return values.slice(storedQuote.length, -storedQuote.length).split(storedQuote + (storedSeparator || ',') + storedQuote);
                } else {
                    return values.split(storedSeparator || ',');
                }
            }
        },

        exportToCsv: function($source, separator, quote, headers, encode, exportLinks, linkSeparator) {
            var content = $source.table2CSV({separator: separator, quote: quote, headers: headers, exportLinks: exportLinks, linkSeparator: linkSeparator});

            var type = "data:text/csv;charset=" + encode ? "utf-16le" : "utf-8";
            if (navigator.userAgent.indexOf('Safari') != -1 && navigator.userAgent.indexOf('Chrome') == -1) {
                type = "application/octet-stream";
            }

            var finalContent = content;
            if (encode) {
                var textEncoder = new CustomTextEncoder('utf-16le', {NONSTANDARD_allowLegacyEncoding: true});
                var csvContentEncoded = textEncoder.encode([content]);
                finalContent = new Uint8Array(2 + csvContentEncoded.length);
                finalContent[0] = 0xFF;
                finalContent[1] = 0xFE;
                finalContent.set(csvContentEncoded, 2);
            }
            var blob = new Blob([finalContent], {
                type: type
            });
            saveAs(blob, "export.csv");
        },

        exportToPdf: function($source, isChart) {
            var imagesToLoad = 0;
            var finished = false;
            var isCloud = $source.hasClass('tf-cloud-table');

            var makePDF = function() {
                if (imagesToLoad == 0 && finished) {
                    pdfMake.createPdf(docDefinition).download('export.pdf');
                }
            };

            var rgb2hex = function(orig) {
                var rgb = (orig && orig != 'transparent' ? orig.replace(/\s/g,'').match(/^rgba?\((\d+),(\d+),(\d+)[,]*(\d+)*/i) : ['noColor']);
                if (rgb[0].indexOf('rgba') !== -1) {
                    function RGBAtoRGB(r, g, b, a, r2, g2, b2){
                        var r3 = Math.round(((1 - a) * r2) + (a * r));
                        var g3 = Math.round(((1 - a) * g2) + (a * g));
                        var b3 = Math.round(((1 - a) * b2) + (a * b));
                        return [r3,g3,b3];
                    }

                    var result = RGBAtoRGB(parseInt(rgb[1]), parseInt(rgb[2]), parseInt(rgb[3]), parseInt(rgb[4]), 255, 255, 255);

                    rgb[1] = result[0];
                    rgb[2] = result[1];
                    rgb[3] = result[2];
                }
                return (rgb && rgb.length === 5) ? "#" +
                    ("0" + parseInt(rgb[1],10).toString(16)).slice(-2) +
                    ("0" + parseInt(rgb[2],10).toString(16)).slice(-2) +
                    ("0" + parseInt(rgb[3],10).toString(16)).slice(-2) : [255, 255, 255];
            };

            var calcRealLength = function(cells) {
                var count = 0;
                for (var i = 0; i < cells.length; i++) {
                    if (!$(cells[i]).hasClass('tf-hidden-column')) {
                        count += 1;
                    }
                }
                return count;
            };

            var getTableObject = function(table) {
                var wide = false;
                var $table = $(table);
                var tableWidth = $table.width();
                if (tableWidth > 870) {
                    wide = true;
                }
                var widths = [];
                if ($table.hasClass('fixed-table')) {
                    $table.find('col').each(function() {
                        widths.push(parseInt($(this).css('width'))  / fontRatio / 1.2);
                    });
                }

                //todo может быть проблема с хедером толщиной больше 1
                var tableObject = {
                    headerRow: 1,
                    style: 'tableStyle',
                    table: {
                        widths: widths,
                        body: []
                    },
                    layout: {
                        hLineColor: '#ddd',
                        vLineColor: '#ddd',
                        paddingLeft: function() { return 6; },
                        paddingRight: function() { return 6; },
                        paddingTop: function() { return 3; },
                        paddingBottom: function() { return 3; }
                    },
                    pageOrientation: wide ? 'landscape' : 'portrait',
                    pageBreak: wide && !isFirst ? 'before' : ''
                };
                var tableRow;
                var maxLength = -1;

                for (var i = 0, count = 0; i < table.rows.length; i++) {
                    tableRow = table.rows[i];
                    if ($(tableRow).css('display') === 'none') {
                        continue;
                    }
                    tableObject.table.body.push([]);
                    var realLength = calcRealLength(tableRow.cells);
                    if (realLength > maxLength) {
                        if (maxLength != -1) {
                            for (var prev = 0; prev < count; prev++) {
                                for (var t = 0; t < realLength - maxLength; t++) {
                                    tableObject.table.body[prev].push({
                                        text: '\n',
                                        style: 'cell',
                                        rowSpan: 1,
                                        colSpan: 1
                                    });
                                }
                            }
                        }
                        maxLength = realLength;
                    }
                    for (var j = 0; j < tableRow.cells.length; j++) {
                        var $tableCell = $(tableRow.cells[j]);
                        if ($tableCell.hasClass('tf-hidden-column')) {
                            continue;
                        }
                        var isTextBold = false;
                        if ($(tableRow).css('font-weight') == 'bold') {
                            isTextBold = true;
                        }

                        tableObject.table.body[count].push({
                            stack: loopIntoChildren($tableCell[0], [], [], [])[0],
                            style: (i === 0 || isTextBold ? 'header' : 'cell'),
                            rowSpan: parseInt($tableCell.attr('rowspan')) || 1,
                            colSpan: parseInt($tableCell.attr('colspan')) || 1,
                            color: rgb2hex($tableCell.css('color')),
                            fillColor: rgb2hex($tableCell.css('background-color'))
                        });
                        if (j == tableRow.cells.length-1 && realLength < maxLength) {
                            for (var t = 0; t < maxLength - realLength; t++) {
                                tableObject.table.body[count].push({
                                    text: '\n',
                                    style: 'cell',
                                    rowSpan: 1,
                                    colSpan: 1,
                                    color: rgb2hex($tableCell.css('color')),
                                    fillColor: rgb2hex($tableCell.css('background-color'))
                                });
                            }
                        }
                    }
                    count += 1;
                }

                for (i = 0; i < maxLength; i++) {
                    tableObject.table.widths.push('auto');
                }

                return tableObject;
            };

            pdfMake.fonts = {
                Roboto: {
                    normal: 'Roboto-Regular.ttf',
                    bold: 'Roboto-Medium.ttf',
                    italics: 'Roboto-Italic.ttf',
                    bolditalics: 'Roboto-MediumItalic.ttf'
                }
            };

            var fontRatio = 1.4;
            var docDefinition = {
                content: [],
                styles: {
                    header: {
                        fontSize: 10,
                        bold: true,
                        color: '#333333'
                    },
                    cell: {
                        fontSize: 10,
                        color: '#333333'
                    },
                    tableStyle: {
                        marginTop: 10
                    }
                },
                defaultStyle: {
                    // alignment: 'justify'
                }
            };

            var isFirst = true;
            var isPivot = $source.hasClass('pivot-table');
            var hasImage = $source.parent().find('#pngdataurl img').length != 0;

            if (isChart && hasImage) {
                var image = $source.parent().find('#pngdataurl img');
                var width = $(image).get(0).width;
                var height = $(image).get(0).height;
                var addMargin = false;

                if (width > 590 && height > 510) {
                    addMargin = true;
                }

                if (width > 590) {
                    if (height > 510) height = 510;
                    if (width > 800) width = 800;
                } else {
                    if (height > 700) height = 700;
                }

                var imageObj = {
                    image: image.attr('src'),
                    alignment: 'center',
                    fit: [width, height]
                };

                // этот маргин тут для какой-то странной линии, которая вылазит под рисунком.
                if (addMargin) {
                    imageObj.margin = [0,0,0,40];
                }

                docDefinition.content.push(imageObj);
                docDefinition.pageOrientation = $(image).get(0).width > 590 ? 'landscape' : 'portrait';
                docDefinition.pageBreak = $(image).get(0).width > 590 ? 'after' : '';
                isFirst = false;
            }

            if ($source.css('display') !== 'none') {
                $source.find('table')
                    .not('.tablefilter-table-wrapper table table')
                    .not('.fht-thead table')
                    .not('#i_confiform_registration_form table').each(function() {
                        if ($(this).css('display') == 'none') return true;

                        $(this).find('thead.tableFloatingHeader tr').hide();
                        $(this).find('tr:not(:has(td,th))').hide();

                        if ($(this).width() > 1200) {
                            docDefinition.styles.header.fontSize = 6;
                            docDefinition.styles.cell.fontSize = 6;
                            fontRatio = 14 / 6;
                        }

                        if (isFirst && !hasImage) {
                            docDefinition.pageOrientation = $(this).width() > 870 ? 'landscape' : 'portrait';
                            docDefinition.pageBreak = $(this).width() > 870 ? 'after' : '';
                        }

                        if (isPivot) {
                            TableUtils.processMergedCells($(this));
                        }
                        docDefinition.content.push(getTableObject($(this).get(0)));

                        $(this).find('thead:eq(1) tr').show();

                        isFirst = false;
                    });
                $source.find('table').each(function() {
                    $(this).data('counted-for-pdf', false);
                });
            }

            finished = true;
            makePDF();

            function loopIntoChildren(parent, parentContent, parentStack, parentDecoration) {
                var $parent = $(parent);
                if ($parent.css('display') != 'none') {
                    $.each(parent.childNodes, function () {
                        var child = this;
                        if (child.nodeType == 3) {
                            var content = {
                                text: child.nodeValue || '\u200b',
                                bold: $parent.css('font-weight') === 'bold',
                                italics: $parent.css('font-style') === 'italic',
                                alignment: $parent.css('text-align'),
                                color: rgb2hex($parent.css('color')),
                                background: $parent.css('background-color') && $parent.css('background-color') != 'rgba(0, 0, 0, 0)' ? rgb2hex($parent.css('background-color')) : undefined,
                                decoration: parentDecoration,
                                decorationColor: 'black',
                                fontSize: parseInt($parent.css('font-size')) / fontRatio
                            };

                            if ($parent.hasClass('status-macro')) {
                                content.text = ' ' + content.text + ' ';
                                content.alignment = $parent.parent().css('text-align');
                            }

                            parentContent.push(content);
                            if (parentContent.length == 1) {
                                parentStack.push({text: parentContent});
                            }
                        } else if (child.nodeType == 1) {
                            var childStack;
                            var decoration = getDecoration($(child));
                            if (child.tagName == 'TABLE') {
                                parentStack.push(getTableObject(child));
                                parentContent = [];
                            } else if (child.tagName == 'IMG') {
                                if (!isCloud) { // в клауде никакие картинки не экспортятся из-за CORS :(
                                    var image = {image: '', width: child.width / 1.5};
                                    imagesToLoad++;
                                    var src = TableUtils.getBase64Image(child, function (url) {
                                        image.image = url;
                                        imagesToLoad--;
                                        makePDF();
                                        if (url) {
                                            parentStack.push(image);
                                            parentContent = [];
                                        }
                                    });
                                    if (src) {
                                        image.image = src;
                                        imagesToLoad--;
                                        parentStack.push(image);
                                        parentContent = [];
                                    }
                                }
                            } else if (child.tagName == 'P') {
                                parentStack.push(loopIntoChildren(child, [], [], decoration)[0]);
                                parentContent = [];
                            } else if (child.tagName == 'UL') {
                                childStack = loopIntoChildren(child, [], [], decoration)[0];
                                parentStack.push({ul: childStack});
                                parentContent = [];
                            } else if (child.tagName == 'OL') {
                                childStack = loopIntoChildren(child, [], [], decoration)[0];
                                parentStack.push({ol: childStack});
                                parentContent = [];
                            } else if (child.tagName == 'LI') {
                                childStack = loopIntoChildren(child, [], [], decoration)[0];
                                parentStack.push(childStack);
                                parentContent = [];
                            } else if (child.tagName != 'STYLE') {
                                parentContent = loopIntoChildren(child, parentContent, parentStack, uniqueConcat(decoration, parentDecoration))[1];
                            }
                        }
                    });
                }
                return [parentStack, parentContent];
            }

            function getDecoration($parent) {
                var decoration = [];
                var olderParent = $parent.parent();
                if ($parent.css('text-decoration') == '' && olderParent.css('text-decoration')) {
                    decoration = '';
                }
                if ($parent.css('text-decoration') == 'underline' || olderParent.css('text-decoration') == 'underline') {
                    decoration.push('underline');
                }
                if ($parent.css('text-decoration') == 'line-through' || olderParent.css('text-decoration') == 'line-through') {
                    decoration.push('lineThrough');
                }

                return decoration;
            }

            function uniqueConcat(arr1, arr2) {
                return arr1.concat(arr2.filter(function(item) {
                    return arr1.indexOf(item) < 0;
                }));
            }
        },

        exportToDoc: function($source, isChart) {
            var content = $source.clone();
            content.find('table .fake').remove();
            content.find('.lockDisabled, .lockEnabled').remove();
            content.find('.pivot-controls').remove();
            content.find('.tf-header-hidden, .tf-hidden-column').remove();
            content.find('thead.tableFloatingHeader').remove();
            content.find('a[href]').each(function () {
                $(this).attr('href', this.href);
            });

            var srcImg = $source.find('img');
            var contentImg = content.find('img');
            for (var i = 0; i < srcImg.length; i++) {
                contentImg[i].width = srcImg[i].width;
                contentImg[i].height = srcImg[i].height;
            }

            if (isChart) {
                content.first().prepend($source.parent().find('#pngdataurl').clone());
                if (content.find('.pivot-content').length !== 0) {
                    content.find('.original-table').remove();
                }
            }

            content.wordExport('export');
        },

        getBase64Image: function(img, callback) {
            var canvas = document.createElement("canvas");
            canvas.width = img.naturalWidth;
            canvas.height = img.naturalHeight;
            var ctx = canvas.getContext("2d");
            try {
                ctx.drawImage(img, 0, 0);
                return canvas.toDataURL();
            } catch(e) {
                canvas = document.createElement("canvas");
                canvas.width = img.naturalWidth;
                canvas.height = img.naturalHeight;
                ctx = canvas.getContext("2d");
                var bufferImg = new Image();
                bufferImg.setAttribute('crossOrigin', 'anonymous');
                bufferImg.onload = function() {
                    ctx.drawImage(bufferImg, 0, 0);
                    callback(canvas.toDataURL());
                };
                bufferImg.onerror = function() {
                    callback('');
                };
                bufferImg.src = img.src;
                return '';
            }
        },

