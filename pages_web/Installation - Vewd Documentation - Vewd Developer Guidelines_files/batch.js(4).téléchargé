                    "Tháng Bảy", "Tháng Tám", "Tháng Chín", "Tháng Mười", "Tháng Mười Một", "Tháng Mười Hai" ],
                monthNamesShort: [ "Tháng 1", "Tháng 2", "Tháng 3", "Tháng 4", "Tháng 5", "Tháng 6",
                    "Tháng 7", "Tháng 8", "Tháng 9", "Tháng 10", "Tháng 11", "Tháng 12" ],
                dayNames: [ "Chủ Nhật", "Thứ Hai", "Thứ Ba", "Thứ Tư", "Thứ Năm", "Thứ Sáu", "Thứ Bảy" ],
                dayNamesShort: [ "CN", "T2", "T3", "T4", "T5", "T6", "T7" ],
                dayNamesMin: [ "CN", "T2", "T3", "T4", "T5", "T6", "T7" ],
                firstDay: 0,
                isRTL: false,
                showMonthAfterYear: false,
                yearSuffix: ""
            },
            "zh_CN": {
                monthNames: [ "一月","二月","三月","四月","五月","六月","七月","八月","九月","十月","十一月","十二月" ],
                monthNamesShort: [ "一月","二月","三月","四月","五月","六月","七月","八月","九月","十月","十一月","十二月" ],
                dayNames: [ "星期日","星期一","星期二","星期三","星期四","星期五","星期六" ],
                dayNamesShort: [ "周日","周一","周二","周三","周四","周五","周六" ],
                dayNamesMin: [ "日","一","二","三","四","五","六" ],
                firstDay: 1,
                isRTL: false,
                showMonthAfterYear: true,
                yearSuffix: "年"
            },
            "zh_TW": {
                monthNames: [ "一月","二月","三月","四月","五月","六月","七月","八月","九月","十月","十一月","十二月" ],
                monthNamesShort: [ "一月","二月","三月","四月","五月","六月","七月","八月","九月","十月","十一月","十二月" ],
                dayNames: [ "星期日","星期一","星期二","星期三","星期四","星期五","星期六" ],
                dayNamesShort: [ "周日","周一","周二","周三","周四","周五","周六" ],
                dayNamesMin: [ "日","一","二","三","四","五","六" ],
                firstDay: 1,
                isRTL: false,
                showMonthAfterYear: true,
                yearSuffix: "年"
            }
        }
    };

    $.extend({
        getUrlHashVars: function() {
            var vars = [], hash;
            var hashes = window.location.href.slice(window.location.href.indexOf('#') + 1).split('&');
            for (var i = 0; i < hashes.length; i++)
            { hash = hashes[i].split('='); vars.push(hash[0]); vars[hash[0]] = decodeURIComponent(hash[1]); }
            return vars;
        },

        getUrlHashVar: function(name) {
            return $.getUrlHashVars()[name];
        }
    });

    $.fn.changeElementType = function(newType) {
        var attrs = {};

        $.each(this[0].attributes, function(idx, attr) {
            attrs[attr.nodeName] = attr.nodeValue;
        });

        this.replaceWith(function() {
            return $("<" + newType + "/>", attrs).append($(this).contents());
        });
    };

})(jQuery);



}catch(e){WRMCB(e)};
;
try {
/* module-key = 'com.stiltsoft.confluence.plugin.tablefilter.tablefilter:web-resources', location = 'js/filter-utils.js' */
(function ($) {

    window.TFUtils = {

        loadScriptsBeforeExport: function(callback) {
            if (window.pdfMake) {
                callback();
            } else {
                TFUtils.loadResource('com.stiltsoft.confluence.plugin.tablefilter.tablefilter:export-pdf-resources', callback);
            }
        },

        loadD3Scripts: function(callback) {
            if (window.d3) {
                callback();
            } else {
                TFUtils.loadResource('com.stiltsoft.confluence.plugin.tablefilter.tablefilter:d3-resources', callback);
            }
        },

        getCurrentUserFullName: function() {
            return AJS.params.currentUserFullname;
        },

        getCurrentUserName: function() {
            return AJS.params.remoteUser;
        },

        saveMacroParams: function(data) {
            $.ajax({
                contentType: "application/x-www-form-urlencoded; charset=utf-8",
                type: 'POST',
                cache: false,
                url: Confluence.getContextPath() + '/rest/table-filter/1.0/service/save-tf',
                dataType: 'json',
                data: data
            });
        },

        loadResource: function(key, callback) {
            $.ajax({
                contentType: "application/x-www-form-urlencoded; charset=utf-8",
                type: 'GET',
                url: Confluence.getContextPath() + '/rest/table-filter/1.0/service/load-resource',
                dataType: 'text',
                data: {
                    key: key
                },
                success: function(data) {
                    $('head').append(data);
                    callback();
                }
            });
        }
    };

})(jQuery);





}catch(e){WRMCB(e)};
;
try {
/* module-key = 'com.stiltsoft.confluence.plugin.tablefilter.tablefilter:web-resources', location = 'tf-libs/js/tablefilter.js' */
(function ($) {

    window.TableFilter = function(showNRows, limitHeight, heightValue, numbering, sorts, dateFormat, separator, macroId, pageId,
                                  sparkline, sparkName, order, isOR, ddSeparator, editorMacro, macroParams) {

        var t = this;

        t.editorMacro = editorMacro;
        t.macroParams = macroParams;
        t.pageId = pageId;
        t.macroId = macroId;
        t.showNRows = showNRows;
        t.numbering = numbering ? numbering.toLowerCase() : false;
        t.dateFormat = dateFormat || $.datepicker.ATOM;
        t.dateFormat = t.dateFormat.replace('yyyy', 'yy');
        t.localeDateFormat = TableUtils.getLocaleDateFormat(t.dateFormat);
        t.separator = separator;
        t.limitHeight = limitHeight == "true";
        t.heightValue = heightValue ? parseInt(heightValue) : false;
        t.sorts = sorts;
        t.cbState = null;
        t.tdNumbers = null;
        t.tables = null;
        t.rows = null;
        t.tableWrapper = editorMacro ? editorMacro.find('.tablefilter-table-wrapper') : $('div#tf-table-' + t.macroId);
        t.comboBoxDiv = $('#comboBoxDiv-' + t.macroId);
        t.outerWrapper = t.comboBoxDiv.parent();
        t.lockDisabled = t.comboBoxDiv.find('div.lockDisabled');
        t.lockEnabled = t.comboBoxDiv.find('div.lockEnabled');
        t.showBtn = t.outerWrapper.children('.tf-shower-wrapper').find('.tf-shower');
        t.rawTables = null;
        t.columns = null;
        t.hasPivot = t.tableWrapper.find('.pivot-content').length > 0;
        t.locale = (AJS.Meta.get("user-locale") || window.parent.AJS.Meta.get("user-locale"));
        t.locale = t.locale ? t.locale.replace('-', '_') : 'en_US';
        t.hideParamName = t.pageId + "_" + t.macroId + "_hidden";
        t.menu = t.comboBoxDiv.find('#filter-menu-view-' + t.macroId);
        t.saveBtn = t.menu.find('.filter-save').not('body.content-preview .filter-save');
        t.resetSortBtn = t.menu.find('.filter-sort-reset');
        t.isLockEnabled = t.editorMacro;
        t.sparkline = sparkline;
        t.sparkName = (sparkName == '$sparkName' ? '' : sparkName);
        t.isAND = isOR != "OR";
        t.ddSeparator = ddSeparator;

        if (t.tableWrapper.length == 0) return null;

        t.fixURIHash();
        t.orderFilters(order);
        t.getTable();
        t.sortTable();
        t.normalizeTable();
        t.addNumbering();
        t.appendSparklines();
        t.createSelects(t.comboBoxDiv);
        t.createDatePickers(t.comboBoxDiv);
        t.bindInputs();
        t.bindReset();
        t.bindDisable();
        t.bindDOMChange();
        t.bindInnerFiltration();
        t.bindHideBtn();
        t.bindAddFilter();
        t.bindChangeSettings();
        t.bindSave();
        t.bindExport();
        t.bindSortReset();
        t.bindCopyUrl();
        t.bindGetUrlValues();
        t.bindMakeInverse();
        t.bindChangeLabel();
        t.updateSelectOptions();
        t.makeSortable();
        t.makeResizable(t.comboBoxDiv);
        t.makeTablesScrollable();
        t.filterTable();

//      Karma add-on fix
        if (!t.editorMacro && window.KarmaUser && t.tables.find('a[data-username]').length) {
            setTimeout(function() {
                t.updateSelectOptions();
                t.filterTable();
            }, 1000);
        }
    };

    TableFilter.prototype.orderFilters = function(order) {
        var t = this;
        if (order) {
            var container = t.comboBoxDiv.find('.sortable-container');
            container.append(container.find('.iconfilter-element'));
            var filters = container.find('.filter-container');
            var array = order.split(',');
            for (var i = 0 ; i < array.length; i++) {
                container.append(filters.eq($.inArray(i.toString(), array)));
            }
        }
    };

    TableFilter.prototype.sortTable = function() {
        var t = this;

        t.tables.bind('tablesorter-initialized.tf', function () {
            var table = $(this);
            var firstLetter = t.dateFormat.replace(/\W+/g, '')[0].toLowerCase();
            if (firstLetter != 'm') {
                var format;
                switch (firstLetter) {
                    case ('d') :
                        format = 'dateFormat-ddmmyyyy';
                        break;
                    case ('y') :
                        format = 'dateFormat-yyyymmdd';
                        break;
                    default :
                        format = false;
                }

                if (format) {
                    table.find('thead th').addClass(format);
                }
            }
        });
        if (t.sorts.length) {
            var event = t.hasPivot ? 'pivot-sorted' : 'tablesorter-initialized.tf';
            t.tables.bind(event, function () {
                var table = $(this);
                table.unbind(event);
                t.sortOneTable(table);
            });
        }
    };

    TableFilter.prototype.sortTables = function() {
        var t = this;
        t.tables.each(function() {
            t.sortOneTable($(this))
        });
    };

    TableFilter.prototype.sortOneTable = function(table) {
        var t = this;
        if (t.sorts.length) {
            t.pauseUpdate = true;
            t.setMutationObserver();
            TableUtils.normalizeTable(table);
            t.pauseUpdate = false;
            t.setMutationObserver();
            var body = $('<div>' + table.prop('outerHTML') + '</div>');
            var columns = TableUtils.getRawColumns(TableUtils.getTables(body, '.tablefilter-table-wrapper'));
            var sortOn = [];
            $.each(t.sorts, function(i, sort) {
                var asc = sort.indexOf(" (\u200DAsc)") == -1 ? 1 : 0;
                var col = sort.replace(' (\u200DAsc)', "").replace(' (\u200DDesc)', "");
                var index = $.inArray(col, columns);
                if (index > -1)
                    sortOn.push([index, asc]);
            });

            if (sortOn.length == 0) return;

            t.resorting = true;
            t.setMutationObserver();
            if (!t.hasPivot)
                t.updateSorter(table);
            t.restoreNumbering = true;
            table.trigger('sorton', [sortOn]);
            t.resorting = false;
            t.resetSortBtn.addClass('tf-disabled-link');
            t.setMutationObserver();
        }
    };

    TableFilter.prototype.normalizeTable = function() {
        var t = this;
        t.pauseUpdate = true;
        t.setMutationObserver();
        TableUtils.normalizeTable(t.tables);
        t.pauseUpdate = false;
        t.setMutationObserver();
        if (!t.hasPivot) {
            TableUtils.processMergedCells(t.tables);
        }
    };

    TableFilter.prototype.addNumbering = function() {
        var t = this;
        if (t.numbering) {
            t.pauseUpdate = true;
            t.setMutationObserver();
            TableUtils.normalizeTable(t.tables);
            t.tables.each(function(tableN) {
                var table = $(this);
                var pivotHeader = table.find('tr.pivot-header:eq(0)');
                if (pivotHeader.find('.tf-numbering').length == 0) {
                    pivotHeader.prepend('<th class="confluenceTh tf-numbering" rowspan="2"/>');
                }

                var rows = t.rows[tableN];

                if (t.tables.find('colgroup col.tfn').length == 0) {
                    var width = 29 + (('' + rows.length).length - 1) * 8;
                    t.tables.find('colgroup').prepend('<col class="tfn" style="width: ' + width + 'px"> ');
                }

                rows.each(function(i) {
                    var tr = $(this);
                    var td = tr.children('th,td').not('.fake').eq(0);
                    var tn = tr.find('.tf-numbering');
                    var clazz = td.attr('class');
                    if (i == 0) {
                        if (tn.length == 0) {
                            if (td.prop("tagName").toLowerCase() == 'th') {
                                tr.prepend('<th class="' + clazz + ' tf-numbering" contenteditable="false"/>');
                            } else {
                                tr.prepend('<td class="' + clazz + ' tf-numbering" contenteditable="false"/>');
                            }
                        } else {
                            tn.attr('contenteditable', false);
                        }
                    } else {
                        var n = t.numbering.indexOf("desc") != -1 ? rows.length - i : i;
                        if (tn.length == 0) {
                            tr.prepend('<td class="' + clazz + ' tf-numbering" contenteditable="false">' + n + '</td>');
                        } else {
                            tn.html(n).attr('contenteditable', false);
                        }
                    }
                });

                t.updateSorter(table);

                if (table.hasClass('metadata-summary-macro')) {   //TBLFILTER-605
                    t.tableWrapper.find('ol.aui-nav').eq(tableN).find('li a[href]').one('mouseup', function () {
                        t.pauseUpdate = true;
                        table.find('.tf-numbering').remove();
                        if (table[0].config && table[0].config.headerContent) {
                            table[0].config.headerContent.splice(0, 1);
                        }
                        table.trigger('updateAll', false);
                        t.pauseUpdate = false;
                    });
                }
            });
            t.pauseUpdate = false;
            t.setMutationObserver();
        } else {
            t.tables.find('.tf-numbering').remove();
            t.tables.find('colgroup col.tfn').remove();
        }
    };

    TableFilter.prototype.updateSorter = function(table) {
        var t = this;

        table.trigger('update', false);

        if (!t.resorting) {
            var tablesorter = $.tablesorter || $.tablesorterEditor;
            var tsRH = tablesorter.restoreHeaders;
            tablesorter.restoreHeaders = function(){};
            table.trigger('updateAll', false);
            table.one('updateComplete', function() {
                tablesorter.restoreHeaders = tsRH;
            });
            setTimeout(function() {
                tablesorter.restoreHeaders = tsRH;
            }, 2000);
        }
    };

    TableFilter.prototype.createSelects = function(container) {
        var t = this;
        container.find("select").change(function(e, data) {
            if (data.selected) {
                $(this).find("option[value='" + data.selected + "']").prop('selected', true);
            } else if (data.deselected) {
                $(this).find("option[value='" + data.deselected + "']").prop('selected', false);
            }
            t.comboBoxDiv.find('.search-choice').filter(function() {
                return TableUtils.clearString($(this).text()) == "-Empty-";
            }).addClass('tf-empty-option');
            t.filterTable();
            t.enableSaveBtn();
        }).each(function() {
                var select = $(this);
                select.chosen({
                    no_results_text: "No matches found. Press \u0027Enter\u0027 to add",
                    placeholder_text_multiple: select.is('.icon-select') ? "Select items..." : "Click or start typing...",
                    display_selected_options: false,
                    width: select.data('width'),
                    addUserOption: true,
                    isHtml: select.data('html')
                })
            });
    };

    TableFilter.prototype.createDatePickers = function(container) {
        var t = this;

        if (t.comboBoxDiv.datePicker) {
            if (!AJS.DatePicker.prototype.localisations) {  //Scroll Viewport
                AJS.DatePicker.prototype.localisations = [];
            }
            if (AJS.DatePicker.prototype.localisations.monthNames) {
                AJS.DatePicker.prototype.localisations = TableUtils.localisations[t.locale];
            } else {
                AJS.DatePicker.prototype.localisations[t.locale] = TableUtils.localisations[t.locale];
            }
            t.datePickerOptions = {overrideBrowserDefault: true, languageCode: t.locale};
        } else {
            t.datePickerOptions = TableUtils.localisations[t.locale];
        }

        container.find("input.date").each(function() {
            var input = $(this);
            var dateFormat = t.getDateFormat(input);
            t.datePickerOptions.dateFormat = dateFormat;

            if (t.comboBoxDiv.datePicker) {
                input.datePicker(t.datePickerOptions);
            } else {
                input.datepicker(t.datePickerOptions);
            }

            input.attr('dateformat', dateFormat);

            var value = input.val();
            input.val(TableUtils.convertToDate(value, dateFormat, t.locale));
        });
    };

    TableFilter.prototype.bindInputs = function() {
        var t = this;
        var timer = null;
        t.comboBoxDiv.delegate("input.user-filter, input.global-filter, input.date, input.number", "input keyup", function () {
            if (timer) {
                window.clearTimeout(timer);
            }
            timer = window.setTimeout(function () {
                timer = null;
                t.filterTable();
                t.enableSaveBtn();
            }, 330);
        });
        t.comboBoxDiv.delegate('input.date', "change", function () {
            t.filterTable();
            t.enableSaveBtn();
            if (typeof AP != "undefined") {
                AP.resize();
            }
        });
    };

    TableFilter.prototype.bindReset = function() {
        var t = this;
        t.comboBoxDiv.find('.filter-reset').click(function() {
            $('body').trigger('filter-options-disable-and-reset', {
                type: 'Reset Filters',
                pageMode: $('body').find('div#macro-browser-preview').length ? 'Macro Preview' : 'Page View'
            });
            t.clearFilters();
            t.filterTable();
            return false;
        });
    };

    TableFilter.prototype.clearFilters = function() {
        var t = this;
        t.comboBoxDiv.find("input.user-filter, input.global-filter, input.date, input.number").val('');
        var select = t.comboBoxDiv.find("select");
        select.find("option:selected").removeAttr('selected');
        select.trigger('chosen:updated');
        t.enableSaveBtn();
    };

    TableFilter.prototype.bindDisable = function() {
        var t = this;
        var width = t.lockEnabled.show().width() + 2;
        t.lockEnabled.hide();
        t.comboBoxDiv.find('div.lockDisabled.not-empty').css('min-width', width > 270 ? width : 270 + 'px');

        t.lockDisabled.find('.disableLock').click(function () {
            $('body').trigger('filter-options-disable-and-reset', {
                type: 'Disable All Filters',
                pageMode: $('body').find('div#macro-browser-preview').length ? 'Macro Preview' : 'Page View'
            });
            t.lockEnabled.width(t.lockDisabled.width());
            t.lockEnabled.height(t.lockDisabled.height());
            t.lockDisabled.hide();
            t.lockEnabled.show();
            t.isLockEnabled = true;
            t.showTableWithoutFilters();
            return false;
        });

        t.lockEnabled.find('.enableLock').click(function () {
            t.lockEnabled.width('');
            t.lockDisabled.show();
            t.lockEnabled.hide();
            t.isLockEnabled = false;
            t.filterTable();
            return false;
        });
    };

    TableFilter.prototype.bindDOMChange = function() {
        var t = this;
        var insertTimer = null;
        var prevValue = null;

        if (t.editorMacro) {
            var pause = false;
            var hash = t.tableWrapper.html();
            var prevTarget;

            var onInsert = function (target, type) {
                if (pause || t.pauseUpdate || t.sorting || t.resorting || t.editorSave) return;

                if (target.hasClass('fake') || target.is('br') || target.hasClass('tablesorter-savemyplace')) {
                    return;
                }
                if (target.is('.tf-numbering')) {
                    var macroParams = Confluence.MacroParameterSerializer.deserialize(t.editorMacro.attr('data-macro-parameters'));
                    t.numbering = macroParams.numbering ? macroParams.numbering : "";
                    return;
                }
                if (insertTimer) {
                    window.clearTimeout(insertTimer);
                }

                var isPrevNotTd = prevTarget && !prevTarget.is('td');

                insertTimer = window.setTimeout(function () {
                    prevTarget = null;
                    insertTimer = null;
                    if (type == 'DOMNodeInserted' && target.parents('.confluenceTable').length == 0) {
                        return;
                    }
                    var tmpHash = t.tableWrapper.html();
                    if (tmpHash == hash) {
                        return;
                    }
                    hash = tmpHash;
                    pause = true;
                    t.getTable();
                    t.addNumbering();
                    t.updateSelectOptions();
                    if (target.is('td') || target.is('.resize-sensor')) {
                        t.makeTablesScrollable();
                    } else {
                        t.limitTableHeight();
                    }
                    t.filterTable();
                    if (target.is('tr')) target.show();
                    target.find('tr').show();
                    if (isPrevNotTd || !target.is('td')) target.parents('tr').show();
                    var tables = t.tables.not('.tablesorter');
                    tables.tablesorterEditor({cancelSelection: false});
                    if (target.is('.tablesorter-header')) {
                        target.parents('table').eq(0).tablesorterEditor({cancelSelection: false});
                    }
                    t.appendSparklines();
                    pause = false;
                }, 500);

                prevTarget = target;
            };

            var onCharacterModified = function (target, eventPrevValue, eventNewValue) {
                if (t.editorSave || t.sorting || t.resorting) {
                    return;
                }
                if (insertTimer) {
                    window.clearTimeout(insertTimer);
                }

                if (prevValue == null) {
                    prevValue = eventPrevValue;
                }

                insertTimer = window.setTimeout(function () {
                    insertTimer = null;
                    hash = t.tableWrapper.html();
                    if (!target.is('td') && !target.is('th')) {
                        target = target.parents('td, th').eq(0);
                    }

                    if (parseInt(target.attr('rowspan')) > 1 || parseInt(target.attr('colspan')) > 1) {
                        t.normalizeTable();
                        t.showOrHideTables();
                    }
                    if (target.parents('.fht-thead').length) {
                        target.parents('.fht-table-wrapper').find('.fht-tbody thead tr').eq(0).find('.tablesorter-header').eq(target[0].cellIndex).text(target.text());
                    }
                    if (target.hasClass('tablesorter-header')) {
                        t.comboBoxDiv.find('[name="' + prevValue + '"]').attr('name', eventNewValue)
                            .each(function() {
                                var label = $(this).parents('.filter-container:eq(0)').find('label .tf-label-name');
                                label.text(label.text().replace(prevValue, eventNewValue));
                            });
                    }
                    prevValue = null;
                    t.updateRawTables();
                    t.updateSelectOptions();
                    t.tables.trigger('updateAll', false);
                    t.tables.trigger('update', false);
                    t.appendSparklines();
                }, 500);
            };

            if (window.MutationObserver) {
                var observerCallback = function(events) {
                    events.forEach(function(event) {
                        if (event.addedNodes.length) {
                            onInsert($(event.addedNodes), "DOMNodeInserted");
                        } else if (event.removedNodes.length) {
                            onInsert($(event.removedNodes), "DOMNodeRemoved");
                        } else if (event.type == 'characterData') {
                            var target = $(event.target);
                            onCharacterModified(target, event.oldValue, target.text());
                        }
                    });
                };
                var observer = new MutationObserver(observerCallback);
                observer.observe(t.tableWrapper[0], {childList: true, subtree: true, characterData: true, characterDataOldValue: true});

                t.tableWrapper.find('td').each(function() {
                    var observer = new MutationObserver(function(events) {
                        events.forEach(function(event) {
                            var target = $(event.target);
                            var hidden = target.hasClass('tf-hidden-column');
                            if (hidden || !TableUtils.isRowVisible(target.parent())) {
                                observer.disconnect();
                                target.attr(event.attributeName, event.oldValue);
                                if (hidden) {
                                    target.addClass('tf-hidden-column');
                                } else {
                                    target.removeClass('tf-hidden-column');
                                }
                                observer.observe(target[0], {
                                    attributes: true,
                                    attributeOldValue: true,
                                    attributeFilter: ['class', 'data-highlight-colour']
                                });
                            }
                        });
                    });
                    observer.observe(this, {
                        attributes: true,
                        attributeOldValue: true,
                        attributeFilter: ['class', 'data-highlight-colour']
                    });
                });
            } else {
                t.tableWrapper.unbind('DOMNodeInserted.tf DOMNodeRemoved.tf').bind('DOMNodeInserted.tf DOMNodeRemoved.tf', function(e) {
                    onInsert($(e.target), e.type);
                }).unbind('DOMCharacterDataModified.tf').bind('DOMCharacterDataModified.tf', function(e) {
                    onCharacterModified($(e.target), e.originalEvent.prevValue, e.originalEvent.newValue);
                });
            }
        } else {
            var onInsert = function(target, relatedNode) {
                if (t.sorting || t.resorting || t.pauseUpdate) return true;
                if (target.hasClass('fht-table') || target.hasClass('fht-table-wrapper') || target.attr('id') == 'floating-scrollbar') return true;

                if ((relatedNode && relatedNode.id == "tf-table-" + t.macroId) || target.hasClass("aui-nav-pagination")
                    || target.is('table') || target.hasClass("user-profile-list-result") || target.attr('id') == "teamcity"
                    || target.hasClass('profilelist') || target.hasClass('static-jira-issues_count') || target.hasClass('pocketquery-view-container')) {
                    if (insertTimer) {
                        window.clearTimeout(insertTimer);
                        insertTimer = null;
                    }

                    insertTimer = window.setTimeout(function () {
                        insertTimer = null;
                        t.getTable();
                        t.appendSparklines();
                        t.addNumbering();
                        if (t.limitHeight) {
                            if (target.hasClass("aui-nav-pagination")) {
                                if ($(relatedNode).hasClass('fht-tbody')) {
                                    setTimeout(function() {
                                        target.parents('.table-wrapper').append(target);
                                    }, 100);
                                }
                            }
                            t.makeTablesScrollable();
                        }
                        setTimeout(function() {
                            t.sortTables();
                            t.updateSelectOptions();
                            t.filterTable();
                        }, 1000);
                    }, 50);

                    return true;
                } else if (target.is('tr')) {
                    if (insertTimer) {
                        window.clearTimeout(insertTimer);
                        insertTimer = null;
                    }

                    insertTimer = window.setTimeout(function () {
                        insertTimer = null;
                        t.getTable();
                        t.appendSparklines();
                        t.addNumbering();
                        t.updateSelectOptions();
                        t.sortTables();
                        t.filterTable();
                    }, 10);

                    return true;
                } else if (target.hasClass('jira-issue')) {
                    if (insertTimer) {
                        window.clearTimeout(insertTimer);
                        insertTimer = null;
                    }

                    insertTimer = window.setTimeout(function () {
                        insertTimer = null;
                        t.updateSelectOptions();
                    }, 10);

                    return true;
                }
            };

            if (window.MutationObserver) {
                t.observerCallback = function(events) {
                    events.some(function(event) {
                        if (event.addedNodes.length) {
                            return onInsert($(event.addedNodes), event.target);
                        }
                    });
                };
                t.observer = new MutationObserver(t.observerCallback);
                t.observer.observe(t.tableWrapper[0], {childList: true, subtree: true});
            } else {
                t.tableWrapper.bind('DOMNodeInserted', function(e) {
                    onInsert($(e.target), e.originalEvent.relatedNode);
                });
            }

            t.tableWrapper.bind('DOMCharacterDataModified', function() {
                if (insertTimer) {
                    window.clearTimeout(insertTimer);
                    insertTimer = null;
                }

                insertTimer = window.setTimeout(function () {
                    insertTimer = null;
                    t.updateSelectOptions();
                }, 50);
            });
        }
    };

    TableFilter.prototype.setMutationObserver = function() {
        var t = this;
        if (t.observer) {
            if (t.sorting || t.resorting || t.pauseUpdate) {
                t.prevRowsLength = 0;
                t.rows.forEach(function(rows) {
                    t.prevRowsLength += rows.length;
                });
                t.observerCallback(t.observer.takeRecords());
                t.observer.disconnect();
            } else {
                if (t.observerRestoring) return;
                t.observerRestoring = true;
                t.getTable();
                var currentRowsLength = 0;
                t.rows.forEach(function(rows) {
                    currentRowsLength += rows.length;
                });
                if (t.prevRowsLength != undefined && currentRowsLength != t.prevRowsLength) {     //PPR + default sorting
                    t.prevRowsLength = currentRowsLength;
                    t.appendSparklines();
                    t.updateSelectOptions();
                    t.filterTable();
                }
                t.observerRestoring = false;
                t.observer.observe(t.tableWrapper[0], {childList: true, subtree: true});
            }
        }
    };

    TableFilter.prototype.processInnerFiltration = function(data) {
        var filtered = false;

        if (data && data.tables) {
            data.tables.each(function(i) {
                data.rows[i].each(function(j) {
                    var tr = $(this);

                    if (j > data.headerSize[i]) {
                        if (!data.state[i][j - 1]) {
                            tr.remove();
                            filtered = true;
                        }
                    }

                    if (data.tdNumbers[i].length) {
                        var tds = tr.children('td,th');
                        $.each(data.tdNumbers[i], function(n, number) {
                            tds.eq(number).remove();
                            filtered = true;
                        });
                    }
                });
            });
        }

        return filtered;
    };

    TableFilter.prototype.bindInnerFiltration = function() {
        var t = this;
        var tfId = t.tableWrapper.find('.pivot-content, .tablefilter-table-wrapper').filter(function () {
            return $(this).parents('table, .tablefilter-table-wrapper').eq(0).is(t.tableWrapper);
        }).eq(0).attr('id');
        if (tfId) {
            t.tableWrapper.bind('tf-filtered pt-calculated', function(e, data) {
                if (tfId == data.id) {
                    var filtered = t.processInnerFiltration(data);
                    t.getTable();
                    if (filtered) {
                        t.updateSelectOptions();
                    }

                    if (!data.tables) {
                        t.addNumbering();
                        if (t.limitHeight) {
                            t.makeTablesScrollable();
                        }
                        t.updateSelectOptions();
                    }
                    t.filterTable();
                }
            });
        }

        if (t.processInnerFiltration(t.tableWrapper.data('inner-filter'))) {
            t.getTable();
        }
    };

    TableFilter.prototype.bindHideBtn = function() {
        var t = this;
        var marginTop = parseInt(t.outerWrapper.css('margin-top'));

        t.comboBoxDiv.find('.tf-hider').click(function () {
            var target = t.tables.eq(0);
            var height = t.comboBoxDiv.outerHeight(true);
            var width = t.comboBoxDiv.width();
            t.comboBoxDiv.css({'position': 'absolute', top: t.comboBoxDiv.position().top - height});
            var targetWidth = target.outerWidth() - 2;
            var css = {'margin-top': (2 * marginTop + height) + 'px'};
            t.outerWrapper.css(css);
            t.comboBoxDiv.animate({top: target.position().top, opacity: 0, width: targetWidth, height: 14}, {complete:  function() {
                t.comboBoxDiv.hide().css({'width': '', height: '', position: ''});
                css = {'margin-top': marginTop + 'px'};
                t.outerWrapper.animate(css, {complete: function() {
                    t.outerWrapper.css('width', '');
                }});
                t.showBtn.show().width(targetWidth);
            }, duration: 300});
            TableUtils.setCookie(t.hideParamName, true);
        });

        t.showBtn.click(function() {
            var target = t.tables.eq(0);
            t.showBtn.hide();
            t.comboBoxDiv.show();
            var width = t.comboBoxDiv.width();
            var h = t.comboBoxDiv.height();
            var height = t.comboBoxDiv.outerHeight(true);
            t.comboBoxDiv.css({'position': 'absolute', opacity: 0});
            t.comboBoxDiv.css({width: target.width() - 2, height: 14});
            var css = {'margin-top': (2 * marginTop + height) + 'px'};
            t.outerWrapper.animate(css, {complete: function() {
                t.comboBoxDiv.css({top:  0})
                    .animate({top: -height, opacity: 1, width: width, height: h}, {complete: function() {
                        t.comboBoxDiv.css({'position': '', width: '', height: ''});
                        t.outerWrapper.css({'margin-top': marginTop + 'px', width: ''});
                    }, duration: 300});
            }});

            TableUtils.setCookie(t.hideParamName, false);
        });
    };

    TableFilter.prototype.bindTableEvents = function() {
        var t = this;

        if (!t.editorMacro) {
            var tableElement = t.limitHeight ? t.tables.eq(0).parents('.fht-table-wrapper') : t.tables.eq(0);
            tableElement.unbind('mouseover.tf' + t.macroId).bind('mouseover.tf' + t.macroId, function() {
                var offset = tableElement.offset();
                t.showBtn.parent().offset({top: offset.top - 14, left: offset.left + tableElement.parent().scrollLeft()});
                t.showBtn.css('visibility', 'visible');
                t.showBtn.width(tableElement.outerWidth() - 2);
            }).unbind('mouseout.tf' + t.macroId).bind('mouseout.tf' + t.macroId, function() {
                t.showBtn.css('visibility', '');
            });

            if (t.showBtnResizeSensor) t.showBtnResizeSensor.detach();
            if (tableElement.length) {
                t.showBtnResizeSensor = new ResizeSensor(tableElement, function() {
                    t.showBtn.width(tableElement.outerWidth() - 2);
                });
            }

            if (t.outerWrapperResizeSensor) t.outerWrapperResizeSensor.detach();
            t.outerWrapperResizeSensor = new ResizeSensor(t.outerWrapper, function() {
                t.showBtn.css('max-width', t.outerWrapper.width() - 2 + 'px');
            });
        }

        t.tables.unbind('sortStart.tfs').bind('sortStart.tfs', function() {
            t.sorting = true;
            t.setMutationObserver();
        }).unbind('sortEnd.tfs').bind('sortEnd.tfs', function() {
            t.sorting = false;
            t.getTable();
            t.resetSortBtn.removeClass('tf-disabled-link');
            if (t.restoreNumbering) {
                t.restoreNumbering = false;
                t.addNumbering();
            }

            t.setMutationObserver();
        });
    };

    TableFilter.prototype.bindAddFilter = function() {
        var t = this;

        var resize = function() {
            if (typeof AP != "undefined") {
                AP.resize();
            }
        };

        var dialog = AJS.InlineDialog(t.comboBoxDiv.find('.lockDisabled .filter-settings'), "tf-add-filter-" + t.macroId, function(contents, trigger, showPopup) {
            contents.css({"min-height":"166px"});
            var availableFilters = [];
            var filters = t.getFilters();
            var allFilters = [{
                type: "selectfilter",
                name: "Dropdown Filter",
                columns: [],
                existing: filters.selectfilters
            }, {
                type: "userfilter",
                name: "Free Text Filter",
                columns: [],
                existing: filters.userfilters
            }, {
                type: "numberfilter",
                name: "Number Filter",
                columns: [],
                existing: filters.numberfilters
            }, {
                type: "datefilter",
                name: "Date Filter",
                columns: [],
                existing: filters.datefilters
            }, {
                type: "iconfilter",
                name: "Visual Filter",
                columns: [],
                existing: filters.iconfilters
            }];

            $.each(allFilters, function(i, filter) {
                $.each(t.columns, function(i, column) {
                    if ($.inArray(column.replace(/\,/g, '‚'), filter.existing) == -1) {
                        filter.columns.push(column);
                    }
                });
                if (filter.columns.length) {
                    availableFilters.push(filter);
                }
            });

            contents.html(TableFilterTemplate.addFilterDialog({
                filters: availableFilters,
                globalfilter: filters.globalfilter.length == 0,
                columnhider: filters.columnhider.length == 0
            }));
            showPopup();
        }, {offsetX: -20, width: 270, noBind: true, gravity: 'n', initCallback: function() {
            var popup = this;
            var filterType = popup.popup.find('.filter-type');
            filterType.change(function() {
                var val = popup.popup.find('.tf-add-filter-column:visible select').val();
                popup.popup.find('div.tf-add-filter-column').hide();
                popup.popup.find('div.' + filterType.val()).show()
                    .find('option').filter(function() {
                        return $(this).text() == val;
                    }).attr('selected', 'selected');
            });
            popup.popup.find('#cancel').click(function() {
                popup.hide();
                resize();
                return false;
            });
            popup.popup.find('#save').click(function() {
                $(this).prop('disabled', true);
                var type = filterType.val();
                t.addFilter(type, popup.popup.find('select.' + type).val());
                popup.hide();
                $('body').trigger('filter-added-deleted', {
                    action: 'Filter Added',
                    type: type,
                    pageMode: $('body').find('div#macro-browser-preview').length ? 'Macro Preview' : 'Page View'
                });
                resize();
                return false;
            });

            if (t.editorMacro) {
                popup.popup.click(function() {
                    return false;
                });
            }
        }, hideCallback: function() {
            resize();
        }});

        t.menu.find('.filter-add').bind('click', function(e) {
            if (typeof AP != 'undefined' && e.clientY + 190 > $('body').height() - 10 && e.clientY - 240 < 0) {
                AP.resize('100%', e.clientY + 190);
            }

            dialog.show();
            if (t.menu.attr('aria-hidden') == 'false') {
                AJS.$('#filter-settings-' + t.macroId).trigger('aui-button-invoke');
            }

            return false;
        });
    };

    TableFilter.prototype.addFilter = function(type, column) {
        var t = this;

        var newFilter = $(TableFilterTemplate.filter({
            type: type,
            column: column
        }));

        var container = t.comboBoxDiv.find('.sortable-container');
        var typedFilter = container.find('.' + type + '-element');
        if (typedFilter.length) {
            typedFilter.last().after(newFilter);
        } else {
            switch (type) {
                case "selectfilter":
                    container.prepend(newFilter);
                    break;
                case "userfilter":
                    var selectFilter = container.find('.selectfilter-element');
                    if (selectFilter.length) {
                        selectFilter.last().after(newFilter);
                    } else {
                        container.prepend(newFilter);
                    }
                    break;
                case "numberfilter":
                    var userFilter = container.find('.userfilter-element');
                    if (userFilter.length) {
                        userFilter.last().after(newFilter);
                    } else {
                        var selectFilter = container.find('.selectfilter-element');
                        if (selectFilter.length) {
                            selectFilter.last().after(newFilter);
                        } else {
                            container.prepend(newFilter);
                        }
                    }
                    break;
                case "datefilter":
                    var numberFilter = container.find('.numberfilter-element');
                    if (numberFilter.length) {
                        numberFilter.last().after(newFilter);
                    } else {
                        var userFilter = container.find('.userfilter-element');
                        if (userFilter.length) {
                            userFilter.last().after(newFilter);
                        } else {
                            var selectFilter = container.find('.selectfilter-element');
                            if (selectFilter.length) {
                                selectFilter.last().after(newFilter);
                            } else {
                                container.prepend(newFilter);
                            }
                        }
                    }
                    break;
                case "iconfilter":
                    var globalFilter = container.find('.globalfilter-element');
                    if (globalFilter.length) {
                        globalFilter.before(newFilter);
                    } else {
                        var columnHider = container.find('.columnhider-element');
                        if (columnHider.length) {
                            columnHider.before(newFilter);
                        } else {
                            container.append(newFilter);
                        }
                    }
                    break;
                case "globalfilter":
                    var columnHider = container.find('.columnhider-element');
                    if (columnHider.length) {
                        columnHider.before(newFilter);
                    } else {
                        container.append(newFilter);
                    }
                    break;
                case "columnhider":
                    container.append(newFilter);
                    break;
            }
        }

        if (type == "selectfilter" || type == "columnhider" || type == "iconfilter") {
            t.createSelects(newFilter);
        } else if (type == "datefilter") {
            t.createDatePickers(newFilter);
        }

        t.updateSelectOptions();
        t.makeResizable(newFilter);
        t.enableSaveBtn();
        return newFilter;
    };

    TableFilter.prototype.bindChangeSettings = function() {
        var t = this;

        var resize = function() {
            if (typeof AP != "undefined") {
                AP.resize();
            }
        };

        var dialog = AJS.InlineDialog(t.comboBoxDiv.find('.lockDisabled .filter-settings'), "tf-change-settings-" + t.macroId, function(contents, trigger, showPopup) {
            contents.css({"min-height":"120px"});

            contents.html(TableFilterTemplate.changeSettingsDialog({
                datepattern: t.dateFormat
            }));
            showPopup();
        }, {offsetX: -20, width: 270, noBind: true, gravity: 'n', initCallback: function() {
            var popup = this;
            var datePatternInput = popup.popup.find('#tf-cs-dialog-datepattern');
            TableUtils.dateFormatAutocomplete(datePatternInput, popup.popup);

            datePatternInput.autocomplete("widget").click(function(e) {
                e.stopPropagation();
            });

            popup.popup.find('#tf-cs-cancel').click(function() {
                popup.hide();
                resize();
                return false;
            });
            popup.popup.find('#tf-cs-save').click(function() {
                t.changeSettings(datePatternInput.val());
                popup.hide();
                resize();
                return false;
            });

            if (t.editorMacro) {
                popup.popup.click(function() {
                    return false;
                });
            }
        }, hideCallback: function() {
            resize();
        }});

        t.menu.find('.tf-change-settings').bind('click', function(e) {
            dialog.show();
            if (t.menu.attr('aria-hidden') == 'false') {
                AJS.$('#filter-settings-' + t.macroId).trigger('aui-button-invoke');
            }

            return false;
        });
    };

    TableFilter.prototype.changeSettings = function(dateFormat) {
        var t = this;

        if (t.dateFormat != dateFormat) {
            t.enableSaveBtn();
            t.dateFormat = dateFormat;
            t.updateDateFilters();
            t.filterTable();
        }
    };

    TableFilter.prototype.updateDateFilters = function() {
        var t = this;

        t.comboBoxDiv.find("input.date").each(function() {
            var input = $(this);
            var dateFormat = t.getDateFormat(input);
            var prevDateFormat = input.attr('dateformat');

            if (dateFormat != prevDateFormat) {
                var value = input.val();
                var date = null;
                try {
                    date = TableUtils.parseDate(value, prevDateFormat, t.locale);
                } catch (e) {}
                var newInput = $(input[0].outerHTML);
                input.replaceWith(newInput);
                newInput.attr('value', date != null ? TableUtils.formatDate(date, dateFormat, t.locale) : value);
                newInput.attr('dateformat', dateFormat);
                $('.aui-datepicker-dialog').remove();

                t.datePickerOptions.dateFormat = dateFormat;

                if (t.comboBoxDiv.datePicker) {
                    newInput.datePicker(t.datePickerOptions);
                } else {
                    newInput.datepicker(t.datePickerOptions);
                }
            }
        });
    };

    TableFilter.prototype.getDateFormat = function(input) {
        var t = this;

        var cellName = input.attr('name');
        var dateFormat = t.dateFormat;

        t.tables.each(function (i) {
            var rows = t.rows[i];
            var tdNumber = TableUtils.getColumnPosition(rows, cellName);
            if (tdNumber != null && rows.find('td:eq(' + tdNumber + ') time').length) {
                dateFormat = t.localeDateFormat;
            }
        });

        return dateFormat;
    };

    TableFilter.prototype.bindSave = function() {
        var t = this;
        t.saveBtn.click(function() {
            var btn = t.saveBtn;
            if (btn.hasClass('tf-disabled-link'))  {
                return;
            }

            $('body').trigger('filter-options-save');

            var filters = t.getFilters();
            var data = {
                pageId: t.pageId,
                id: t.macroId,
                defaults: filters.defaults.join(','),
                width: filters.width.join(','),
                inverse: filters.inverse.join(','),
                order: filters.order.join(','),
                labels: t.getFilterLabels(),
                datepattern: t.dateFormat
            };
            if (filters.selectfilters.length) data.column = filters.selectfilters.join(',');
            if (filters.userfilters.length) data.userfilter = filters.userfilters.join(',');
            if (filters.numberfilters.length) data.numberfilter = filters.numberfilters.join(',');
            if (filters.datefilters.length) data.datefilter = filters.datefilters.join(',');
            if (filters.iconfilters.length) data.iconfilter = filters.iconfilters.join(',');
            if (filters.globalfilter.length) data.globalfilter = 'true';
            if (filters.columnhider.length) data.columnhider = 'true';

            var success = function() {
                btn.addClass('tf-disabled-link');
            };

            if (t.editorMacro) {
                data.column ? t.macroParams.column = data.column : delete t.macroParams.column;
                data.userfilter ? t.macroParams.userfilter = data.userfilter : delete t.macroParams.userfilter;
                data.numberfilter ? t.macroParams.numberfilter = data.numberfilter : delete t.macroParams.numberfilter;
                data.datefilter ? t.macroParams.datefilter = data.datefilter : delete t.macroParams.datefilter;
                data.globalfilter ? t.macroParams.globalFilter = data.globalfilter : delete t.macroParams.globalFilter;
                data.columnhider ? t.macroParams.hideColumns = data.columnhider : delete t.macroParams.hideColumns;
                data.iconfilter ? t.macroParams.iconfilters = data.iconfilter : delete t.macroParams.iconfilters;
                data.width ? t.macroParams['cell-width'] = data.width : delete t.macroParams['cell-width'];
                data.defaults ? t.macroParams['default'] = data.defaults : delete t.macroParams['default'];
                data.inverse ? t.macroParams['inverse'] = data.inverse : delete t.macroParams['inverse'];
                data.datepattern ? t.macroParams['datepattern'] = data.datepattern : delete t.macroParams['datepattern'];
                data.labels ? t.macroParams['labels'] = data.labels : delete t.macroParams['labels'];
                t.editorMacro.attr('data-macro-parameters', Confluence.MacroParameterSerializer.serialize(t.macroParams));
                success();
            } else {
                TFUtils.saveMacroParams(data);
                success();
            }
        });
    };

    TableFilter.prototype.bindExport = function() {
        var t = this;

        if (typeof saveAs == "undefined") {
            t.menu.find('.export-file-pdf').hide();
            t.menu.find('.export-file-doc').hide();
            t.menu.find('.export-file-csv').hide();
        } else {
            t.menu.find('.export-file-pdf').bind('click', function() {
                TFUtils.loadScriptsBeforeExport(function() {
                    TableUtils.exportToPdf($('#tf-table-' + t.macroId));
                });
            });

            t.menu.find('.export-file-doc').bind('click', function() {
                TableUtils.exportToDoc($('#tf-table-' + t.macroId));
            });

            t.menu.find(".export-file-csv").click(function() {
                TableUtils.createDialog(t.tableWrapper, t.rows[0], t.macroId).show();
                if (typeof AP != "undefined") {
                    $("#export-csv-dialog-" + t.macroId).css('top', t.comboBoxDiv.offset().top).css('margin-top', 0);
                }
            });
        }
    };

    TableFilter.prototype.bindSortReset = function() {
        var t = this;

        t.resetSortBtn.bind('click', function() {
            if (t.resetSortBtn.hasClass('tf-disabled-link'))  {
                return;
            }
            if (t.sorts.length) {
                t.sortTables();
            } else {
                t.tables.trigger('sorton', [[]]);
            }
            t.resetSortBtn.addClass('tf-disabled-link')
        });
    };

    TableFilter.prototype.bindCopyUrl = function() {
        var t = this;

        t.menu.find('.get-tf-url').bind('click', function() {
            var filters = t.getFilters();
            filters.columnhider = filters.columnhider.length;
            filters.globalfilter = filters.globalfilter.length;
            var hash = window.location.hash;
            var href = window.location.href.replace(hash, '');
            var param = $.getUrlHashVar('tf-filters');
            if (param) {
                var hashParam = 'tf-filters=' + encodeURIComponent(param);
                hash = hash.replace('&' + hashParam, '');
                hash = hash.replace('#' + hashParam, '');
            }
            href = href + hash + (hash ? '&' : '#');
            var span = $('<span></span>');
            $('body').append(span);
            span.text(href + 'tf-filters=' + encodeURIComponent(JSON.stringify(filters)));
            var rng = document.createRange();
            rng.selectNode(span[0]);
            var sel = window.getSelection();
            sel.removeAllRanges();
            sel.addRange(rng);
            document.execCommand('copy');
            span.remove();
        });
    };

    TableFilter.prototype.bindGetUrlValues = function() {
        var t = this;

        var setInverse = function(inverse, input) {
            if (inverse) {
                input.addClass('inverse');
                input.parents('.filter-container:eq(0)').addClass('inverse');
            } else {
                input.removeClass('inverse');
                input.parents('.filter-container:eq(0)').removeClass('inverse');
            }
        };

        var setFilters = function() {
            var param = $.getUrlHashVar('tf-filters');
            if (!param) return;
            var filters = JSON.parse(param);
            var n = 0;

            t.clearFilters();

            filters.selectfilters.forEach(function(filter) {
                filter = filter.replace(/\‚/g, ',');
                var input = t.comboBoxDiv.find('select.select[name="' + filter + '"]');
                if (input.length == 0) {
                    input = t.addFilter('selectfilter', filter).find('select');
                }
                setInverse(filters.inverse[n], input);
                var originalValues = filters.defaults[n++].replace(/\‚/g, ',');
                var values = TableUtils.clearStringLowerCase(originalValues).split('&&');
                originalValues = originalValues.split('&&');
                var selectedIndexes = [];

                input.find('option:selected').prop('selected', false);
                input.find('option').each(function() {
                    var opt = $(this);
                    var optIndex = $.inArray(TableUtils.clearStringLowerCase(opt.text()), values);
                    if (optIndex != -1) {
                        opt.prop('selected', true);
                        selectedIndexes.push(optIndex);
                    }
                });

                originalValues.forEach(function(v, i) {
                    if (v && $.inArray(i, selectedIndexes) == -1) {
                        input.append($('<option selected="selected"></option>').text(v));
                    }
                });

                input.trigger('chosen:updated').trigger('change', {});
            });

            filters.userfilters.forEach(function(filter) {
                filter = filter.replace(/\‚/g, ',');
                var input = t.comboBoxDiv.find('input.user-filter[name="' + filter + '"]');
                if (input.length == 0) {
                    input = t.addFilter('userfilter', filter).find('input');
                }
                setInverse(filters.inverse[n], input);
                input.val(filters.defaults[n++].replace(/\‚/g, ',')).trigger('input');
            });

            filters.numberfilters.forEach(function(filter) {
                filter = filter.replace(/\‚/g, ',');
                var inputFrom = t.comboBoxDiv.find('input.number.from[name="' + filter + '"]');
                if (inputFrom.length == 0) {
                    inputFrom = t.addFilter('numberfilter', filter).find('input.from');
                }
                var inputTo = t.comboBoxDiv.find('input.number.to[name="' + filter + '"]');
                setInverse(filters.inverse[n], inputFrom);
                setInverse(filters.inverse[n], inputTo);
                var values = filters.defaults[n++].replace(/\‚/g, ',').split('->');
                inputFrom.val(values[0]).trigger('input');
                inputTo.val(values[1]).trigger('input');
            });

            filters.datefilters.forEach(function(filter) {
                filter = filter.replace(/\‚/g, ',');
                var inputFrom = t.comboBoxDiv.find('input.date.from[name="' + filter + '"]');
                if (inputFrom.length == 0) {
                    inputFrom = t.addFilter('datefilter', filter).find('input.from');
                }
                var inputTo = t.comboBoxDiv.find('input.date.to[name="' + filter + '"]');
                setInverse(filters.inverse[n], inputFrom);
                setInverse(filters.inverse[n], inputTo);
                var values = filters.defaults[n++].replace(/\‚/g, ',').split('->');
                inputFrom.val(TableUtils.convertToDate(values[0], inputFrom.attr('dateformat'), t.locale)).trigger('input');
                inputTo.val(TableUtils.convertToDate(values[1], inputTo.attr('dateformat'), t.locale)).trigger('input');
            });

            if (filters.globalfilter) {
                var input = t.comboBoxDiv.find('input.global-filter');
                if (input.length == 0) {
                    input = t.addFilter('globalfilter', '').find('input');
                }
                setInverse(filters.inverse[n], input);
                input.val(filters.defaults[n++].replace(/\‚/g, ',')).trigger('input');
            }

            if (filters.columnhider) {
                var input = t.comboBoxDiv.find('select.column-select');
                if (input.length == 0) {
                    input = t.addFilter('columnhider', '').find('select');
                }
                setInverse(filters.inverse[n], input);
                var originalValues = filters.defaults[n++].replace(/\‚/g, ',');
                var values = TableUtils.clearStringLowerCase(originalValues).split('&&');
                originalValues = originalValues.split('&&');
                var selectedIndexes = [];

                input.find('option:selected').prop('selected', false);
                input.find('option').each(function() {
                    var opt = $(this);
                    var optIndex = $.inArray(TableUtils.clearStringLowerCase(opt.text()), values);
                    if (optIndex != -1) {
                        opt.prop('selected', true);
                        selectedIndexes.push(optIndex);
                    }
                });

                originalValues.forEach(function(v, i) {
                    if (v && $.inArray(i, selectedIndexes) == -1) {
                        input.append($('<option selected="selected"></option>').text(v));
                    }
                });

                input.trigger('chosen:updated').trigger('change', {});
            }

            filters.iconfilters.forEach(function(filter) {
                filter = filter.replace(/\‚/g, ',');
                var input = t.comboBoxDiv.find('select.icon-select[name="' + filter + '"]');
                if (input.length == 0) {
                    input = t.addFilter('iconfilter', filter).find('select');
                }
                setInverse(filters.inverse[n], input);
                var values = TableUtils.clearStringLowerCase(filters.defaults[n++].replace(/\‚/g, ',')).split('&&');
                input.find('option:selected').prop('selected', false);
                input.find('option').each(function() {
                    var opt = $(this);
                    if ($.inArray(TableUtils.clearStringLowerCase(opt.text()), values) != -1) {
                        opt.prop('selected', true);
                    }
                });
                input.trigger('chosen:updated').trigger('change', {});
            });
        };

        $(window).bind('hashchange', setFilters);
        $('.tablefilter-table-wrapper').one('tf-filtered', setFilters);
    };

    TableFilter.prototype.fixURIHash = function () {
        var param = $.getUrlHashVar('tf-filters');
        if (param) {
            var hash = location.hash;
            var cleanHash = location.hash.replace('&tf-filters=' + encodeURIComponent(param), '');
            if (hash != cleanHash) {
                location.hash = cleanHash;
                location.hash = hash;                 
            }
        }
    };

    TableFilter.prototype.bindMakeInverse = function() {
        var t = this;

        t.comboBoxDiv.delegate('.eq', 'click', function() {
            var filterWrapper = $(this).parents('.filter-container:eq(0)');
            filterWrapper.addClass('inverse');
            filterWrapper.find('input, select').addClass('inverse');
            $('body').trigger('filter-inverse-direct', {
                type: getFilterTypeByClassName(filterWrapper),
                pageMode: $('body').find('div#macro-browser-preview').length ? 'Macro Preview' : 'Page View',
                filterMode: 'Inverse'
            });
            t.enableSaveBtn();
            t.filterTable();
        });

        t.comboBoxDiv.delegate('.not-eq', 'click', function() {
            var filterWrapper = $(this).parents('.filter-container:eq(0)');
            filterWrapper.removeClass('inverse');
            filterWrapper.find('input, select').removeClass('inverse');
            $('body').trigger('filter-inverse-direct', {
                type: getFilterTypeByClassName(filterWrapper),
                pageMode: $('body').find('div#macro-browser-preview').length ? 'Macro Preview' : 'Page View',
                filterMode: 'Normal'
            });
            t.enableSaveBtn();
            t.filterTable();
        });
    };

    TableFilter.prototype.updateSelectOptions = function() {
        var t = this;
        t.pauseUpdate = true;
        t.setMutationObserver();
        TableUtils.normalizeTable(t.tables);
        t.pauseUpdate = false;
        t.setMutationObserver();

        var columnsLowerCase = t.columns.map(function(elem) { return elem.toLowerCase(); });
        var filters = t.comboBoxDiv.find('select.select, input.user-filter, div.range, select.icon-select');

        filters.each(function() {
            var filter = $(this);
            if ($.inArray(TableUtils.clearStringLowerCase(filter.attr('name')), columnsLowerCase) == -1) {
                filter.parents('.filter-container').eq(0).hide();
            } else {
                filter.parents('.filter-container').eq(0).show();
            }
        });

        var selects = t.comboBoxDiv.find('select.select, select.icon-select').filter(function() {
            return $(this).parent().css('display') != "none";
        });
        selects.find('option:not(:selected)').remove();

        selects.each(function() {
            var select = $(this);
            var name = select.attr('name').toLowerCase();
            var isIcon = select.data('html');
            var lookForCheckboxes = true;
            var values = [];
            var options = [];
            select.find('option').each(function() {
                if (isIcon) {
                    var text = $(this).text();
                    if (text != "-Empty-") {
                        var el = $(text);
                        if (el.is('img')) {
                            text = el[0].src;
                        }
                    } else {
                        options.push('');
                    }
                    options.push(text.toLocaleLowerCase());
                } else {
                    options.push($(this).hasClass('tf-empty-option') ? '' : TableUtils.clearStringLowerCase($(this).text()));
                }
            });

            $.each(t.rawTables, function(i, table) {
                var tdNumber = TableUtils.getCellPosition(table, name);

                if (tdNumber != null) {
                    $.each(isIcon ? t.rawIconTables[i] : table, function(j, row) {
                        if (j != 0) {
                            var tdValue = row[tdNumber];
                            if (tdValue == undefined) {
                                return;
                            }
                            if (isIcon && tdValue) {
                                var el = $(tdValue);
                                el.filter('img').each(function() {
                                    tdValue = TableUtils.clearStringLowerCase(this.src);
                                    if ($.inArray(tdValue, options) == -1) {
                                        options.push(tdValue);
                                        values.push(this.outerHTML);
                                    }
                                });

                                if (el.filter('ul').length) {
                                    if (lookForCheckboxes) {
                                        lookForCheckboxes = false;
                                        if ($.inArray('<ul class="inline-task-list"><li></li></ul>', options) == -1) {
                                            values.push('<ul class="inline-task-list"><li></li></ul>');
                                            options.push('<ul class="inline-task-list"><li></li></ul>');
                                        }
                                        if ($.inArray('<ul class="inline-task-list"><li class="checked"></li></ul>', options) == -1) {
                                            values.push('<ul class="inline-task-list"><li class="checked"></li></ul>');
                                            options.push('<ul class="inline-task-list"><li class="checked"></li></ul>');
                                        }
                                    }
                                    return true;
                                }
                            } else if (!t.ddSeparator || tdValue == '') {
                                tdValue = tdValue.toLocaleLowerCase();
                                if ($.inArray(tdValue, options) == -1) {
                                    options.push(tdValue);
                                    values.push(row[tdNumber]);
                                }
                            } else {
                                tdValue.split(t.ddSeparator).forEach(function(value) {
                                    value = value.trim();
                                    if (value) {
                                        tdValue = value.toLocaleLowerCase();
                                        if ($.inArray(tdValue, options) == -1) {
                                            options.push(tdValue);
                                            values.push(value);
                                        }
                                    }
                                });
                            }
                        }
                    });

                    if (!isIcon && $.inArray("@currentuser", options) == -1) {
                        tdNumber++;
                        var userLink = t.tables.eq(i).find('tr td:nth-child(' + tdNumber + '), tr th:nth-child(' + tdNumber + ')').find('.confluence-userlink').eq(0);
                        if (userLink.length && userLink.parents('td').eq(0).text().trim() == userLink.text().trim()) {
                            options.push("@currentuser");
                            values.push("@currentUser");
                        }
                    }
                }
            });

            values.sort();

            $.each(values, function(i, value) {
                var clazz = '';
                var optVal = value;
                if (value == '') {
                    clazz = ' class="tf-empty-option"';
                    optVal = "-Empty-";
                }
                select.append($('<option' + clazz + '></option>').text(optVal));
            });

            select.trigger('chosen:updated');
        });

        t.comboBoxDiv.find('.search-choice').filter(function() {
            return TableUtils.clearString($(this).text()) == "-Empty-";
        }).addClass('tf-empty-option');

        var hideColumns = t.comboBoxDiv.find('select.column-select');
        hideColumns.find('option:not(:selected)').remove();
        var options = [];
        hideColumns.find('option').each(function() {
            options.push(TableUtils.clearString($(this).text()));
        });
        $.each(t.columns, function(i, value) {
            if ($.inArray(value, options) == -1) {
                hideColumns.append($('<option></option>').text(value));
            }
        });

        hideColumns.trigger('chosen:updated');

        t.updateDateFilters();

        var filtersVisible = filters.filter(function(){return $(this).parent().css('display') != "none";}).length > 0
            || hideColumns.length > 0 || t.comboBoxDiv.find('.global-filter').length > 0;
        var showNRowsEnabled = t.showNRows != "all";

        if (t.rawTables.length == 0) {
            t.comboBoxDiv.show();
            t.comboBoxDiv.find('.no-table-message').show();
            t.comboBoxDiv.find('.empty-message').hide();
            t.lockDisabled.hide();
            t.lockEnabled.hide();
            t.outerWrapper.addClass('no-table');
            t.comboBoxDiv.find('.tf-hider-wrapper').hide();
        } else {
            t.comboBoxDiv.find('.tf-hider-wrapper').show();
            t.outerWrapper.removeClass('no-table');
            t.comboBoxDiv.find('.no-table-message').hide();
            t.isLockEnabled = t.isLockEnabled && (filtersVisible || showNRowsEnabled);
            if (!t.isLockEnabled) {
                t.lockDisabled.show();
                t.lockEnabled.hide();
            } else {
                t.lockDisabled.hide();
                t.lockEnabled.show();
            }

            if (!(!filtersVisible && !showNRowsEnabled && (t.numbering || t.limitHeight || t.sorts.length || t.sparkline))) {
                if(!t.editorMacro && TableUtils.getCookie(t.hideParamName) == 'true') {
                    t.comboBoxDiv.hide();
                    t.showBtn.show();
                } else {
                    t.comboBoxDiv.show();
                    t.showBtn.hide();
                }
            } else if (!t.editorMacro) {
                t.comboBoxDiv.hide();
                t.showBtn.show();
            }

            if (!filtersVisible && showNRowsEnabled) {
                t.comboBoxDiv.find('.disableLock').show();
                t.comboBoxDiv.find('.show-n-rows-only-message').show();
            } else {
                t.comboBoxDiv.find('.show-n-rows-only-message').hide();
            }

            if (filtersVisible || showNRowsEnabled) {
                t.comboBoxDiv.find('.filter-buttons-container').show();
                t.comboBoxDiv.find('.empty-message').hide();
                t.lockDisabled.addClass('not-empty').removeClass('empty');
                var parentCss = t.outerWrapper.css('display');
                t.outerWrapper.show();
                var css = t.comboBoxDiv.css('display');
                t.comboBoxDiv.show();
                t.lockEnabled.css('width', '');
                var width = Math.ceil(t.lockEnabled.show().width());
                t.lockDisabled.css('min-width', width + 'px');
                t.lockDisabled.show();
                t.lockEnabled.width(Math.ceil(t.lockDisabled.width()));
                t.lockEnabled.height(t.lockDisabled.height());
                t.comboBoxDiv.css('display', css);
                t.outerWrapper.css('display', parentCss);
                if (!t.isLockEnabled) t.lockEnabled.hide();
                else                  t.lockDisabled.hide();
            } else {
                t.lockDisabled.addClass('empty').removeClass('not-empty').css('min-width', '');
                t.comboBoxDiv.find('.filter-buttons-container').show();
                t.comboBoxDiv.find('.empty-message').show();
            }

            if (filtersVisible) {
                t.comboBoxDiv.find('.lockDisabled .filter-reset').prop('disabled', false);
                t.comboBoxDiv.find('.lockDisabled .disableLock').prop('disabled', false);
            } else {
                t.comboBoxDiv.find('.lockDisabled .filter-reset').prop('disabled', true);
                if (!showNRowsEnabled)
                    t.comboBoxDiv.find('.lockDisabled .disableLock').prop('disabled', true);
            }
        }

        t.outerWrapper.css('visibility', 'visible');
    };

    TableFilter.prototype.makeSortable = function() {
        var t = this;
        t.comboBoxDiv.find('.sortable-container').sortable({
            cancel: 'input,textarea,button,select,option,.chosen-drop',
            scroll: false,
            items: ".sortable",
            sort: function(e, ui) {
                t.highlightRemoveFilter(ui.offset, ui.helper);
            },
            beforeStop: function(e, ui) {
                t.enableSaveBtn();
                t.removeFilter(ui.offset, ui.item);
            },
            update: function(e, ui) {
                $('body').trigger('filter-reordered', {
                    type: getFilterTypeByClassName(ui.item),
                    pageMode: $('body').find('div#macro-browser-preview').length ? 'Macro Preview' : 'Page View'
                });
            }
        });
    };

    TableFilter.prototype.makeResizable = function(container) {
        var t = this;
        if ($('body.content-preview').length == 0) {
            container.find(".chosen-container, div.tf-resizeable").resizable({
                handles: "e",
                resize: function(e, ui) {
                    if (ui.element.hasClass('chosen-container')) {
                        ui.element.parent().find('select').attr('data-width', ui.size.width);
                    } else {
                        ui.element.find('input').attr('data-width', ui.size.width);
                    }
                    t.enableSaveBtn();
                }
            });
        }
    };

    TableFilter.prototype.checkHead = function(table) {
        var head = table.children('thead').filter(function() {
            return TableUtils.getRows($(this)).length > 0;
        });
        return head.length != 0;
    };

    TableFilter.prototype.makeTablesScrollable = function() {
        var t = this;

        t.tableWrapper.find('.fht-table-wrapper').each(function() {
            var fht = $(this);
            fht.find('.resize-sensor, .resize-sensor-shrink, .resize-sensor-expand').remove();
            fht.before(fht.find('.fht-tbody').find('table.fht-table'));
            fht.remove();
        });

        t.tables.fixedHeaderTable('destroy');
        t.tables.removeClass('fht-table fht-table-init');
        t.tables.css('margin-top', '');
        t.tables.css('margin-right', '');

        if (t.editorMacro) {
            t.tables.each(function() {
                var table = $(this);
                var thead = table.find('thead.limit-height');
                table.find('tbody').eq(0).prepend(thead.children());
                thead.remove();
            });
        }
        t.limitTableHeight();
    };

    TableFilter.prototype.limitTableHeight = function() {
        var t = this;

        if (t.limitHeight) {
            TableUtils.normalizeTable(t.tables);
            var rows = t.rows.slice(0);

            t.tables.not('.fht-table, .stickyTableHeaders').each(function(n) {
                var table = $(this);

                if (table.find("caption").filter(function() {return $(this).css('display') != 'none'}).length) {
                    if (t.limitTableHeightTimeout) {
                        clearTimeout(t.limitTableHeightTimeout);
                        t.limitTableHeightTimeout = null;
                    }

                    t.limitTableHeightTimeout = setTimeout(function() {
                        t.limitTableHeight();
                    }, 1000);
                    return;
                }

                if (!t.checkHead(table)) {
                    if (table.data('fixed-header') || t.editorMacro) {
                        table.data('fixed-header', false);
                        table.prepend($('<thead class="limit-height"/>').append(TableUtils.getRows(table).eq(0)));
                    } else {
                        if (t.limitTableHeightTimeout) {
                            clearTimeout(t.limitTableHeightTimeout);
                            t.limitTableHeightTimeout = null;
                        }

                        table.data('fixed-header', true);
                        t.limitTableHeightTimeout = setTimeout(function() {
                            t.limitTableHeight();
                        }, 1000);
                        return;
                    }
                }

                try {
                    t.showTableWithoutFilters();
                    t.pauseUpdate = true;
                    if ("MacIntel".indexOf(navigator.platform) > -1 && table.hasClass('fixed-table') && navigator.userAgent.indexOf("Safari") > -1) {
                        table.removeClass('fixed-table');
                        table.addClass('safari');
                    }
                    t.setMutationObserver();
                    table.fixedHeaderTable({height: (t.heightValue ? t.heightValue : Math.floor($(window).height() * 0.7)) + ''});
                    t.pauseUpdate = false;
                    t.setMutationObserver();
                    table.css('width', '');

                    var fhtBody = table.parents('.fht-tbody');
                    var fhtWrapper = table.parents('.fht-table-wrapper');
                    var headTableWrapper = fhtWrapper.find('.fht-thead');
                    var headTable = headTableWrapper.find('table');
                    var fhtHeadCells = headTableWrapper.find('tr:not(.tf-header-hidden):eq(0)').find('th,td');
                    var isAUI = table.hasClass('aui');
                    if (isAUI) {
                        fhtWrapper.css('width', '100%');
                    }
                    table.find('.fht-cell').remove();
                    headTableWrapper.find('.fht-cell').remove();

                    var colString = "<colgroup>";
                    fhtHeadCells.each(function() {
                        colString += "<col>";
                    });
                    colString += "</colgroup>";
                    headTable.prepend(colString);
                    var bodyColgroup = table.find('colgroup');
                    var headColgroup = headTable.find('colgroup');
                    var headCols = headTable.find('col');
                    var row = rows[n].not('.tf-header-hidden').eq(0);
                    var tds = row.children();

                    if ("MacIntel".indexOf(navigator.platform) > -1 && table.hasClass('fixed-table') && navigator.userAgent.indexOf("Safari") > -1) {
                        table.addClass('fixed-table');
                    }

                    if (bodyColgroup.length) {
                        headTable.width('');
                    }

                    var colgroupChanged = false;
                    if (t.editorMacro) {
                        headTable.find('thead').changeElementType('tbody');
                        AJS.bind('synchrony.stop', function (e, o) {
                            if (o && o.id == "confluence.table-resize-plugin") {
                                if (headTable.hasClass('active-resizable') || headTable.is(':hover')) {
                                    table.css('width', headTable.css('width'));
                                    bodyColgroup.html(headColgroup.html());
                                    fhtWrapper.css({width: headTable.width() + 'px', 'min-width': ''});
                                } else if (table.hasClass('active-resizable') || table.is(':hover')) {
                                    headTable.css('width', table.css('width'));
                                    fhtWrapper.css({width: headTable.width() + 'px', 'min-width': ''});
                                    headCols.each(function (i) {
                                        $(this).width(tds.eq(i).outerWidth());
                                    });
                                }
                            }

                            colgroupChanged = true;

                            table.css('margin-top', '-' + headTableWrapper.height() + 'px');
                            fhtBody.css('max-height', (parseInt(fhtWrapper.css('max-height')) - headTableWrapper.height()) + 'px');

                            if (fhtBody[0].offsetHeight < fhtBody[0].scrollHeight - 1) {
                                fhtWrapper.addClass('overflow');
                            } else {
                                fhtWrapper.removeClass('overflow');
                            }
                        });
                    }

                    t.resize = function() {
                        if (colgroupChanged) {
                            colgroupChanged = false;
                            return;
                        }
                             
                        if ("MacIntel".indexOf(navigator.platform) < 0) {
                            table.add(headTable).css('margin-right', (-18 / (window.devicePixelRatio || 1)) + 'px');
                        }

                        if (table.hasClass('fixed-table')) {
                            fhtWrapper.css({width: table.width() + 'px', 'min-width': ''});
                        }

                        headCols.each(function (i) {
                            $(this).width(tds.eq(i).outerWidth());
                        });

                        table.css('margin-top', '-' + headTableWrapper.height() + 'px');
                        fhtBody.css('max-height', (parseInt(fhtWrapper.css('max-height')) - headTableWrapper.height()) + 'px');

                        if (fhtBody[0].offsetHeight < fhtBody[0].scrollHeight - 1) {
                            fhtWrapper.addClass('overflow');
                        } else {
                            fhtWrapper.removeClass('overflow');
                        }
                        if (table.width() - 5 > fhtWrapper.width()) {
                            fhtWrapper.css('min-width', table.width() + 'px');
                        } else if (table.width() + 5 < fhtWrapper.width()) {
                            fhtWrapper.css('min-width', table.width() + 'px');
                        }
                    };
                    var bindResize = function() {
                        if (t.fhtResizeSensor) t.fhtResizeSensor.detach();
                        t.fhtResizeSensor = new ResizeSensor(table.add(tds), t.resize);
                    };

                    t.resize();
                    bindResize();

                    t.bindScrollableEvents(table);
                    t.getTable();
                    t.filterTable();

                    if (t.editorMacro) {
                        if (fhtWrapper.next().hasClass('fht-table-wrapper')) {
                            fhtWrapper.after('<p></p>')
                        }
                        if (fhtWrapper.prev().hasClass('fht-table-wrapper')) {
                            fhtWrapper.before('<p></p>')
                        }
                    }
                } catch(e) {
                    t.filterTable();
                }
            });
        }
    };

    TableFilter.prototype.bindScrollableEvents = function(table) {
        var t = this;
        var fhtBody = table.parents('.fht-tbody');
        var fhtWrapper = table.parents('.fht-table-wrapper');
        var headTable = fhtWrapper.find('.fht-thead');
        var headSortableThs = headTable.find('th.sortableHeader, td.sortableHeader, th.jira-tablesorter-header, th.tablesorter-header, th.tablesorter-header, td.tablesorter-header');
        var bodySortableThs = table.find('th.sortableHeader, td.sortableHeader, th.jira-tablesorter-header, th.tablesorter-header, th.tablesorter-header, td.tablesorter-header');
        headSortableThs.mousedown(function(e) {
            bodySortableThs.eq($.inArray(this, headSortableThs)).trigger(e);
        }).mouseup(function(e) {
            bodySortableThs.eq($.inArray(this, headSortableThs)).trigger(e);
            headSortableThs.each(function(i) {
                $(this).attr('class', bodySortableThs.eq(i).attr('class'));
            });
        }).click(function(e) {
            bodySortableThs.eq($.inArray(this, headSortableThs)).trigger(e);
            headSortableThs.each(function(i) {
                $(this).attr('class', bodySortableThs.eq(i).attr('class'));
            });
        });

        var headCollapsers = headTable.find('.collapser');
        var bodyCollapsers = table.find('.collapser');
        headCollapsers.unbind('click.tf mousedown.tf mouseup.tf').bind('click.tf mousedown.tf mouseup.tf', function(e) {
            bodyCollapsers.eq($.inArray(this, headCollapsers)).trigger(e);
        });
        table.unbind('pivot-collapsed').bind('pivot-collapsed', function() {t.makeTablesScrollable()});

        fhtBody.unbind('mousewheel.tf').bind('mousewheel.tf', function(e) {
            if (fhtBody.get(0).scrollHeight > fhtBody.get(0).clientHeight + 5) {
                var delta = e.originalEvent.wheelDelta;
                if (delta && !e.originalEvent.wheelDeltaX) {
                    fhtBody.scrollTop(fhtBody.scrollTop() - delta);
                    return false;
                }
            }
        });
    };

    TableFilter.prototype.enableSaveBtn = function() {
        this.saveBtn.removeClass('tf-disabled-link');
    };

    TableFilter.prototype.shouldRemoveFilter = function(offset) {
        var t = this;
        var divOffset = t.comboBoxDiv.offset();
        var right = divOffset.left + t.comboBoxDiv.width() + 10;
        var left = divOffset.left - 10;
        var bottom = divOffset.top + t.comboBoxDiv.height() - 10;
        var top = divOffset.top - 20;

        return offset.top < top || offset.top > bottom || offset.left > right || offset.left < left;
    };

    TableFilter.prototype.removeFilter = function(offset, item) {
        var t = this;

        t.outerWrapper.click();
        if (t.shouldRemoveFilter(offset)) {
            item.remove();
            $('body').trigger('filter-added-deleted', {
                action: 'Filter Deleted',
                type: getFilterTypeByClassName(item),
                pageMode: $('body').find('div#macro-browser-preview').length ? 'Macro Preview' : 'Page View'
            });
            setTimeout(function() {t.filterTable(); t.updateSelectOptions();}, 500);
        } else {
            item.removeClass('filter-removing');
        }
    };

    TableFilter.prototype.highlightRemoveFilter = function(offset, item) {
        var t = this;

        if (t.shouldRemoveFilter(offset)) {
            item.addClass('filter-removing');
        } else {
            item.removeClass('filter-removing');
        }
    };

    TableFilter.prototype.getFilters = function() {
        var t = this;
        var data = {
            selectfilters: [],
            userfilters: [],
            numberfilters: [],
            datefilters: [],
            globalfilter: t.comboBoxDiv.find('input.global-filter'),
            columnhider: t.comboBoxDiv.find('select.column-select'),
            iconfilters: [],
            defaults: [],
            width: [],
            inverse: [],
            order: []
        };

        var filterContainers = t.comboBoxDiv.find('.sortable-container .filter-container');

        t.comboBoxDiv.find('select.select').each(function() {
            var selected = [];
            var select = $(this);
            data.selectfilters.push(select.attr('name').replace(/\,/g, '‚'));
            data.width.push(select.attr('data-width').replace('px', ''));
            select.find('option:selected').each(function() {
                selected.push($(this).text().replace(/\,/g, '‚'));
            });
            data.defaults.push(selected.join('&&'));
            data.inverse.push(select.hasClass('inverse'));
            data.order.push(filterContainers.index(select.parents('.filter-container')));
        });
        t.comboBoxDiv.find('input.user-filter').each(function() {
            var input = $(this);
            data.userfilters.push(input.attr('name').replace(/\,/g, '‚'));
            data.width.push(input.attr('data-width').replace('px', ''));
            data.defaults.push(input.val().replace(/\,/g, '‚'));
            data.inverse.push(input.hasClass('inverse'));
            data.order.push(filterContainers.index(input.parents('.filter-container')));
        });
        t.comboBoxDiv.find('.range.numbers').each(function() {
            var selected = [];
            var input = $(this);
            data.numberfilters.push(input.attr('name').replace(/\,/g, '‚'));
            input.find('input.number').each(function() {
                selected.push($(this).val().replace(/\,/g, '‚'));
            });
            data.defaults.push(selected.join('->'));
            data.inverse.push(input.parent().hasClass('inverse'));
            data.order.push(filterContainers.index(input.parents('.filter-container')));
        });
        t.comboBoxDiv.find('.range.dates').each(function() {
            var selected = [];
            var input = $(this);
            data.datefilters.push(input.attr('name').replace(/\,/g, '‚'));
            input.find('input.date').each(function() {
                var dateInput = $(this);
                var value = dateInput.val();
                try {
                    var date = TableUtils.parseDate(value, dateInput.attr('dateformat'), t.locale);
                    var timestamp = date.getTime() - date.getTimezoneOffset() * 60000;
                    value = timestamp + '';
                } catch (e) {}
                selected.push(value.replace(/\,/g, '‚'));
            });
            data.defaults.push(selected.join('->'));
            data.inverse.push(input.parent().hasClass('inverse'));
            data.order.push(filterContainers.index(input.parents('.filter-container')));
        });

        if (data.globalfilter.length) {
            data.defaults.push(data.globalfilter.val().replace(/\,/g, '‚'));
            data.width.push(data.globalfilter.attr('data-width').replace('px', ''));
            data.inverse.push(data.globalfilter.hasClass('inverse'));
            data.order.push(filterContainers.index(data.globalfilter.parents('.filter-container')));
        }

        if (data.columnhider.length) {
            var selected = [];
            data.columnhider.find('option:selected').each(function() {
                selected.push($(this).text().replace(/\,/g, '‚'));
            });
            data.defaults.push(selected.join('&&'));
            data.width.push(data.columnhider.attr('data-width').replace('px', ''));
            data.inverse.push(data.columnhider.hasClass('inverse'));
            data.order.push(filterContainers.index(data.columnhider.parents('.filter-container')));
        }

        t.comboBoxDiv.find('select.icon-select').each(function() {
            var selected = [];
            var select = $(this);
            data.iconfilters.push(select.attr('name').replace(/\,/g, '‚'));
            data.width.push(select.attr('data-width').replace('px', ''));
            select.find('option:selected').each(function() {
                selected.push($(this).text().replace(/\,/g, '‚'));
            });
            data.defaults.push(selected.join('&&'));
            data.inverse.push(select.hasClass('inverse'));
            data.order.push(filterContainers.index(select.parents('.filter-container')));
        });

        return data;
    };

    TableFilter.prototype.getTable = function() {
        var t = this;

        var getMaxRowspan = function($tr) {
            var maxRowspan = 1;

            $tr.find('td, th').each(function() {
                var rowspan = $(this).attr('rowspan');
                if (typeof rowspan != 'undefined' && rowspan > maxRowspan) {
                    maxRowspan = rowspan;
                }
            });

            return maxRowspan - 1;
        };

        t.tables = t.tableWrapper.find(t.hasPivot ? '.pivot-table table' : t.editorMacro ? 'table.confluenceTable' : 'table')
            .not('.fht-thead table')
            .not('#i_confiform_registration_form table')
            .not('.wysiwyg-macro').filter(function () {
                var parents = $(this).parents('table:not(.wysiwyg-macro), .tablefilter-table-wrapper');
                return parents.eq(0).is(t.tableWrapper) ||
                    (parents.eq(0).is('.tablefilter-table-wrapper') && parents.eq(1).is(t.tableWrapper));
            });

        //эта нормализация таблицы должна оставаться здесь, т.к. Scaffolding reports тоже переделывает таблицу наоборот,
        //но уже после нашей основной нормализации
        t.normalizeTable();

        t.rows = [];
        t.headerSize = [];

        t.tables.each(function() {
            var rows = TableUtils.getRows($(this));
            t.rows.push(rows);
            t.headerSize.push(getMaxRowspan($(rows[0])));
        });

        t.updateRawTables();
        t.bindTableEvents();
    };

    TableFilter.prototype.updateRawTables = function() {
        var t = this;
        var html = "";
        t.tables.each(function() {
            html += this.outerHTML;
        });
        t.rawTables = TableUtils.getTables($('<div class="tablefilter-table-wrapper">' + html + '</div>'), '.tablefilter-table-wrapper');
        t.rawIconTables = TableUtils.getTables($('<div class="tablefilter-table-wrapper">' + html + '</div>'), '.tablefilter-table-wrapper', true);
        t.columns = TableUtils.getColumns(t.rawTables);
    };

    TableFilter.prototype.filterTable = function() {
        var t = this;
        if (t.isLockEnabled) {
            return;
        }
        this.showTable(t.isAND);
        t.filtered = false;

        t.comboBoxDiv.find('select.select').filter(function() {
            return $(this).parent().css('display') != "none";
        }).each(function() {
            t.treatSelectFilter($(this));
        });
        t.comboBoxDiv.find('input.user-filter').filter(function() {
            return $(this).parent().css('display') != "none";
        }).each(function() {
            t.treatUserFilter($(this));
        });
        t.comboBoxDiv.find('input.number').filter(function() {
            return $(this).parent().parent().css('display') != "none";
        }).each(function() {
            t.treatNumberFilter($(this));
        });
        t.comboBoxDiv.find('input.date').filter(function() {
            return $(this).parent().parent().css('display') != "none";
        }).each(function() {
            t.treatDateFilter($(this));
        });
        t.comboBoxDiv.find('input.global-filter').each(function() {
            t.treatGlobalFilter($(this));
        });
        t.comboBoxDiv.find('select.column-select').each(function() {
            t.treatColumnFilter($(this));
        });
        t.comboBoxDiv.find('select.icon-select').filter(function() {
            return $(this).parent().css('display') != "none";
        }).each(function() {
            t.treatIconFilter($(this));
        });

        if (!t.filtered && !t.isAND) {
            t.showTable(true);
        }

        t.showOrHideTables();
        if (t.updateSparkline) {
            t.appendSparklines();
            t.updateSparkline = false;
        }
    };

    TableFilter.prototype.showTable = function(value) {
        var t = this;

        var length = t.tables.length;

        t.cbState = new Array(length);
        t.tdNumbers = new Array(length);

        for (var i = 0; i < length; i++) {
            t.tdNumbers[i] = [];
            t.cbState[i] = new Array(t.rows[i].length > 0 ? t.rows[i].length - 1 : 0);
            for (var j = 0; j < t.cbState[i].length; j++) {
                t.cbState[i][j] = value;
            }
        }
    };

    TableFilter.prototype.treatSelectFilter = function(filter) {
        var t = this;
        var tdValues = [];
        filter.find('option:selected').each(function() {
            var text = $.trim($(this).text());
            if (text == "-Empty-") {
                text = '';
            } else if (text == "@currentUser") {
                text = TFUtils.getCurrentUserFullName();
                tdValues.push(TFUtils.getCurrentUserName());
            }
            tdValues.push(text.toLocaleLowerCase());
        });

        if (tdValues.length == 0) {
            return;
        }

        t.filtered = true;
        var cellName = filter.attr('name');
        var inverse = filter.hasClass('inverse');
        var isSelected;
        if (t.ddSeparator) {
            isSelected = function(cellValue, selectedOptions) {
                if (cellValue == '') {
                    return $.inArray(cellValue, selectedOptions) == -1;
                }

                var selected = true;
                cellValue.split(t.ddSeparator).some(function(value) {
                    value = value.trim();
                    if (value) selected = $.inArray(value, selectedOptions) == -1;
                    return !selected;
                });
                return selected;
            };
        } else {
            isSelected = function(cellValue, selectedOptions) {
                return $.inArray(cellValue, selectedOptions) == -1;
            };
        }

        t.rawTables.forEach(function (rows, i) {
            var tdNumber = TableUtils.getCellPosition(rows, cellName);

            if (tdNumber != null) {
                rows.forEach(function(row, j) {
                    if (j > 0 && (t.isAND ? t.cbState[i][j - 1] : !t.cbState[i][j - 1])) {
                        var tdCurrent = TableUtils.clearStringLowerCase(row[tdNumber]);
                        var inArray = isSelected(tdCurrent, tdValues);
                        inArray = t.isAND ? inArray : !inArray;
                        if (inverse != inArray) {
                            t.cbState[i][j - 1] = !t.isAND;
                        }
                    }
                });
            }
        });
    };

    TableFilter.prototype.treatIconFilter = function(filter) {
        var t = this;
        var tdValues = [];
        var tdChecked = [];
        var tdEmpty = false;
        filter.find('option:selected').each(function() {
            var text = $.trim($(this).text());
            if (text == "-Empty-") {
                tdEmpty = true;
            } else {
                var el = $(text);
                if (el.length) {
                    if (el.is('ul')) {
                        tdChecked.push('ul.inline-task-list li' + (el.find('li').hasClass('checked') ? '.checked' : ':not(.checked)'));
                    } else {
                        text = el[0].src;
                        if (text) {
                            text = TableUtils.clearStringLowerCase(text);
                            var index = text.indexOf('images/icons/emoticons/');
                            if (index >= 0) {
                                text = text.substr(index, text.length);
                            }
                        }
                        tdValues.push(text);
                    }
                }
            }
        });

        if (tdValues.length == 0 && tdChecked.length == 0 && !tdEmpty) {
            return;
        }

        t.filtered = true;
        tdChecked = tdChecked.join(',');

        var cellName = filter.attr('name');
        var inverse = filter.hasClass('inverse');

        t.tables.each(function (i) {
            var rows = t.rows[i];
            var tdNumber = TableUtils.getColumnPosition(rows, cellName);

            if (tdNumber != null) {
                rows.each(function(j) {
                    if (j > 0 && (t.isAND ? t.cbState[i][j - 1] : !t.cbState[i][j - 1])) {
                        var td = TableUtils.getNthTdElem(this, tdNumber);

                        var isNotIcon;
                        if (tdValues.length) {
                            TableUtils.getTdIconsSrc(td).some(function(src) {
                                if (src) {
                                    src = TableUtils.clearStringLowerCase(src);
                                    var index = src.indexOf('images/icons/emoticons/');
                                    if (index >= 0) {
                                        src = src.substr(index, src.length);
                                    }
                                }
                                isNotIcon = $.inArray(src, tdValues) == -1;
                                return !isNotIcon;
                            });
                        }

                        var isNotCheckBox;
                        if (tdChecked) {
                            isNotCheckBox = td.find(tdChecked).length == 0;
                        }

                        var isEmpty;
                        if (tdEmpty) {
                            isEmpty = td.find('ul.inline-task-list li, img[src]').length > 0;
                        }

                        var matches = (isNotIcon !== false && isEmpty !== false && isNotCheckBox === true) ||
                            (isNotIcon === true && isEmpty !== false  && isNotCheckBox !== false) ||
                            (isNotIcon !== false && isEmpty === true  && isNotCheckBox !== false);
                        matches = t.isAND ? matches : !matches;

                        if (inverse != matches) {
                            t.cbState[i][j - 1] = !t.isAND;
                        }
                    }
                });
            }
        });
    };

    TableFilter.prototype.treatInputFilter = function(context, userFilter) {
        var t = this;
        var equation = context.val();
        var tdValue = $.trim(equation).toLocaleLowerCase();
        if (equation == "@currentUser") {
            equation = TFUtils.getCurrentUserFullName() + "|" + TFUtils.getCurrentUserName();
        }
        if (tdValue.indexOf('&') > 0) {
            var tmp = '';
            $.each(equation.split('&'), function(i, item) {
                tmp += '(?=.*' + item.trim() + ')'
            });
            equation = tmp;
        }
        try {
            var regExp = new RegExp(equation);
        } catch (e) {
        }

        var cellName;
        if(userFilter) {
            cellName = context.attr('name');
        }
        //IE8-9 fix
        if (tdValue == '' || context.val() == "Start typing...\u200b") {
            return;
        }

        t.filtered = true;
        var inverse = context.hasClass('inverse');

        t.rawTables.forEach(function(rows, i) {
            var tdNumber = TableUtils.getCellPosition(rows, cellName);

            if (!userFilter || tdNumber != null) {
                rows.forEach(function(row, j) {
                    if (j > 0 && (t.isAND ? t.cbState[i][j - 1] : !t.cbState[i][j - 1])) {
                        var rowText = (userFilter ? row[tdNumber] : row.join(' ')) || '';
                        var matches = TableUtils.clearStringLowerCase(rowText).indexOf(tdValue) == -1 &&
                            (!regExp || rowText.match(regExp) == null);
                        matches = t.isAND ? matches : !matches;

                        if (inverse != matches) {
                            t.cbState[i][j - 1] = !t.isAND;
                        }
                    }
                });
            }
        });
    };

    TableFilter.prototype.treatGlobalFilter = function(filter) {
        this.treatInputFilter(filter, false);
    };

    TableFilter.prototype.treatUserFilter = function(filter) {
        this.treatInputFilter(filter, true);
    };

    TableFilter.prototype.treatDateFilter = function(input) {
        var t = this;
        var tdValue = $.trim(input.val()).toLocaleLowerCase();
        var cellName = input.attr('name');
        var dateFormat = input.attr('dateformat');
        var isFrom = input.hasClass('from');
        var inverse = input.hasClass('inverse');

        if (isFrom) {
            t.rangeFilterValues = {
                from: getDate()
            }
        } else {
            t.rangeFilterValues.to = getDate();

            if (t.rangeFilterValues.from || t.rangeFilterValues.to) {
                t.filtered = true;
                t.rawTables.forEach(function(rows, i) {
                    var tdNumber = TableUtils.getCellPosition(rows, cellName);

                    if (tdNumber != null) {
                        rows.forEach(function(row, j) {
                            if (j > 0 && (t.isAND ? t.cbState[i][j - 1] : !t.cbState[i][j - 1])) {
                                var text = row[tdNumber] || '';
                                try {
                                    var cellDate = TableUtils.parseDate(text, dateFormat, t.locale);
                                    if (cellDate) {
                                        if (!inverse) {
                                            if (t.rangeFilterValues.from) {
                                                if (t.rangeFilterValues.to) {
                                                    hider(t.rangeFilterValues.from > cellDate || t.rangeFilterValues.to < cellDate, i, j);
                                                } else {
                                                    hider(t.rangeFilterValues.from > cellDate, i, j);
                                                }
                                            } else if (t.rangeFilterValues.to) {
                                                hider(t.rangeFilterValues.to < cellDate, i, j);
                                            }
                                        } else {
                                            if (t.rangeFilterValues.from) {
                                                if (t.rangeFilterValues.to) {
                                                    hider(t.rangeFilterValues.from <= cellDate && t.rangeFilterValues.to >= cellDate, i, j);
                                                } else {
                                                    hider(t.rangeFilterValues.from <= cellDate, i, j);
                                                }
                                            } else if (t.rangeFilterValues.to) {
                                                hider(t.rangeFilterValues.to >= cellDate, i, j);
                                            }
                                        }
                                    } else {
                                        hider(!inverse, i, j);
                                    }
                                } catch(e) {AJS.log("Error while filtering date '" + text + "': " + e)}
                            }
                        });
                    }
                });
            }
        }

        function hider(condition, i, j) {
            condition = t.isAND ? condition : !condition;
            if (condition) {
                t.cbState[i][j - 1] = !t.isAND;
            }
        }

        function getDate() {
            try {
                var date;
                if (tdValue == "today" || tdValue == "heute") {
                    date = new Date();
                    return date.setHours(0, 0, 0, 0);
                } else {
                    var places = ['y', 'm', 'w', 'd'];
                    var dynamic = false;
                    places.some(function(place) {
                        if ((new RegExp('^[+-]?\\d+' + place)).test(tdValue)) {
                            dynamic = true;
                            return true;
                        }
                    });
                    if (dynamic) {
                        date = new Date();
                        date.setHours(0, 0, 0, 0);

                        places.forEach(function(place, i) {
                            var parts = tdValue.split(place);
                            if (parts.length == 2) {
                                var value = parseInt(parts[0]);
                                if (i == 3) {
                                    date.setDate(date.getDate() + value);
                                } else if (i == 2) {
                                    date.setDate(date.getDate() + value * 7);
                                } else if (i == 1) {
                                    date.setMonth(date.getMonth() + value);
                                } else {
                                    date.setYear(date.getFullYear() + value);
                                }
                                tdValue = parts[1];
                            }
                        });

                        return date;
                    }

                    return date = TableUtils.parseDate(tdValue, dateFormat, t.locale);
                }
            } catch(e) {
                AJS.log("Error while filtering by date '" + tdValue + "': " + e);
                return false;
            }
        }
    };

    TableFilter.prototype.treatNumberFilter = function(input) {
        var t = this;
        var tdValue = $.trim(input.val()).toLocaleLowerCase();
        var cellName = input.attr('name');
        var inverse = input.hasClass('inverse');
        var isFrom = input.hasClass('from');
        var valueType = TableUtils.getValueType(tdValue);

        if (isFrom) {
            t.rangeFilterValues = {
                from: isNaN(TableUtils.parseValueByType(valueType, tdValue, t.separator)) ? false : TableUtils.parseValueByType(valueType, tdValue, t.separator),
                type: valueType
            }
        } else {
            if (t.rangeFilterValues.type) valueType = t.rangeFilterValues.type;
            t.rangeFilterValues.to = isNaN(TableUtils.parseValueByType(valueType, tdValue, t.separator)) ? false : TableUtils.parseValueByType(valueType, tdValue, t.separator);

            if (t.rangeFilterValues.from !== false || t.rangeFilterValues.to !== false) {
                t.filtered = true;
                t.rawTables.forEach(function(rows, i) {
                    var tdNumber = TableUtils.getCellPosition(rows, cellName);

                    if (tdNumber != null) {
                        rows.forEach(function(row, j) {
                            if (j > 0 && (t.isAND ? t.cbState[i][j - 1] : !t.cbState[i][j - 1])) {
                                var text = row[tdNumber] || '';
                                try {
                                    var cellNumber = TableUtils.parseValueByType(valueType, text, t.separator);
                                    if (!isNaN(cellNumber)) {
                                        if (!inverse) {
                                            if (t.rangeFilterValues.from !== false) {
                                                if (t.rangeFilterValues.to !== false) {
                                                    hider(t.rangeFilterValues.from > cellNumber || t.rangeFilterValues.to < cellNumber, i, j);
                                                } else {
                                                    hider(t.rangeFilterValues.from > cellNumber, i, j);
                                                }
                                            } else if (t.rangeFilterValues.to !== false) {
                                                hider(t.rangeFilterValues.to < cellNumber, i, j);
                                            }
                                        } else {
                                            if (t.rangeFilterValues.from !== false) {
                                                if (t.rangeFilterValues.to !== false) {
                                                    hider(t.rangeFilterValues.from <= cellNumber && t.rangeFilterValues.to >= cellNumber, i, j);
                                                } else {
                                                    hider(t.rangeFilterValues.from <= cellNumber, i, j);
                                                }
                                            } else if (t.rangeFilterValues.to !== false) {
                                                hider(t.rangeFilterValues.to >= cellNumber, i, j);
                                            }
                                        }
                                    } else {
                                        hider(!inverse, i, j);
                                    }
                                } catch(e) {AJS.log("Error while filtering number '" + text + "': " + e)}
                            }
                        });
                    }
                });
            }
        }

        function hider(condition, i, j) {
            condition = t.isAND ? condition : !condition;
            if (condition) {
                t.cbState[i][j - 1] = !t.isAND;
            }
        }
    };

    TableFilter.prototype.treatColumnFilter = function(filter) {
        var t = this;
        t.updateSparkline = true;
        var tdValues = [];
        filter.find('option:selected').each(function() {
            tdValues.push($.trim($(this).text()).toLowerCase());
        });

        var inverse = filter.hasClass('inverse');

        if (tdValues.length == 0) {
            return;
        }

        t.rawTables.forEach(function(rows, i) {
            var tds = rows[0];
            tds.forEach(function(td, n) {
                if (inverse != $.inArray(TableUtils.clearStringLowerCase(td), tdValues) >= 0) {
                    t.tdNumbers[i].push(n);
                    if (n == tds.length - 1) {
                        t.updateSparkline = false;
                    }
                }
            });
        });
    };

    TableFilter.prototype.showOrHide = function (i, j, tr, rowNumber) {
        var t = this;
        var res = 0;
        if (j > t.headerSize[i]) {
            if (!t.cbState[i][j - 1]) {
                var $tr = $(tr);
                $tr.find('td[rowspan], th[rowspan]').trigger('hide');
                $tr[0].style.display = 'none';
            } else if (t.numbering && (t.numbering.indexOf("dynamic") != -1 || t.numbering == "true")) {
                $(tr).find('.tf-numbering').text(rowNumber);
                res = 1;
            }
        }

        if (t.tdNumbers[i].length) {
            var tds = $(tr).children('td,th');
            $.each(t.tdNumbers[i], function(n, number) {
                tds.eq(number).trigger('hideCol').addClass('tf-hidden-column');
            });
        }

        return res;
    };

    TableFilter.prototype.showTableWithoutFilters = function() {
        var t = this;
        var showNRows = t.showNRows;
        t.showNRows = "all";
        t.showTable(true);
        t.showOrHideTables();
        t.showNRows = showNRows;
    };

    TableFilter.prototype.showOrHideTables = function() {
        var t = this;
        var totalRows = 0;
        var visibleRows = 0;

        t.tables.find('.tf-hidden-column').removeClass('tf-hidden-column');
        t.tables.find('td[colspan], th[colspan]').each(function() {
            var td = $(this);
            td.attr('colspan', $.data(td[0], 'colspan'));
        });

        t.tables.each(function (i) {
            var table = $(this);
            var fhtThead = table.parents('.fht-table-wrapper').find('.fht-thead');
            var fhtTHs = fhtThead.find('th, td').removeClass('tf-hidden-column');
            var fhtCols = fhtThead.find('col').show();
            var cols = table.find('col').show();
            var pivotHeaders = table.find('tr.pivot-header');
            if (t.tdNumbers[i].length) {
                $.each(t.tdNumbers[i], function(n, number) {
                    cols.eq(number).hide();
                    fhtCols.eq(number).hide();
                    fhtTHs.eq(number).addClass('tf-hidden-column');
                    pivotHeaders.each(function() {
                        $(this).find('th, td').eq(number).trigger('hideCol').addClass('tf-hidden-column');
                    });
                });
            }

            var rows = t.rows[i];

            if (table.is('.attachments')) {
                table.find('.attachment-summary').each(function(j) {
                    if (!t.cbState[i][j]) {
                        $(this).addClass('hidden');
                        rows.eq(j + 1).find('.icon-section-opened').removeClass('icon-section-opened').addClass('icon-section-closed');
                    }
                });
            }

            var fakes = rows.find('.fake').hide();
            rows.find('td[rowspan], th[rowspan]').each(function() {
                var td = $(this);
                td.attr('rowspan', $.data(td[0], 'rowspan'));
            });
            rows.css('display', '');
            totalRows += rows.length - 1;
            var thisVisibleRows = $.grep(t.cbState[i], function(a) {return a}).length;
            var rowNumber = 1;
            var incr = 1;
            if (t.numbering && t.numbering.indexOf("desc") != -1) {
                rowNumber = thisVisibleRows;
                incr = -1;
            }
            rows.each(function(j) {
                rowNumber += t.showOrHide(i, j, this, rowNumber) * incr;
            });

            if (/^\d+\+?$/i.test(t.showNRows) &&
                ((t.showNRows != 0 && t.showNRows.indexOf('+') != t.showNRows.length - 1) || thisVisibleRows == rows.length - 1)) {
                rows.filter(function() {return TableUtils.isRowVisible($(this))}).filter(':gt(' + t.showNRows.replace('+', '') + ')').hide();
                thisVisibleRows = Math.min(thisVisibleRows, t.showNRows);
            }

            if (t.editorMacro) {        //для выделятора ячеек в редакторе
                fakes.filter('[rowspan]:not(:visible)').attr('rowspan', -1);
                fakes.filter('[colspan]:not(:visible)').attr('colspan', -1);
            }

            visibleRows += thisVisibleRows;
        });

        var filterIcon = t.comboBoxDiv.find('.filter-logo');
        if (totalRows == visibleRows) {
            filterIcon.removeClass('filtered').addClass('default')
                .attr('title', AJS.format("Nothing filtered. Showing {0} rows", totalRows));
        } else {
            filterIcon.removeClass('default').addClass('filtered')
                .attr('title', AJS.format("Showing {0} of {1} rows", visibleRows, totalRows));
        }

        if (t.resize) {
            t.resize();
        }

        var data = {id: "tf-table-" + t.macroId, tables: t.tables, rows: t.rows, state: t.cbState, tdNumbers: t.tdNumbers, headerSize: t.headerSize};

        t.tableWrapper.trigger('tf-filtered', data);
        t.tables.trigger('tf-filtered-charts');
        t.comboBoxDiv.parents('.tablefilter-table-wrapper, .pivot-content, table').eq(0).data('inner-filter', data);
    };

    TableFilter.prototype.appendSparklines = function() {
        var t = this;

        if (t.sparkline) {
            if (window.d3) {
                t.rows.forEach(function(rows) {
                    drawSparklines(rows);
                });
            } else {
                TFUtils.loadD3Scripts(function() {
                    t.rows.forEach(function(rows) {
                        drawSparklines(rows);
                    });
                });
            }
        } else {
            t.tableWrapper.find('.tf-sparkline-header, .tf-sparkline-container').remove();
        }

        function drawSparklines($rows) {

            $rows.each(function(i, tr) {
                var $tr = $(tr);
                $tr.find('.tf-sparkline-header, .tf-sparkline-container').remove();

                var clazz = $tr.find(":first-child").attr('class').replace('tf-numbering', '').replace('tf-hidden-column', '');
                if (i == 0) {
                    $tr.append('<th class="' + (typeof clazz != "undefined" ? clazz : "") + ' tf-sparkline-header" contenteditable="false" data-sorter="false"><div>' + t.sparkName + '</div></th>');
                } else {
                    var data = [];
                    $tr.children('td, th').each(function() {
                        if ($(this).hasClass('tf-numbering') || $(this).hasClass('numberingColumn') || $(this).hasClass('tf-hidden-column')) {
                            return true;
                        }
                        var text = $(this).text().replace(/\s/g, '');
                        var value = t.separator == '.' ? parseFloat(text.replace(/,/g, '')) : parseFloat(text.replace(/\./g, '').replace(/,/g, '.'));
                        if (!isNaN(value)) {
                            data.push(value);
                        }
                    });
                    $tr.append('<td id="sparkline-container-' + i + '" class="' + (typeof clazz != "undefined" ? clazz : "") + ' tf-sparkline-container" contenteditable="false"></td>');
                    drawSparkline($(this), i, data);

                    if (!t.editorMacro) {
                        $tr.find('#sparkline-container-' + i).append('<canvas></canvas>');

                        var $svg = $tr.find("svg");
                        var html = $svg.attr("version", 1.1).attr("xmlns", "http://www.w3.org/2000/svg").prop('outerHTML');

                        if (typeof btoa != "undefined" && html) {
                            var imgsrc = 'data:image/svg+xml;base64,'+ btoa(unescape(encodeURIComponent(html)));
                            var canvas = $tr.find("canvas").attr('width', 100).attr('height', 25)[0];
                            var context = canvas.getContext("2d");

                            var image = new Image;
                            image.src = imgsrc;
                            image.onload = function() {
                                context.drawImage(image, 0, 0);
                                var canvasdata = canvas.toDataURL("image/png");
                                var pngimg = '<img src="'+canvasdata+'">';
                                $tr.find('#sparkline-container-' + i).html(pngimg);
                            };
                        }
                    }
                }
            });

            function drawSparkline($tr, id, data) {
                var width = 100;
                var height = 25;
                var graph = d3.select($tr.find('#sparkline-container-' + id).get(0)).append("svg:svg").attr("width", width).attr("height", height);

                var x = d3.scale.linear().domain([0, data.length-1]).range([0, width]);
                var y = d3.scale.linear().domain([d3.min(data, function(d) {return d;}), d3.max(data, function(d) {return d;})]).range([height, 0]);

                var line = d3.svg.line()
                    .x(function(d,i) {
                        return x(i);
                    })
                    .y(function(d) {
                        return y(d);
                    });

                graph.append("svg:path")
                    .attr("d", line(data))
                    .attr("style", "stroke: rgb(100, 100, 255); stroke-width: 1; fill: none;");
            }
        }
    };

    TableFilter.prototype.bindChangeLabel = function() {
        var t = this;

        t.comboBoxDiv.on("dblclick", ".filter-container label.hideble", function() {
            var label = $(this).find('.tf-label-name');
            var visible = true;
            var parent = $(this).parent();
            var labelWidth = label.css('width');

            label.hide();
            parent.prepend(TableFilterTemplate.labelChanger({
                value: label.text(),
                width: (parseInt(labelWidth, 10) + 6) + "px"
            }));

            var space = false;
            var changer = parent.find('.tf-label-changer');
            changer.focus();
            changer.on('keydown', function(e) {
                if (e.keyCode == 13) {
                    label.text(changer.val());
                    changer.remove();
                    label.show();
                    visible = false;
                    t.enableSaveBtn()
                }
                if (e.keyCode == 32) {
                    space = true;
                }
            });
            changer.on('input', function() {
                var width = changer.textWidth(changer.html()) + 6;
                if (space) {
                    width += 4;
                    space = false;
                }
                changer.css('width', (width < 10 ? 10 : width) + 'px');
            });

            $(document).on('click', function(event) {
                if (!$(event.target).closest('.tf-label-changer').length && visible) {
                    changer.remove();
                    label.show();
                    visible = false;
                }
            });
        });

        //todo куда-нибудь красиво запихнуть эту штуку.
        $.fn.textWidth = function(text, font) {
            if (!$.fn.textWidth.fakeEl) $.fn.textWidth.fakeEl = $('<span>').hide().appendTo(document.body);
            $.fn.textWidth.fakeEl.text(text || this.val() || this.text()).css('font', font || this.css('font'));
            return $.fn.textWidth.fakeEl.width();
        };
    };

    TableFilter.prototype.getFilterLabels = function() {
        var t = this;
        var labels = [];

        addLabels(".selectfilter-element");
        addLabels(".userfilter-element");
        addLabels(".numberfilter-element");
        addLabels(".datefilter-element");
        addLabels(".globalfilter-element");
        addLabels(".columnhider-element");
        addLabels(".iconfilter-element");

        return labels.join('‚');

        function addLabels(selector) {
            t.comboBoxDiv.find(selector + " .tf-label-name").each(function() {
                labels.push($(this).text());
            });
        }
    }

    function getFilterTypeByClassName(item) {
        if (item.hasClass('selectfilter-element')) return 'Dropdown Filter';
        if (item.hasClass('userfilter-element')) return 'Free Text Filter';
        if (item.hasClass('numberfilter-element')) return 'Number Filter';
        if (item.hasClass('datefilter-element')) return 'Date Filter';
        if (item.hasClass('iconfilter-element')) return 'Icon Filter';
        if (item.hasClass('globalfilter-element')) return 'Global Filter';
        if (item.hasClass('columnhider-element')) return 'Hide Columns';
    }
})(jQuery);

}catch(e){WRMCB(e)};
;
try {
/* module-key = 'com.stiltsoft.confluence.plugin.tablefilter.tablefilter:web-resources', location = 'js/tablefilter-export.js' */
(function($) {

    AJS.toInit(function() {

        $('[name="inlinecommentform"]').one('click', function() {
            $('.tf-form, .pivot-content form, .chart-controls form').attr('name', '');
        });

        var getTablesHtml = function(tables) {
            tables = tables.clone();

            tables.each(function() {
                var table = $(this);
                var header = table.find('.tableFloatingHeader');
                if (header.css('display') == 'none') {
                    header.remove();
                } else {
                    table.find('.tableFloatingHeaderOriginal').remove();
                }

            });

            tables.find('a[href]').each(function () {
                $(this).attr('href', this.href);
            });

            tables.find('.tf-header-hidden').remove();
            tables.find('.resize-sensor').remove();
            tables.find('.refresh-action-group').remove();
            tables.find('tr').filter(function() {
                return $(this).css('display') == 'none';
            }).remove();
            tables.find('td,th').filter(function() {
                return $(this).css('display') == 'none' || $(this).hasClass('tf-hidden-column');
            }).remove();
            tables.find('.fht-cell').remove();
            var markups = [];
            tables.each(function() {
                var t = $(this);
                if (t.hasClass('fht-table')) {
                    t.css('margin-top', '');
                }
                markups.push($('<div/>').append(this).html());
            });
            return markups;
        };

        $('#action-export-pdf-link, #action-export-word-link, #com-k15t-confluence-scroll-pdf-launcher, #com-k15t-confluence-scroll-office-launcher')
            .unbind('click.tf').bind('click.tf', function() {
            var ids = [];
            var pageIds = [];
            var htmls = [];
            $('.tablefilter-outer-wrapper, .pivot-content, .table-chart-contents').filter(function() {
                return !$(this).parents('.original-table.pivot-hidden').length && !$(this).parents('.table-chart-contents').length;
            }).each(function() {
                var tf = $(this);

                if (tf.hasClass('table-chart-contents')) {
                    ids.push(tf.data('id'));
                    pageIds.push(tf.data('pageid'));
                    var imageHtml = tf.find("#pngdataurl").html();
                    var tablesHtml = [];
                    if (!tf.data('hidden')) {
                        tablesHtml = getTablesHtml(tf.find('table')
                            .not('.tablefilter-outer-wrapper table table')
                            .not('.pivot-content table table')
                            .not('.table-chart-contents table table')
                            .not('.fht-thead table')
                            .not('.original-table.pivot-hidden table'));
                    }

                    htmls.push(imageHtml + tablesHtml.join(""));
                } else {
                    var tables = tf.find('table')
                        .not('.tablefilter-outer-wrapper table table')
                        .not('.pivot-content table table')
                        .not('.table-chart-contents table table')
                        .not('.fht-thead table')
                        .not('.original-table.pivot-hidden table');
                    if (tf.find('.filter-container').hasClass('filtered') || tables.find('.tf-numbering').length
                        || tables.find('.tf-hidden-column').length || tf.hasClass('pivot-content')
                        || tf.find('th.tablesorter-headerAsc, th.tablesorter-headerDesc').length || tf.find('th.tf-sparkline-header').length) {

                        pageIds.push(tf.data('pageid'));
                        ids.push(tf.data('id'));
                        var html = getTablesHtml(tables).join("<table-filter-splitter>");
                        htmls.push(html);
                    }
                }
            });

            if (ids.length) {
                $.ajax({
                    contentType: "application/x-www-form-urlencoded; charset=utf-8",
                    type: 'POST',
                    async: false,
                    cache: false,
                    url: Confluence.getContextPath() + '/rest/table-filter/1.0/service/export',
                    dataType: 'json',
                    data: {
                        pageIds: pageIds,
                        ids: ids,
                        htmls: htmls
                    }
                });
            }
        });
    });

})(AJS.$);


}catch(e){WRMCB(e)};
