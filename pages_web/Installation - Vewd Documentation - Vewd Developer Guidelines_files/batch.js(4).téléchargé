                monthNames: [ "一月","二月","三月","四月","五月","六月","七月","八月","九月","十月","十一月","十二月" ],
                monthNamesShort: [ "一月","二月","三月","四月","五月","六月","七月","八月","九月","十月","十一月","十二月" ],
                dayNames: [ "星期日","星期一","星期二","星期三","星期四","星期五","星期六" ],
                dayNamesShort: [ "周日","周一","周二","周三","周四","周五","周六" ],
                dayNamesMin: [ "日","一","二","三","四","五","六" ],
                firstDay: 1,
                isRTL: false,
                showMonthAfterYear: true,
                yearSuffix: "年"
            }
        }
    };

    $.extend({
        getUrlHashVars: function() {
            var vars = [], hash;
            var hashes = window.location.href.slice(window.location.href.indexOf('#') + 1).split('&');
            for (var i = 0; i < hashes.length; i++)
            { hash = hashes[i].split('='); vars.push(hash[0]); vars[hash[0]] = decodeURIComponent(hash[1]); }
            return vars;
        },

        getUrlHashVar: function(name) {
            return $.getUrlHashVars()[name];
        }
    });

    $.fn.changeElementType = function(newType) {
        var attrs = {};

        $.each(this[0].attributes, function(idx, attr) {
            attrs[attr.nodeName] = attr.nodeValue;
        });

        this.replaceWith(function() {
            return $("<" + newType + "/>", attrs).append($(this).contents());
        });
    };

})(jQuery);



}catch(e){WRMCB(e)};
;
try {
/* module-key = 'com.stiltsoft.confluence.plugin.tablefilter.tablefilter:macro-browser-resources', location = 'tf-libs/js/pivot-macro.js' */
(function($) {

    window.PivotTableMacro = function() {
        this.tables = [];
        this.tableColumns = [];
        this.columnSelect = null;
        this.rowSelect = null;
        this.aggregateSelect = null;
        this.tfBody = "";
        this.typeParams = [];
        this.columnParams = [];
        this.rowParams = [];
        this.aggregateParams = [];
        this.sortColumns = [];
        this.gotBody = false;
    };

    PivotTableMacro.prototype.getMacroDetails = function(macroBody) {
        var self = this;

        self.gotBody = true;
        self.tfBody = macroBody;
        var body = $('<div><div class="original-table">' + macroBody + '</div></div>');

        self.tables = TableUtils.getTables(body, '.original-table', false, ':not(.tf-numbering)');

        self.tableColumns = TableUtils.getColumns(self.tables);
    };

    PivotTableMacro.prototype.transformElements = function(params) {
        var self = this;

        $('#macro-browser-dialog .macro-desc .macro-doco-link').css('float', 'right')
            .before("Post \u003ca href=\u0022https://stiltsoft.customercase.com/forums/table-filter-for-confluence/ideas/create?from=macro\u0022\u003eIdea\u003c/a\u003e or \u003ca href=\u0022https://stiltsoft.customercase.com/forums/support-requests/ideas/create?from=macro&category=Table%2BFilter%2Band%2BCharts%2Bfor%2BConfluence\u0022\u003eIssue\u003c/a\u003e.");

        $('#macro-param-div-id').hide();

        if (params["column"]) {
            self.columnParams = params["column"].split('‚');
            $.each(self.columnParams, function (i, name){
                self.columnParams[i] = $.trim(name);
            });
        } else {
            self.columnParams = [];
        }

        if (params["row"]) {
            self.rowParams = params["row"].split('‚');
            $.each(self.rowParams, function (i, name){
                self.rowParams[i] = $.trim(name);
            });
        } else {
            self.rowParams = [];
        }

        if (params["aggregation"]) {
            self.aggregateParams = params["aggregation"].split('‚');
            $.each(self.aggregateParams, function (i, name){
                self.aggregateParams[i] = $.trim(name);
            });
        } else {
            self.aggregateParams = [];
        }

        self.typeParams = ["Count"];
        if (params["type"]) {
            self.typeParams = params["type"].split('‚');
        }

        if (!params["version"]) {
            if (self.columnParams.length == 2) {
                self.rowParams = [self.columnParams[1]];
                self.columnParams.length = 1;
            }
        }

        if (!params["version"] || params["version"] == 1) {
            params["version"] = 2;
            var type = self.typeParams[self.typeParams.length - 1];
            if (type == "Count" || self.aggregateParams.length == 0) {
                params['sort'] = type + " (\u200DDesc)";
            } else {
                params['sort'] = type + " of " + self.aggregateParams[self.aggregateParams.length - 1] + " (\u200DDesc)";
            }
        }

        setSavedParams(params);

        var cols = $.merge([], self.columnParams);
        $.merge(cols, self.rowParams);
        $.merge(cols, self.aggregateParams);
        $.each(cols, function(i, name) {
            if ($.inArray(name, self.tableColumns) == -1) {
                self.tableColumns.push(name);
            }
        });

        self.columnSelect = createMultipleSelect($('#macro-param-column'), self.tableColumns, self.columnParams, true, self);
        self.rowSelect = createMultipleSelect($('#macro-param-row'), self.tableColumns, self.rowParams, true, self);
        self.aggregateSelect = createMultipleSelect($('#macro-param-aggregation'), self.tableColumns, self.aggregateParams, true, self);
        self.sortSelect = $('#macro-param-sort');
        self.sortSelect.chosen();

        $('.search-field input').one('focus', function() {
            $('.search-choice').css('max-width', $('.chosen-container').width() - 40);
        });

        var typeSelect = $('#macro-param-type');
        typeSelect.attr('multiple', 'multiple');
        typeSelect.find('option').each(function() {
            var opt = $(this);
            if ($.inArray(opt.text(), self.typeParams) == -1) {
                opt.prop('selected', false);
            } else {
                opt.prop('selected', true);
            }
        });

        initChosen(typeSelect, $('<input>'), self.typeParams, false, self);
        appendTabs();
        if (self.columnParams.length <= 1) {
            self.updateSortOptions(params['sort']);
        } else {
            self.sortSelect.parent().addClass('chart-browser-hidden');
        }
    };

    PivotTableMacro.prototype.updateSortOptions = function(storedValue) {
        var self = this;
        var selected = storedValue || self.sortSelect.val();
        self.sortColumns = [];
        self.sortSelect.empty().append('<option selected="selected" value="false"></option>');

        appendValue(self.columnParams[0]);
        $.each(self.tables, function (i, rows) {
            $.each(self.typeParams, function (j, type) {
                if (self.rowParams.length > 0) {
                    if (type == 'Count') {
                        makeStr(type, 0, rows);
                    } else {
                        $.each(self.aggregateParams, function (k, aggr) {
                            makeStr(type + " of " + aggr, 0, rows);
                        });
                    }
                } else {
                    if (type == 'Count') {
                        appendValue(type);
                    } else {
                        $.each(self.aggregateParams, function (k, aggr) {
                            appendValue(type + " of " + aggr);
                        });
                    }
                }
            });
        });

        $.each(self.sortColumns.sort(), function(i, value) {
            self.sortSelect.append($('<option></option>').val(value).text(value));
        });
        self.sortSelect.val(selected);
        self.sortSelect.trigger('chosen:updated');

        function makeStr(str, position, rows) {
            var index = TableUtils.getCellPosition(rows, self.rowParams[position]);
            if (index != null) {
                $.each(rows, function(i, row) {
                    if (i != 0) {
                        if (position + 1 < self.rowParams.length) {
                            makeStr(str + " " + row[index], position + 1, getFilteredTable(rows, index, row[index]));
                            makeStr(str, position + 1, getFilteredTable(rows, index, row[index]));
                        } else {
                            appendValue(str + " " + row[index]);
                        }
                    }
                });
            } else if (position + 1 < self.rowParams.length) {
                makeStr(str, position + 1, rows);
            }
            appendValue(str);
        }

        function getFilteredTable(table, index, value) {
            var filtered = [];

            $.each(table, function(i, row) {
                if (i == 0 || row[index] == value) {
                    filtered.push(row);
                }
            });

            return filtered;
        }

        function appendValue(value) {
            if ($.trim(value).length) {
                if ($.inArray(value + ' (\u200DAsc)', self.sortColumns) == -1) {
                    self.sortColumns.push(value + ' (\u200DAsc)');
                }
                if ($.inArray(value + ' (\u200DDesc)', self.sortColumns) == -1) {
                    self.sortColumns.push(value + ' (\u200DDesc)');
                }
            }
        }
    };

    function createMultipleSelect(input, tableColumns, params, userOption, pivotMacro) {
        input.hide();

        var select = $('<select multiple/>');

        $.each(params, function(i, name) {
            name = $.trim(name);
            if ($.inArray(name, tableColumns) == -1) {
                tableColumns.push(name);
            }
        });

        tableColumns.sort();

        $.each(tableColumns, function(i, name) {
            var selected = $.inArray(name, params) == -1 ? '' : 'selected="selected" ';
            select.append($('<option ' + selected + '></option>').text(name));
        });

        input.after(select);
        initChosen(select, input, params, userOption, pivotMacro);

        return select;
    }

    function initChosen(select, input, params, userOption, pivotMacro) {
        select.chosen({
            no_results_text: userOption ? "No matches found. Press \u0027Enter\u0027 to add" : "No matches for",
            placeholder_text_multiple: "Click or start typing...",
            width: "100%",
            display_selected_options: false,
            addUserOption: userOption
        }).change(function(e, data) {
            if (data.selected) {
                select.find('option').filter(function() {
                    return $(this).text() == data.selected;
                }).prop('selected', true);
                params.push(data.selected);
            } else if (data.deselected) {
                select.find('option').filter(function() {
                    return $(this).text() == data.deselected;
                }).prop('selected', false);
                params.splice($.inArray(data.deselected, params), 1);
            }
            if (input.attr('id') == 'macro-param-column') {
                var sort = pivotMacro.sortSelect.parent();
                if ((select.val() != null && select.val().length > 1) && !sort.hasClass('chart-browser-hidden')) {
                    sort.addClass('chart-browser-hidden');
                } else if (select.val() == null || select.val().length <= 1) {
                    sort.removeClass('chart-browser-hidden');
                }
            }

            pivotMacro.updateSortOptions();
            input.val(params.join('‚'));
        });

        var choices = TableUtils.sortSelectedOptions(select, params);

        choices.sortable({
            tolerance: 'pointer',
            cursor: 'move',
            helper: 'clone',
            items: ".search-choice",
            stop: function() {
                params.length = 0;
                choices.find('.search-choice').each(function() {
                    params.push($(this).text())
                });
                input.val(params.join('‚'));
            }
        });
    }

    function setSavedParams(params) {
        if (params["decimalSeparator"] || params["decimalseparator"]) {
            $('#macro-param-decimalseparator').val(params["decimalSeparator"] || params["decimalseparator"]);
        }

        if (params["decimals"]) {
            $('#macro-param-decimals').val(params["decimals"]);
        }

        if (params["separator"]) {
            $('#macro-param-separator').val(params["separator"]);
        }

        if (params["showrawdata"] == "true") {
            $('#macro-param-showrawdata').prop("checked", true);
        }

        if (params["hidePane"] == "true") {
            $('#macro-param-hidePane').prop("checked", true);
        }

        if (params['ddSeparator']) {
            $('#macro-param-ddSeparator').val(params['ddSeparator']);
        }
    }

    function appendTabs() {
        $('#macro-param-div-column').before(PivotTemplate.macrobrowserTabs());

        var columns = [$('#macro-param-div-column'), $('#macro-param-div-row'), $('#macro-param-div-aggregation'),
            $('#macro-param-div-type')];
        var options = [$('#macro-param-div-sort'), $('#macro-param-div-decimalseparator'), $('#macro-param-div-separator'), $('#macro-param-div-decimals'),
            $('#macro-param-div-showrawdata'), $('#macro-param-div-hidePane'), $('#macro-param-div-ddSeparator')];

        $('.tf-macrobrowser-tabs a').attr('disabled', 'disabled');

        var columnsTab = $('#tab-columns');
        var optionsTab = $('#tab-options');
        var tabs = [columnsTab, optionsTab];

        columnsTab.click(function() {
            setActive(columnsTab);
            fadeElements([columns, options]);
        });

        optionsTab.click(function() {
            setActive(optionsTab);
            fadeElements([options, columns]);
        });

        columnsTab.trigger('click');

        function fadeElements(elements) {
            fade(elements[0], 'in');
            fade(elements[1], 'out');

            function fade(elements, state) {
                for (var i = 0; i < elements.length; i++) {
                    if (state == 'in') {
                        elements[i].show();
                    } else {
                        elements[i].hide();
                    }
                }
            }
        }

        function setActive(tab) {
            if (!tab.hasClass('active-tab')) {
                tabs.forEach(function(_tab) {
                    _tab.removeClass('active-tab');
                });
                tab.addClass('active-tab');
            }
        }
    }
})(jQuery);
}catch(e){WRMCB(e)};
;
try {
/* module-key = 'com.stiltsoft.confluence.plugin.tablefilter.tablefilter:macro-browser-resources', location = 'js/pivot-macro.js' */
(function($) {

    var chosenUpdated = false;
    var pivotTableMacroJsOverride;

    var PivotTableMacroJsOverride = function() {
        this.pivotTableMacro = new PivotTableMacro();
    };

    PivotTableMacroJsOverride.prototype.getMacroDetailsFromSelectedMacro = function(metadataList, macro) {
        chosenUpdated = false;
        pivotTableMacroJsOverride.pivotTableMacro.getMacroDetails(macro.body);
    };

    PivotTableMacroJsOverride.prototype.postPreview = function(iframe) {
        var self = pivotTableMacroJsOverride.pivotTableMacro;
        var contents = $(iframe).contents();

        var updateColumns = function() {
            var body = $('<div><div class="original-table">' + contents.find('.original-table').html() + '</div></div>');
            TableUtils.normalizeTable(body);

            self.tables = TableUtils.getTables(body, '.original-table', false, ':not(.tf-numbering)');
            var hasNew = false;

            $.each(self.tables, function(i, table) {
                $.each(table[0], function(i, cell) {
                    if ($.inArray(cell, self.tableColumns) == -1) {
                        hasNew = true;
                        self.tableColumns.push(cell);
                        self.columnSelect.append($('<option></option>').text(cell));
                        self.rowSelect.append($('<option></option>').text(cell));
                        self.aggregateSelect.append($('<option></option>').text(cell));
                    }
                });
            });

            if (hasNew) {
                self.columnSelect.trigger('chosen:updated');
                self.rowSelect.trigger('chosen:updated');
                self.aggregateSelect.trigger('chosen:updated');
                TableUtils.sortSelectedOptions(self.columnSelect, self.columnParams);
                TableUtils.sortSelectedOptions(self.rowSelect, self.rowParams);
                TableUtils.sortSelectedOptions(self.aggregateSelect, self.aggregateParams);
            }

            var ds = $('#macro-param-decimalseparator');
            var ts = $('#macro-param-separator');
            ds.add(ts).change(function() {
                if (ds.val() == ts.val()) {
                    ts.val('');
                }
            });
        };

        contents.find('.original-table').bind('DOMNodeInserted', function(e) {
            var target = $(e.target);
            if (e.relatedNode.className == this.className || target.hasClass("aui-nav-pagination") ||
                target.hasClass("user-profile-list-result") || target.attr('id') == "teamcity") {
                updateColumns();
            }
        });

        updateColumns();
    };

    PivotTableMacroJsOverride.prototype.beforeParamsSet = function(params) {
        var self = pivotTableMacroJsOverride.pivotTableMacro;
        if (!self.gotBody) {
            pivotTableMacroJsOverride.getMacroDetailsFromSelectedMacro(null, {body: tinymce.activeEditor.selection.getContent()});
        }
        self.gotBody = false;
        self.transformElements(params);

        params['id'] = new Date().getTime() + "_" + TableUtils.hashCode(self.tfBody);

        return params;
    };

    PivotTableMacroJsOverride.prototype.beforeParamsRetrieved = function(params) {
        var self = pivotTableMacroJsOverride.pivotTableMacro;
        if (!chosenUpdated) {
            self.columnSelect.trigger('chosen:updated');
            self.rowSelect.trigger('chosen:updated');
            self.aggregateSelect.trigger('chosen:updated');
            TableUtils.sortSelectedOptions(self.columnSelect, self.columnParams);
            TableUtils.sortSelectedOptions(self.rowSelect, self.rowParams);
            TableUtils.sortSelectedOptions(self.aggregateSelect, self.aggregateParams);
            chosenUpdated = true;
        }

        params["type"] = self.typeParams.join("‚");
        params["column"] = self.columnParams.join("‚");
        params["row"] = self.rowParams.join("‚");
        params["aggregation"] = self.aggregateParams.join("‚");

        return params;
    };

    pivotTableMacroJsOverride = new PivotTableMacroJsOverride();
    AJS.MacroBrowser.setMacroJsOverride("pivot-table", pivotTableMacroJsOverride);

})(AJS.$);


}catch(e){WRMCB(e)};
;
try {
/* module-key = 'com.stiltsoft.confluence.plugin.tablefilter.tablefilter:macro-browser-resources', location = 'tf-libs/js/chart-macro.js' */
(function($) {

    window.TableChartMacro = function() {
        this.tables = [];
        this.tableColumns = [];
        this.columnSelect = null;
        this.aggregateSelect = null;
        this.typeSelect = null;
        this.tfBody = "";
        this.columnParams = [];
        this.aggregateParams = [];
        this.isPivot = false;
        this.gotBody = false;
        this.transpose = false;
        this.colors = [];
        this.pieKeys = [];
        this.isCloud = false;
    };

    TableChartMacro.prototype.getMacroDetails = function(macroBody, dataOrientation, isPivot, isCloud) {
        var self = this;

        self.isCloud = isCloud;
        self.transpose = dataOrientation == "Horizontal";
        self.gotBody = true;
        self.isPivot = isPivot ? isPivot : false;
        self.tfBody = macroBody;
        if (self.tfBody.indexOf('<ac:structured-macro ac:name="pivot-table"') != -1) {
            self.isPivot = true;
            return;
        }
        var body = $('<div><div class="chart-body">' + self.tfBody + '</div></div>');

        self.tables = TableUtils.getTables(body, '.chart-body', false, ':not(.tf-numbering)');
        if (self.transpose) {
            self.tables = TableUtils.transpose(self.tables);
        }

        self.tableColumns = TableUtils.getColumns(self.tables);
    };

    TableChartMacro.prototype.transformElements = function(params) {
        var self = this;

        if (!params["version"] && !params["formatVersion"]) {
            // TBLFILTER-268
            params["version"] = 2;
            if (params['type'] && (params['type'].indexOf('Line') >= 0 || params['type'].indexOf('Area') >= 0)) {
                var tmpCol = params["column"];
                params["column"] = params["aggregation"];
                params["aggregation"] = tmpCol;
            }
        }

        self.transpose = params["dataorientation"] == "Horizontal";

        $('#macro-browser-dialog .macro-desc .macro-doco-link').css('float', 'right')
            .before("Post \u003ca href=\u0022https://stiltsoft.customercase.com/forums/table-filter-for-confluence/ideas/create?from=macro\u0022\u003eIdea\u003c/a\u003e or \u003ca href=\u0022https://stiltsoft.customercase.com/forums/support-requests/ideas/create?from=macro&category=Table%2BFilter%2Band%2BCharts%2Bfor%2BConfluence\u0022\u003eIssue\u003c/a\u003e.");

        $('#macro-param-div-id').hide();

        if (params["column"]) {
            self.columnParams = params["column"].split('‚');
            $.each(self.columnParams, function (i, name){
                self.columnParams[i] = $.trim(name);
            });
        } else {
            self.columnParams = self.tableColumns.length ? [TableUtils.getRawColumns(self.tables)[0]] : [];
        }

        if (params["aggregation"]) {
            self.aggregateParams = params["aggregation"].split('‚');
            $.each(self.aggregateParams, function (i, name){
                self.aggregateParams[i] = $.trim(name);
            });
        } else {
            self.aggregateParams = self.tableColumns.length > 1 ? [TableUtils.getRawColumns(self.tables)[1]] : [];
        }

        if (params["colors"]) {
            self.colors = params["colors"].split(',');
        }
        TableUtils.chartColors.forEach(function(color) {
            if ($.inArray(color, self.colors) == -1) {
                self.colors.push(color);
            }
        });

        if (params["pieKeys"]) {
            self.pieKeys = params["pieKeys"].split('‚');
        }

        setSavedValues(params);

        var columnInput = $('#macro-param-column');
        var aggregateInput = $('#macro-param-aggregation');
        self.typeSelect = $('#macro-param-type');

        self.columnSelect = createMultipleSelect(columnInput, self.tableColumns, self.columnParams, self.aggregateParams, "none", [], self);
        self.aggregateSelect = createMultipleSelect(aggregateInput, self.tableColumns, self.aggregateParams, [], "Column|Bar|Line|Area", self.colors);

        $('.search-field input').one('focus', function() {
            $('.search-choice').css('max-width', $('.chosen-container').width() - 40);
        });

        var dateFormatInput = $('#macro-param-datepattern');
        TableUtils.dateFormatAutocomplete(dateFormatInput);
        dateFormatInput.val(params["datepattern"] || (localStorage.getItem('tf-dateformat') || TableUtils.getLocaleDateFormat($.datepicker.ATOM)));

        var dateFormatContainer = dateFormatInput.parent().addClass('chart-browser-hidden');
        var columnWrapper = $('#macro-param-div-column');
        var aggregationWrapper = $('#macro-param-div-aggregation');
        var gridWrapper = $('#macro-param-div-grid').addClass('chart-browser-hidden');
        var legendWrapper = $('#macro-param-div-legend').addClass('chart-browser-hidden');
        var minvalueWrapper = $('#macro-param-div-minvalue').addClass('chart-browser-hidden');
        var maxvalueWrapper = $('#macro-param-div-maxvalue').addClass('chart-browser-hidden');
        var xMinvalueWrapper = $('#macro-param-div-xminvalue').addClass('chart-browser-hidden');
        var xMaxvalueWrapper = $('#macro-param-div-xmaxvalue').addClass('chart-browser-hidden');
        var scaleStepWrapper = $('#macro-param-div-scalestep').addClass('chart-browser-hidden');
        var xScaleStepWrapper = $('#macro-param-div-xscalestep').addClass('chart-browser-hidden');
        var xTitleWrapper = $('#macro-param-div-xtitle');
        var yTitleWrapper = $('#macro-param-div-ytitle');
        var logWrapper = $('#macro-param-div-log').addClass('chart-browser-hidden');
        var xLogWrapper = $('#macro-param-div-xlog').addClass('chart-browser-hidden');
        xScaleStepWrapper.find('.macro-param-desc').addClass('chart-browser-hidden');
        scaleStepWrapper.find('.macro-param-desc').addClass('chart-browser-hidden');
        var prevMultipleChoice = params['type'] && (params['type'].indexOf('Bar') >= 0 || params['type'].indexOf('Column') >= 0 ||
                params['type'].indexOf('Line') >= 0 || params['type'].indexOf('Area') >= 0);

        self.typeSelect.change(function() {
            var isSeries = true;
            var isBar = self.typeSelect.val().indexOf('Bar') >= 0 || self.typeSelect.val().indexOf('Column') >= 0;
            self.isMultipleChoice = isBar || self.typeSelect.val().indexOf('Line') >= 0 || self.typeSelect.val().indexOf('Area') >= 0;

            if (self.typeSelect.val() == "Time Line" || self.typeSelect.val() == "Time Area") {
                showElement(dateFormatContainer);
                aggregationWrapper.find('label').text("Values column");
                columnWrapper.find('label').text("Dates column");
                showElement(scaleStepWrapper.find('.macro-param-desc'));
                xTitleWrapper.find('label').text("Date axis label");
                hideElement(xLogWrapper);
                showElement(logWrapper);
                logWrapper.find('label').text("Logarithmic scale");
            } else {
                hideElement(scaleStepWrapper.find('.macro-param-desc'));
                hideElement(dateFormatContainer);
                if (self.typeSelect.val() == "Line" || self.typeSelect.val() == "Area") {
                    aggregationWrapper.find('label').text("Y-axis value column");
                    columnWrapper.find('label').text("X-axis value column");
                    xTitleWrapper.find('label').text("X-axis label");
                    logWrapper.find('label').text("Logarithmic scale Y");
                    xLogWrapper.find('label').text("Logarithmic scale X");
                    showElement(logWrapper);
                    showElement(xLogWrapper);
                } else {
                    isSeries = false;
                    columnWrapper.find('label').text("Labels column");
                    aggregationWrapper.find('label').text("Values column");
                    xTitleWrapper.find('label').text("Row axis label");
                    hideElement(xLogWrapper);
                    if (isBar) {
                        showElement(logWrapper);
                        logWrapper.find('label').text("Logarithmic scale");
                    } else {
                        hideElement(logWrapper);
                    }
                }
            }

            if (self.isPivot) {
                hideElement(columnWrapper);
                if (self.tableColumns.length > 2) {
                    aggregationWrapper.show();
                } else {
                    hideElement(aggregationWrapper);
                    self.typeSelect.find('option').each(function() {
                        var opt = $(this);
                        if (opt.val() == 'Line' || opt.val() == 'Area') {
                            opt.hide();
                        }
                    });
                }
            }

            if (self.isMultipleChoice ? !prevMultipleChoice : prevMultipleChoice) {
                if (prevMultipleChoice) {
                    if (self.aggregateParams.length > 1) {
                        self.aggregateParams.length = 1;
                        self.aggregateSelect.find('option:selected').removeAttr('selected');
                        self.aggregateSelect.find('option').filter(function() {
                            return $(this).text() == self.aggregateParams[0];
                        }).attr('selected', 'selected');
                        aggregateInput.val(self.aggregateParams[0]);
                        self.aggregateSelect.trigger('chosen:updated');
                    }
                }
                prevMultipleChoice = self.isMultipleChoice;
            }

            if (isSeries) {
                minvalueWrapper.find('label').text("Minimal Y value");
                maxvalueWrapper.find('label').text("Maximal Y value");
                scaleStepWrapper.find('label').text("Scale Y step");
                yTitleWrapper.find('label').text("Y-axis label");
                showElement(xMinvalueWrapper);
                showElement(xMaxvalueWrapper);
                showElement(gridWrapper);
                showElement(xScaleStepWrapper);
                scaleStepWrapper.addClass('multiple').css('display', '');
            } else {
                minvalueWrapper.find('label').text("Minimal value");
                maxvalueWrapper.find('label').text("Maximal value");
                scaleStepWrapper.find('label').text("Scale step");
                yTitleWrapper.find('label').text("Value axis label");
                hideElement(gridWrapper);
                hideElement(xMinvalueWrapper);
                hideElement(xMaxvalueWrapper);
                hideElement(xScaleStepWrapper);
                scaleStepWrapper.removeClass('multiple');
            }

            if (self.isMultipleChoice) {
                showElement(legendWrapper);
                showElement(minvalueWrapper);
                showElement(maxvalueWrapper);
                showElement(scaleStepWrapper);
            } else {
                hideElement(legendWrapper);
                hideElement(minvalueWrapper);
                hideElement(maxvalueWrapper);
                hideElement(scaleStepWrapper);
            }

            if (self.typeSelect.val() == "Pie" || self.typeSelect.val() == "Donut" || self.typeSelect.val() == "3D Donut") {
                hideElement(xTitleWrapper);
                hideElement(yTitleWrapper);
            } else {
                showElement(xTitleWrapper);
                showElement(yTitleWrapper);
            }
        });

        self.typeSelect.val(params['type'] || 'Pie');

        self.typeSelect.trigger('change');

        $('#macro-param-dataorientation').change(function() {
            self.transpose = $(this).val() == "Horizontal";
            self.tables = TableUtils.transpose(self.tables);
            self.tableColumns = TableUtils.getColumns(self.tables);
            var tc = TableUtils.getRawColumns(self.tables);

            self.columnSelect.find('option').remove();
            self.aggregateSelect.find('option').remove();
            $.each(self.tableColumns, function(j, cell) {
                self.columnSelect.append($('<option></option>').text(cell));
                self.aggregateSelect.append($('<option></option>').text(cell));
            });
            self.columnSelect.val(tc[0]).trigger('change', {selected: tc[0]}).trigger('chosen:updated');
            self.aggregateSelect.val(tc[1]).trigger('change', {selected: tc[1]}).trigger('chosen:updated');
        });

        self.updatePieKeys();
        self.initInlineDialog($('#macro-param-div-colors'), "set-tf-chart-colors");
        appendTabs();
    };

    TableChartMacro.prototype.updatePieKeys = function() {
        var self = this;

        if (!self.isMultipleChoice) {
            var column = self.columnParams.length ? self.columnParams[0] : self.tableColumns[0];
            var pieKeys = [];
            self.tables.forEach(function(table) {
                if (table.length) {
                    var position = $.inArray(column, table[0]);
                    table.forEach(function(row, i) {
                        if (i > 0)
                            pieKeys.push(row[position]);
                    });
                }
            });

            var colors = self.colors.slice();
            self.pieKeys.forEach(function(param, i) {
                var index = $.inArray(param, pieKeys);
                if (index >= 0) {
                    self.colors[index] = colors[i % colors.length];
                }
            });

            self.pieKeys = pieKeys.slice();
        }
    };

    TableChartMacro.prototype.getTrimmedColors = function() {
        var self = this;
        return self.colors.slice(0, self.isMultipleChoice ? self.aggregateParams.length : self.pieKeys.length);
    };

    TableChartMacro.prototype.initInlineDialog = function(macroElem, dialogName) {
        var self = this;
        var input = macroElem.find('input');
        var label = macroElem.find('label');
        input.hide();
        label.hide();

        macroElem.prepend(TableFilterMacro.subtleButton({id: dialogName, name: label.text()}));

        bindHideMacroDialogEvents(dialogName);

        $("#inline-dialog-" + dialogName).remove();

        AJS.InlineDialog($("#button" + dialogName), dialogName,
            function (content, trigger, showPopup) {
                if (self.isCloud) {
                    self.updatePieKeys();
                }

                content.html(ChartTemplate.colorDialog({
                    colors: self.colors,
                    palette: TableUtils.chartColors,
                    labels: self.isMultipleChoice ? self.aggregateParams : self.pieKeys
                }));

                showPopup();
                return false;
            }, {
                hideDelay: null,
                initCallback: function () {
                    var colors = self.colors.slice();
                    var popup = this;

                    popup.popup.find('#cancel').click(function() {
                        popup.hide();
                        return false;
                    });

                    popup.popup.find('#apply').click(function() {
                        $(this).prop('disabled', true);
                        popup.hide();
                        self.colors = colors;
                        return false;
                    });

                    $('.tfc-color:not(.selected)').click(function() {
                        var a = $(this);
                        colors[a.data('index')] = a.data('color');
                        popup.popup.find('button .tfc-color').eq(a.data('index')).css('background', a.data('color')).attr('data-color', a.data('color'));
                    });

                    popup.popup.find('.aui-dropdown2').click(function() {
                        return false;
                    });

                    $('.tf-macrobrowser-tabs #tab-columns, .tf-macrobrowser-tabs #tab-advanced-options').click(function() {
                        popup.hide();
                        return false;
                    });

                    $('#buttonset-tf-chart-colors').click(function() {
                        if ($(this).hasClass('active')) {
                            popup.hide();
                            return false;
                        }
                    });
                },
                gravity: 'w',
                persistent: true,
                width: 200
            }
        );
    };

    function createMultipleSelect(input, tableColumns, params, additionalParams, multipleType, selfColors, tfc) {
        input.hide();

        var select = $('<select multiple/>');

        $.each(params, function(i, name) {
            name = $.trim(name);
            if ($.inArray(name, tableColumns) == -1) {
                tableColumns.push(name);
            }
        });

        tableColumns.sort();

        $.each(tableColumns, function(i, name) {
            select.append($('<option></option>').text(name));
        });
        $.each(additionalParams, function(i, name) {
            if ($.inArray(name, tableColumns) == -1) {
                tableColumns.push(name);
                select.append($('<option></option>').text(name));
            }
        });

        input.after(select);
        initChosen(select, input, params, multipleType, selfColors, tfc);

        return select;
    }

    function initChosen(select, input, params, multipleType, selfColors, tfc) {
        var multipleTypeRegex = new RegExp(multipleType);
        var typeSelect = $('#macro-param-type');

        select.chosen({
            no_results_text: "No matches found. Press \u0027Enter\u0027 to add",
            placeholder_text_multiple: "Click or start typing...",
            width: "100%",
            display_selected_options: false,
            addUserOption: true
        }).change(function(e, data) {
            if (data.selected) {
                var isMultiple = multipleTypeRegex.test(typeSelect.val());
                if (!isMultiple) {
                    select.find('option:selected').prop('selected', false);
                    params.length = 0;
                }
                select.find('option').filter(function() {
                    return $(this).text() == data.selected;
                }).prop('selected', true);
                params.push(data.selected);
                if (!isMultiple) {
                    select.trigger('chosen:updated');
                }
            } else if (data.deselected) {
                select.find('option').filter(function() {
                    return $(this).text() == data.deselected;
                }).prop('selected', false);
                params.splice($.inArray(data.deselected, params), 1);
            }

            input.val(params.join('‚'));

            if (tfc) {
                tfc.updatePieKeys();
            }
        });

        var choices = TableUtils.sortSelectedOptions(select, params);

        choices.sortable({
            tolerance: 'pointer',
            cursor: 'move',
            helper: 'clone',
            items: ".search-choice",
            stop: function() {
                var prevParams = params.slice();
                params.length = 0;
                choices.find('.search-choice').each(function() {
                    params.push($(this).text())
                });
                var colors = selfColors.slice();
                prevParams.forEach(function(param, i) {
                    var index = $.inArray(param, params);
                    if (index >= 0 && colors.length > index) {
                        selfColors[index] = colors[i];
                    }
                });
                input.val(params.join('‚'));
            }
        });

        select.val(params);
        select.trigger('chosen:updated');
        input.val(params.join('‚'));
    }

    function setSavedValues(params) {
        if (params['dateFormat'] || params['datepattern']) {
            $('#macro-param-datepattern').val(params['dateFormat'] || params['datepattern']);
        }

        if (params['tfc-height'] || params['height']) {
            $('#macro-param-tfc-height').val(params['tfc-height'] || params['height']);
        }

        if (params['tfc-width'] || params['width']) {
            $('#macro-param-tfc-width').val(params['tfc-width'] || params['width']);
        }

        if (params['separator'] || params['decimalSeparator']) {
            $('#macro-param-separator').val(params['separator'] || params['decimalSeparator']);
        }

        if (params['hide'] == 'true' || params['hideSource'] == 'true') {
            $('#macro-param-hide').prop('checked', true);
        }

        if (params['hideControls'] == 'true' || params['hidecontrols'] == 'true') {
            $('#macro-param-hidecontrols').prop('checked', true);
        }

        if (params['dataorientation']) {
            $('#macro-param-dataorientation').val(params['dataorientation']);
        }

        if (params['grid'] == 'false') {
            $('#macro-param-grid').prop('checked', false);
        } else {
            $('#macro-param-grid').prop('checked', true);
        }

        if (params['legend'] == 'false') {
            $('#macro-param-legend').prop('checked', false);
        } else {
            $('#macro-param-legend').prop('checked', true);
        }

        if (params['minvalue']) {
            $('#macro-param-minvalue').val(params['minvalue']);
        }

        if (params['maxvalue']) {
            $('#macro-param-maxvalue').val(params['maxvalue']);
        }

        if (params['xminvalue']) {
            $('#macro-param-xminvalue').val(params['xminvalue']);
        }

        if (params['xmaxvalue']) {
            $('#macro-param-xmaxvalue').val(params['xmaxvalue']);
        }

        if (params['scalestep']) {
            $('#macro-param-scalestep').val(params['scalestep']);
        }

        if (params['xscalestep']) {
            $('#macro-param-xscalestep').val(params['xscalestep']);
        }

        if (params['title']) {
            $('#macro-param-title').val(params['title']);
        }

        if (params['xtitle']) {
            $('#macro-param-xtitle').val(params['xtitle']);
        }

        if (params['ytitle']) {
            $('#macro-param-ytitle').val(params['ytitle']);
        }

        if (params['align']) {
            $('#macro-param-align').val(params['align']);
        }

        if (params['log'] == 'true') {
            $('#macro-param-log').prop('checked', true);
        }

        if (params['xlog'] == 'true') {
            $('#macro-param-xlog').prop('checked', true);
        }
    }

    function bindHideMacroDialogEvents(uniqueDialogName) {
        $('#macro-insert-container .macro-input-fields').scroll(function () {
            $("#inline-dialog-" + uniqueDialogName).hide();
        });

        var ev = 'hide';
        var el = $.fn[ev];
        $.fn[ev] = function () {
            if (this.attr('id') == 'macro-browser-dialog') {
                $("#inline-dialog-" + uniqueDialogName).hide();
            }
            return el.apply(this, arguments);
        };
    }

    function appendTabs() {
        $('#macro-param-div-column').before(ChartTemplate.macrobrowserTabs());

        var columns = [$('#macro-param-div-column'), $('#macro-param-div-aggregation'), $('#macro-param-div-type')];
        var displayOptions = [$('#macro-param-div-tfc-height'), $('#macro-param-div-tfc-width'), $('#macro-param-div-colors'),
            $('#macro-param-div-align'), $('#macro-param-div-legend'), $('#macro-param-div-grid'), $('#macro-param-div-hide'),
            $('#macro-param-div-hidecontrols'), $('#macro-param-div-title'), $('#macro-param-div-xtitle'), $('#macro-param-div-ytitle')];
        var advancedOptions = [$('#macro-param-div-datepattern'), $('#macro-param-div-separator'), $('#macro-param-div-dataorientation'),
            $('#macro-param-div-xminvalue'), $('#macro-param-div-xmaxvalue'), $('#macro-param-div-minvalue'), $('#macro-param-div-maxvalue'),
            $('#macro-param-div-xscalestep'), $('#macro-param-div-scalestep'), $('#macro-param-div-log'), $('#macro-param-div-xlog')];

        $('.tf-macrobrowser-tabs a').attr('disabled', 'disabled');

        var columnsTab = $('#tab-columns');
        var displayOptionsTab = $('#tab-display-options');
        var advancedOptionsTab = $('#tab-advanced-options');
        var tabs = [columnsTab, displayOptionsTab, advancedOptionsTab];

        columnsTab.click(function() {
            if (!columnsTab.hasClass('active-tab')) {
                setActive(columnsTab);
                fadeElements([columns, displayOptions, advancedOptions]);
            }
        });

        displayOptionsTab.click(function() {
            if (!displayOptionsTab.hasClass('active-tab')) {
                setActive(displayOptionsTab);
                fadeElements([displayOptions, columns, advancedOptions]);
            }
        });

        advancedOptionsTab.click(function() {
            if (!advancedOptionsTab.hasClass('active-tab')) {
                setActive(advancedOptionsTab);
                fadeElements([advancedOptions, columns, displayOptions]);
            }
        });

        columnsTab.trigger('click');

        function fadeElements(elements) {
            fade(elements[0], 'in');
            for (var i = 1; i < elements.length; i++) {
                fade(elements[i], 'out');
            }

            function fade(elements, state) {
                for (var i = 0; i < elements.length; i++) {
                    if (state == 'in') {
                        elements[i].removeClass('chart-browser-hidden-by-tab');
                    } else {
                        elements[i].addClass('chart-browser-hidden-by-tab');
                    }
                }
            }
        }

        function setActive(tab) {
            if (!tab.hasClass('active-tab')) {
                tabs.forEach(function(_tab) {
                    _tab.removeClass('active-tab');
                });
                tab.addClass('active-tab');
            }
        }
    }

    function hideElement(element) {
        if (!element.hasClass('chart-browser-hidden')) {
            element.addClass('chart-browser-hidden');
        }
    }

    function showElement(element) {
        if (element.hasClass('chart-browser-hidden')) {
            element.removeClass('chart-browser-hidden');
        }
    }
})(jQuery);



}catch(e){WRMCB(e)};
;
try {
/* module-key = 'com.stiltsoft.confluence.plugin.tablefilter.tablefilter:macro-browser-resources', location = 'js/chart-macro.js' */
(function($) {

    var chosenUpdated = false;
    var tableChartMacroJsOverride;

    var TableChartMacroJsOverride = function() {
        this.tableChartMacro = null;
    };

    TableChartMacroJsOverride.prototype.getMacroDetailsFromSelectedMacro = function(metadataList, macro) {
        chosenUpdated = false;
        getChartMacro(tableChartMacroJsOverride).getMacroDetails(macro.body, macro.params["dataorientation"], false);
    };

    TableChartMacroJsOverride.prototype.beforeParamsRetrieved = function(params) {
        var self = tableChartMacroJsOverride.tableChartMacro;
        if (!chosenUpdated) {
            self.columnSelect.trigger('chosen:updated');
            self.aggregateSelect.trigger('chosen:updated');
            TableUtils.sortSelectedOptions(self.columnSelect, self.columnParams);
            TableUtils.sortSelectedOptions(self.aggregateSelect, self.aggregateParams);
            chosenUpdated = true;
        }

        params["colors"] = self.getTrimmedColors().join(',');
        params["pieKeys"] = self.pieKeys.join('‚');

        return params;
    };

    TableChartMacroJsOverride.prototype.postPreview = function(iframe) {
        var self = tableChartMacroJsOverride.tableChartMacro;
        var contents = $(iframe).contents();

        var updateColumns = function() {
            var body = $('<div><div class="chart-body">' + contents.find('.chart-body').html() + '</div></div>');
            body.find('.pivot-content .original-table').remove();
            TableUtils.normalizeTable(body);
            var hasNew = false;

            self.tables = TableUtils.getTables(body, '.chart-body', false);
            if (self.transpose) {
                self.tables = TableUtils.transpose(self.tables);
            }
            $.each(self.tables, function(i, table) {
                $.each(table[0], function(i, cell) {
                    cell = $.trim(cell);
                    if ($.inArray(cell, self.tableColumns) == -1) {
                        hasNew = true;
                        self.tableColumns.push(cell);
                        var shouldSelect = (self.columnParams && $.inArray(cell, self.columnParams) >= 0) || (!self.columnParams && i == 0);
                        var selected = shouldSelect ? 'selected="selected"' : '';
                        self.columnSelect.append($('<option ' + selected + '></option>').text(cell));
                        shouldSelect = (!self.aggregateParams && i == 0) || (self.aggregateParams && $.inArray(cell, self.aggregateParams) >= 0);
                        selected = shouldSelect ? 'selected="selected"' : '';
                        self.aggregateSelect.append($('<option ' + selected + '></option>').text(cell));
                    }
                });
            });

            if (self.isPivot) {
                if (self.tableColumns.length > 2) {
                    $('#macro-param-div-aggregation').removeClass('chart-browser-hidden');
                    self.typeSelect.find('option').each(function() {
                        var opt = $(this);
                        if (opt.val() == 'Line' || opt.val() == 'Area') {
                            opt.show();
                        }
                    });
                }
            }

            if (hasNew) {
                self.columnSelect.trigger('chosen:updated');
                self.aggregateSelect.trigger('chosen:updated');
                TableUtils.sortSelectedOptions(self.columnSelect, self.columnParams);
                TableUtils.sortSelectedOptions(self.aggregateSelect, self.aggregateParams);
            }

            self.typeSelect.trigger('change');
            self.updatePieKeys();
        };

        contents.find('.chart-body').bind('DOMNodeInserted', function(e) {
            var target = $(e.target);
            if (e.relatedNode.className == this.className || target.hasClass("aui-nav-pagination") ||
                target.hasClass("user-profile-list-result") || target.attr('id') == "teamcity") {
                updateColumns();
            }
        });

        updateColumns();
    };

    TableChartMacroJsOverride.prototype.beforeParamsSet = function(params) {
        $('.tfc-container').remove();

        var self = getChartMacro(tableChartMacroJsOverride);

        if (!self.gotBody) {
            tableChartMacroJsOverride.getMacroDetailsFromSelectedMacro(null, {body: tinymce.activeEditor.selection.getContent(), params: params});
        }
        self.gotBody = false;
        self.transformElements(params);

        params['id'] = new Date().getTime() + "_" + TableUtils.hashCode(self.tfBody);

        return params;
    };

    tableChartMacroJsOverride = new TableChartMacroJsOverride();
    AJS.MacroBrowser.setMacroJsOverride("table-chart", tableChartMacroJsOverride);

    function getChartMacro(tableChartMacroJsOverride) {
        if (tableChartMacroJsOverride.tableChartMacro == null) {
            tableChartMacroJsOverride.tableChartMacro = new TableChartMacro();
        }
        return tableChartMacroJsOverride.tableChartMacro;
    }
})(AJS.$);



}catch(e){WRMCB(e)};
;
try {
/* module-key = 'com.stiltsoft.confluence.plugin.tablefilter.tablefilter:macro-browser-resources', location = 'tf-libs/js/tf-macro.js' */
(function($) {

    window.TablefilterMacro = function() {
        var self = this;
        self.tables = [];
        self.iconTables = [];
        self.tableColumns = [];
        self.sortColumns = [];
        self.columnSelect = null;
        self.userfilterSelect = null;
        self.numberfilterSelect = null;
        self.datefilterSelect = null;
        self.iconfilterSelect = null;
        self.sortSelect = null;
        self.columnParams = [];
        self.userfilterParams = [];
        self.numberfilterParams = [];
        self.datefilterParams = [];
        self.iconfilterParams = [];
        self.sortParams = [];
        self.dateFormatInput = null;
        self.dateFormatContainer = null;
        self.separatorContainer = null;
        self.supportOldSize = false;
        self.tfBody = "";
        self.defaults = {};
        self.widths = {};
        self.inverse = {};
        self.gotBody = false;
        self.order = {};
        self.labels = {};
    };

    TablefilterMacro.prototype.getMacroDetails = function(macroBody) {
        var self = this;
        self.columnParams = [];
        self.userfilterParams = [];
        self.numberfilterParams = [];
        self.datefilterParams = [];
        self.iconfilterParams = [];
        self.sortParams = [];
        self.chosenUpdated = false;
        self.gotBody = true;
        self.tfBody = macroBody;
        self.body = $('<div><div class="tablefilter-table-wrapper">' + macroBody + '</div></div>');

        self.tables = TableUtils.getTables(self.body, '.tablefilter-table-wrapper');
        self.iconTables = TableUtils.getTables(self.body, '.tablefilter-table-wrapper', true);
        self.tableColumns = TableUtils.getColumns(self.tables);
        self.sortColumns = [];
        $.each(self.tableColumns, function(i, column) {
            if ($.trim(column).length) {
                self.sortColumns.push(column + " (\u200DAsc)");
                self.sortColumns.push(column + " (\u200DDesc)");
            }
        });
    };

    TablefilterMacro.prototype.transformElements = function(params) {
        var self = this;

        $('#macro-browser-dialog .macro-desc .macro-doco-link').css('float', 'right')
            .before("Post \u003ca href=\u0022https://stiltsoft.customercase.com/forums/table-filter-for-confluence/ideas/create?from=macro\u0022\u003eIdea\u003c/a\u003e or \u003ca href=\u0022https://stiltsoft.customercase.com/forums/support-requests/ideas/create?from=macro&category=Table%2BFilter%2Band%2BCharts%2Bfor%2BConfluence\u0022\u003eIssue\u003c/a\u003e.");

        $('#macro-param-div-id').hide();

        var columnParams = [];
        var userfilterParams = [];
        var numberfilterParams = [];
        var datefilterParams = [];
        var iconfilterParams = [];
        var sortParams = [];
        if (params["column"] != undefined) {
            columnParams = params["column"].split(',');
        }

        if (params["userfilter"] != undefined) {
            userfilterParams = params["userfilter"].split(',');
        }

        if (params["sort"]) {
            sortParams = params["sort"].split(',');
        }

        if (params["iconfilter"] != undefined) {
            iconfilterParams = params["iconfilter"].split(',');
        }

        setSavedParams(params);

        self.separatorContainer = $('#macro-param-div-separator');
        self.dateFormatInput = $('#macro-param-datepattern');
        TableUtils.dateFormatAutocomplete(self.dateFormatInput);
        self.dateFormatContainer = self.dateFormatInput.parent();
        if (params["datefilter"] != undefined) {
            datefilterParams = params["datefilter"].split(',');
        }

        if (params["numberfilter"] != undefined) {
            numberfilterParams = params["numberfilter"].split(',');
        }

        self.dateFormatInput.val(params["datepattern"] || (localStorage.getItem('tf-dateformat') || TableUtils.getLocaleDateFormat($.datepicker.ATOM)));
        var column = $('#macro-param-column');
        var userfilter = $('#macro-param-userfilter');
        var datefilter = $('#macro-param-datefilter');
        var numberfilter = $('#macro-param-numberfilter');
        var iconfilter = $('#macro-param-iconfilter');
        var sort = $('#macro-param-sort');

        var cols = $.merge([], columnParams);
        $.merge(cols, userfilterParams);
        $.merge(cols, datefilterParams);
        $.merge(cols, numberfilterParams);
        $.merge(cols, iconfilterParams);
        $.each(cols, function(i, name) {
            var col = $.trim(name.replace(/\‚/g, ','));
            if ($.inArray(col, self.tableColumns) == -1) {
                self.tableColumns.push(col);
                self.sortColumns.push(col + " (\u200DAsc)");
                self.sortColumns.push(col + " (\u200DDesc)");
            }
        });
        $.each(sortParams, function(i, name) {
            var col = $.trim(name.replace(/\‚/g, ','));
            if ($.inArray(col, self.sortColumns) == -1) {
                self.sortColumns.push(col);
            }
        });

        self.tableColumns.sort();
        self.sortColumns.sort();

        self.columnSelect = self.createSelect(column, self.tableColumns, columnParams, self.columnParams);
        self.userfilterSelect = self.createSelect(userfilter, self.tableColumns, userfilterParams, self.userfilterParams);
        self.datefilterSelect = self.createSelect(datefilter, self.tableColumns, datefilterParams, self.datefilterParams);
        self.numberfilterSelect = self.createSelect(numberfilter, self.tableColumns, numberfilterParams, self.numberfilterParams);
        self.iconfilterSelect = self.createSelect(iconfilter, self.tableColumns, iconfilterParams, self.iconfilterParams);
        self.sortSelect = self.createSelect(sort, self.sortColumns, sortParams, self.sortParams);

        if (!self.chosenUpdated) {
            self.columnSelect.trigger('chosen:updated');
            self.userfilterSelect.trigger('chosen:updated');
            self.datefilterSelect.trigger('chosen:updated');
            self.numberfilterSelect.trigger('chosen:updated');
            self.iconfilterSelect.trigger('chosen:updated');
            self.sortSelect.trigger('chosen:updated');
            TableUtils.sortSelectedOptions(self.columnSelect, self.columnParams);
            TableUtils.sortSelectedOptions(self.userfilterSelect, self.userfilterParams);
            TableUtils.sortSelectedOptions(self.datefilterSelect, self.datefilterParams);
            TableUtils.sortSelectedOptions(self.numberfilterSelect, self.numberfilterParams);
            TableUtils.sortSelectedOptions(self.iconfilterSelect, self.iconfilterParams);
            TableUtils.sortSelectedOptions(self.sortSelect, self.sortParams);
            self.chosenUpdated = true;
        }

        $('.search-field input').one('focus', function() {
            $('.search-choice').css('max-width', $('.chosen-container').width() - 40);
        });

        var defaultMacroElem = $('#macro-param-div-default');
        var inputWidthMacroElem = $('#macro-param-div-cell-width');
        var inputInverseMacroElem = $('#macro-param-div-inverse');
        var inputLabelsMacroElem = $('#macro-param-div-labels');

        self.initInlineDialog(defaultMacroElem, 'DefaultValDialog');
        self.initInlineDialog(inputWidthMacroElem, 'CellWidthDialog');
        self.initInlineDialog(inputInverseMacroElem, 'InverseFilterDialog');
        self.initInlineDialog(inputLabelsMacroElem, 'FilterLabelsDialog');

        var defValues = params["default"] ? params["default"].split(',') : [];
        var widthValues = params["cell-width"] ? params["cell-width"].split(',') : [];
        var inverseValues = params["inverse"] ? params["inverse"].split(',') : [];
        var orderValues = params["order"] ? params["order"].split(',') : [];
        var labelsValues = params["labels"] ? params["labels"].split('‚'): [];
        var n = 0, w = 0, l = 0, inv = 0, o = 0;

        self.order["select_"] = [];
        $.each(columnParams, function(i, param) {
            self.defaults["select_" + param] = defValues.length > n ? defValues[n++] : "";
            self.inverse["select_" + param] = inverseValues.length > inv ? inverseValues[inv++] == 'true' : false;
            self.widths["select_" + param] = widthValues.length > w ? widthValues[w++] : "";
            self.labels["select_" + param] = labelsValues.length > l ? labelsValues[l++] : "";
            self.order["select_"].push(orderValues.length > o ? parseInt(orderValues[o++]) : o++);
        });
        self.order["freetext_"] = [];
        $.each(userfilterParams, function(i, param) {
            self.defaults["freetext_" + param] = defValues.length > n ? defValues[n++] : "";
            self.inverse["freetext_" + param] = inverseValues.length > inv ? inverseValues[inv++] == 'true' : false;
            self.widths["freetext_" + param] = widthValues.length > w ? widthValues[w++] : "";
            self.labels["freetext_" + param] = labelsValues.length > l ? labelsValues[l++] : "";
            self.order["freetext_"].push(orderValues.length > o ? parseInt(orderValues[o++]) : o++);
        });
        self.order["number_"] = [];
        $.each(numberfilterParams, function(i, param) {
            self.defaults["number_" + param] = defValues.length > n ? defValues[n++] : "";
            self.inverse["number_" + param] = inverseValues.length > inv ? inverseValues[inv++] == 'true' : false;
            self.labels["number_" + param] = labelsValues.length > l ? labelsValues[l++] : "";
            self.order["number_"].push(orderValues.length > o ? parseInt(orderValues[o++]) : o++);

        });
        self.order["date_"] = [];
        $.each(datefilterParams, function(i, param) {
            self.defaults["date_" + param] = defValues.length > n ? defValues[n++] : "";
            self.inverse["date_" + param] = inverseValues.length > inv ? inverseValues[inv++] == 'true' : false;
            self.labels["date_" + param] = labelsValues.length > l ? labelsValues[l++] : "";
            self.order["date_"].push(orderValues.length > o ? parseInt(orderValues[o++]) : o++);

        });
        self.order["global"] = [];
        if (params["globalFilter"]) {
            self.defaults["global"] = defValues.length > n ? defValues[n++] : "";
            self.inverse["global"] = inverseValues.length > inv ? inverseValues[inv++] == 'true' : false;
            self.widths["global"] = widthValues.length > w ? widthValues[w++] : "";
            self.labels["global"] = labelsValues.length > l ? labelsValues[l++] : "";
            self.order["global"].push(orderValues.length > o ? parseInt(orderValues[o++]) : o++ + iconfilterParams.length);

        }
        self.order["hideColumns"] = [];
        if (params["hideColumns"]) {
            self.defaults["hideColumns"] = defValues.length > n ? defValues[n++] : "";
            self.inverse["hideColumns"] = inverseValues.length > inv ? inverseValues[inv++] == 'true' : false;
            self.widths["hideColumns"] = widthValues.length > w ? widthValues[w++] : "";
            self.labels["hideColumns"] = labelsValues.length > l ? labelsValues[l++] : "";
            self.order["hideColumns"].push(orderValues.length > o ? parseInt(orderValues[o++]) : o++ + iconfilterParams.length);

        }
        self.order["icon"] = [];
        $.each(iconfilterParams, function(i, param) {
            self.defaults["icon" + param] = defValues.length > n ? defValues[n++] : "";
            self.inverse["icon" + param] = inverseValues.length > inv ? inverseValues[inv++] == 'true' : false;
            self.widths["icon" + param] = widthValues.length > w ? widthValues[w++] : "";
            self.labels["icon" + param] = labelsValues.length > l ? labelsValues[l++] : "";
            self.order["icon"].push(orderValues.length > o ? parseInt(orderValues[o++]) : o++ - self.order["hideColumns"].length - self.order["global"].length);
        });

        self.columnSelect.one('chosen:showing_dropdown', function() {
            self.columnSelect.click();
        });

        var sparkline = $('#macro-param-sparkline');
        var sparkNameInput = $('#macro-param-sparkName');
        sparkNameInput.val('Sparkline');
        var sparkNameContainer = sparkNameInput.parent();
        sparkNameContainer.addClass('tf-always-hidden');

        if (params['sparkline']) {
            sparkline.prop('checked', true);
            sparkNameContainer.removeClass('tf-always-hidden');
            sparkNameInput.val(params['sparkName']);
        }

        sparkline.change(function() {
            if ($(this).prop('checked')) {
                $(".macro-input-fields").animate({
                    scrollTop: $('.macro-input-fields form.aui').height()
                }, 750);
                sparkNameContainer.removeClass('tf-always-hidden');
            } else {
                sparkNameContainer.addClass('tf-always-hidden');
            }
        });

        if (self.isCloud) {
            var limitHeight = $('#macro-param-limitHeight');
            var heightValue = $('#macro-param-heightValue');
            var heightValueContainer = heightValue.parent();
            heightValueContainer.addClass('tf-always-hidden');

            if (params['limitHeight']) {
                limitHeight.prop('checked', true);
                heightValueContainer.removeClass('tf-always-hidden');
                heightValue.val(params['heightValue']);
            }

            limitHeight.change(function() {
                if ($(this).prop('checked')) {
                    $(".macro-input-fields").animate({
                        scrollTop: $('.macro-input-fields form.aui').height()
                    }, 750);
                    heightValueContainer.removeClass('tf-always-hidden');
                } else {
                    heightValueContainer.addClass('tf-always-hidden');
                }
            });
        }

        appendTabs(self.isCloud);
    };

    TablefilterMacro.prototype.createSelect = function(input, tableColumns, params, originalParams) {
        var self = this;

        input.hide();

        $.each(params, function (i, name){
            originalParams.push($.trim(name));
        });

        $.each(params, function (i, elem) {
            params[i] = $.trim(elem.replace(/\‚/g, ','));
        });

        var select = $('<select multiple/>');

        $.each(tableColumns, function(i, name) {
            var selected = $.inArray(name, params) == -1 ? '' : 'selected="selected" ';
            var css = "";
            if (input.is('#macro-param-sort')) {
                var column = name;
                if (column.indexOf('\u200DAsc') != -1) {
                    column = column.replace('\u200DAsc', '\u200DDesc');
                } else {
                    column = column.replace('\u200DDesc', '\u200DAsc');
                }
                if ($.inArray(column, params) != -1) {
                    css = 'style="display: none"';
                }
            }
            select.append($('<option ' + selected + css + '></option>').text(name));
        });

        input.after(select);
        select.chosen({
            no_results_text: "No matches found. Press \u0027Enter\u0027 to add",
            placeholder_text_multiple: "Click or start typing...",
            width: "100%",
            display_selected_options: false,
            addUserOption: true
        }).change(function(e, data) {
            if (data.selected != undefined) {
                select.find('option').filter(function() {
                    return $(this).text() == data.selected;
                }).prop('selected', true);

                var value = data.selected;
                if (value == '') value = ' ';

                originalParams.push(value.replace(/\,/g, '‚'));

                if (input.is('#macro-param-sort')) {
                    var column = data.selected;
                    if (column.indexOf('\u200DAsc') != -1) {
                        column = column.replace('\u200DAsc', '\u200DDesc');
                    } else {
                        column = column.replace('\u200DDesc', '\u200DAsc');
                    }
                    select.find('option').filter(function() {
                        return $(this).text() == column;
                    }).hide();
                    select.trigger('chosen:updated');
                    var choices = select.parent().find('ul.chosen-choices');
                    for (var i = originalParams.length - 1; i >= 0; i--) {
                        choices.prepend(choices.find('li').filter(function() {
                            return $(this).text() == originalParams[i].replace(/\‚/g, ',');
                        }));
                    }
                }
            } else if (data.deselected != undefined) {
                $(this).find('option').filter(function() {
                    return $(this).text() == data.deselected;
                }).prop('selected', false);

                if (data.deselected == '') {
                    originalParams.splice($.inArray(data.deselected, originalParams), 1);
                }
                originalParams.splice($.inArray(data.deselected.replace(/\,/g, '‚'), originalParams), 1);

                if (input.is('#macro-param-sort')) {
                    var column = data.deselected;
                    if (column.indexOf('\u200DAsc') != -1) {
                        column = column.replace('\u200DAsc', '\u200DDesc');
                    } else {
                        column = column.replace('\u200DDesc', '\u200DAsc');
                    }
                    select.find('option').filter(function() {
                        return $(this).text() == column;
                    }).show();
                    select.trigger('chosen:updated');
                    var choices = select.parent().find('ul.chosen-choices');
                    for (var i = originalParams.length - 1; i >= 0; i--) {
                        choices.prepend(choices.find('li').filter(function(n) {
                            return $(this).text() == originalParams[i].replace(/\‚/g, ',');
                        }));
                    }
                }
            }

            input.val(originalParams.join(','));
        });

        var choices = TableUtils.sortSelectedOptions(select, originalParams);

        choices.sortable({
            tolerance: 'pointer',
            cursor: 'move',
            helper: 'clone',
            items: ".search-choice",
            stop: function() {
                originalParams = [];
                choices.find('.search-choice').each(function() {
                    originalParams.push($(this).text().replace(/\,/g, '‚'))
                });
                input.val(originalParams.join(','));
            }
        });

        return select;
    };

    TablefilterMacro.prototype.initInlineDialog = function(macroElem, dialogName) {
        var self = this;
        var input = macroElem.find('input');
        var label = macroElem.find('label');
        input.hide();
        label.hide();

        macroElem.prepend(TableFilterMacro.subtleButton({id: dialogName, name: label.text()}));

        bindHideMacroDialogEvents(dialogName);

        $("#inline-dialog-" + dialogName).remove();

        self.locale = AJS.Meta.get("user-locale").replace('-', '_');

        AJS.InlineDialog($("#button" + dialogName), dialogName,
            function (content, trigger, showPopup) {
                var inputs = [];

                switch (macroElem[0].id) {
                    case 'macro-param-div-cell-width':
                        self.supportOldSize = !/,+?/i.test(input.val());
                        inputs = self.getInputsWithWidth();
                        break;
                    case 'macro-param-div-default':
                        inputs = self.getInputsWithDefValue();
                        break;
                    case 'macro-param-div-inverse':
                        inputs = self.getInputsWithInverseValue();
                        break;
                    case 'macro-param-div-labels':
                        inputs = self.getInputsWithLabels();
                        break;
                }

                content.html(TableFilterMacro.inlineDialog({'inputs': inputs, 'idDialog': dialogName}));
                initChosen(content);

                if (content.datePicker) {
                    if (AJS.DatePicker.prototype.localisations.monthNames) {
                        AJS.DatePicker.prototype.localisations = TableUtils.localisations[self.locale];
                    } else {
                        AJS.DatePicker.prototype.localisations[self.locale] = TableUtils.localisations[self.locale];
                    }
                    content.find('.date').each(function() {
                        $(this).datePicker({overrideBrowserDefault: true, dateFormat: self.dateFormatInput.val() || $.datepicker.ATOM, languageCode: self.locale});
                    });
                } else {
                    var options = TableUtils.localisations[self.locale];
                    options.dateFormat = self.dateFormatInput.val() || $.datepicker.ATOM;
                    content.find('.date').datepicker(options);
                    $('#ui-datepicker-div').click(function(e) {e.stopPropagation()});
                }
                showPopup();
                return false;
            }, {
                hideDelay: null,
                initCallback: function () {
                    var popup = this.popup;
                    if ("CellWidthDialog" == dialogName) {
                        $("#inline-dialog-DefaultValDialog").hide();
                        $("#inline-dialog-InverseFilterDialog").hide();
                        $("#inline-dialog-FilterLabelsDialog").hide();
                        $("#buttonDefaultValDialog").removeClass('active');
                        $("#buttonInverseFilterDialog").removeClass('active');
                        $("#buttonFilterLabelsDialog").removeClass('active');
                    } else if ("DefaultValDialog" == dialogName) {
                        $("#inline-dialog-CellWidthDialog").hide();
                        $("#inline-dialog-InverseFilterDialog").hide();
                        $("#inline-dialog-FilterLabelsDialog").hide();
                        $("#buttonCellWidthDialog").removeClass('active');
                        $("#buttonInverseFilterDialog").removeClass('active');
                        $("#buttonFilterLabelsDialog").removeClass('active');
                    } else if ("InverseFilterDialog") {
                        $("#inline-dialog-CellWidthDialog").hide();
                        $("#inline-dialog-DefaultValDialog").hide();
                        $("#inline-dialog-FilterLabelsDialog").hide();
                        $("#buttonCellWidthDialog").removeClass('active');
                        $("#buttonDefaultValDialog").removeClass('active');
                        $("#buttonFilterLabelsDialog").removeClass('active');
                    } else {
                        $("#inline-dialog-CellWidthDialog").hide();
                        $("#inline-dialog-DefaultValDialog").hide();
                        $("#inline-dialog-InverseFilterDialog").hide();
                        $("#buttonCellWidthDialog").removeClass('active');
                        $("#buttonDefaultValDialog").removeClass('active');
                        $("#buttonInverseFilterDialog").removeClass('active');
                    }

                    popup.css('z-index', '5000');
                    popup.find("#save" + dialogName).click(function () {
                        var form = popup.find("#" + dialogName);
                        if ("CellWidthDialog" == dialogName) {
                            getFormValue(form, {values: self.widths}, false);
                        } else if ("DefaultValDialog" == dialogName) {
                            getFormValue(form, {values: self.defaults, locale: self.locale, dateFormat: self.dateFormatInput.val()}, false);
                        } else if ("InverseFilterDialog" == dialogName) {
                            getFormValue(form, {values: self.inverse}, false);
                        } else {
                            getFormValue(form, {values: self.labels}, true);
                        }
                        popup.hide();
                    });

                    popup.find("#cancel" + dialogName).click(function () {
                        popup.hide();
                    });

                    popup.find('.simple-input').keydown(function(e) {
                        if (e.keyCode == 13) {
                            popup.find("#save" + dialogName).click();
                            return false;
                        }
                    });

                    $('.tf-macrobrowser-tabs #tab-filters, .tf-macrobrowser-tabs #tab-table-options').click(function() {
                        popup.hide();
                        return false;
                    });

                    $("#button" + dialogName).click(function() {
                        if ($(this).hasClass('active')) {
                            popup.hide();
                            return false;
                        }
                    });
                },
                gravity: 'w',
                persistent: true
            }
        );
    };

    TablefilterMacro.prototype.getInputsWithWidth = function() {
        var self = this;
        var inputs = [];
        var index = 0;

        $('.macro-input-fields .macro-param-div:not(#macro-param-div-datefilter, #macro-param-div-numberfilter, #macro-param-div-sort) .chosen-choices .search-choice').each(function (i) {
            var field = $(this);
            var isText = field.parents('#macro-param-div-userfilter').length > 0;
            var isIcon = field.parents('#macro-param-div-iconfilter').length > 0;
            var type = isIcon ? "icon" : isText ? "freetext_" : "select_";
            var value = self.widths[type + field.text()] || '';
            inputs.push({'field': field.text(), 'value': value, 'type': type, 'isIcon': isIcon});
            index = i;
        });

        if ($('#macro-param-globalFilter').prop('checked')) {
            var globalFilterName = "Global filter";
            inputs.push({'field': globalFilterName, 'value': self.widths["global"] || '', 'type': "global"});
        }

        if ($('#macro-param-hideColumns').prop('checked')) {
            var hideColumnsName = "Hide columns";
            inputs.push({'field': hideColumnsName, 'value': self.widths["hideColumns"] || '', 'type': "hideColumns"});
        }

        self.supportOldSize = false;
        return inputs;
    };

    TablefilterMacro.prototype.getInputsWithLabels = function() {
        var self = this;
        var inputs = [];
        var index = 0;

        $('.macro-input-fields .macro-param-div:not(#macro-param-div-sort) .chosen-choices .search-choice').each(function (i) {
            var field = $(this);
            var isDate = field.parents('#macro-param-div-datefilter').length > 0;
            var isNumber = field.parents('#macro-param-div-numberfilter').length > 0;
            var isText = field.parents('#macro-param-div-userfilter').length > 0;
            var isIcon = field.parents('#macro-param-div-iconfilter').length > 0;
            var type = isIcon ? "icon" : isDate ? "date_" : isNumber ? "number_" : isText ? "freetext_" : "select_";
            var value = self.labels[type + field.text()] || field.text();
            inputs.push({'field': field.text(), 'value': value, 'type': type, 'isIcon': isIcon});
            index = i;
        });

        if ($('#macro-param-globalFilter').prop('checked')) {
            var globalFilterName = "Global filter";
            inputs.push({'field': globalFilterName, 'value': self.labels["global"] || globalFilterName, 'type': "global"});
        }

        if ($('#macro-param-hideColumns').prop('checked')) {
            var hideColumnsName = "Hide columns";
            inputs.push({'field': hideColumnsName, 'value': self.labels["hideColumns"] || hideColumnsName, 'type': "hideColumns"});
        }

        return inputs;
    };

    TablefilterMacro.prototype.getInputsWithDefValue = function(self) {
        if (typeof self == 'undefined') {
            var self = this;
        }
        var inputs = [];

        $('div.macro-param-div:not(#macro-param-div-sort) .chosen-choices .search-choice').each(function () {
            var field = $(this);
            var valueTo = '';
            var isSelect = field.parents('#macro-param-div-column').length > 0;
            var isDate = field.parents('#macro-param-div-datefilter').length > 0;
            var isNumber = field.parents('#macro-param-div-numberfilter').length > 0;
            var isText = field.parents('#macro-param-div-userfilter').length > 0;
            var isIcon = field.parents('#macro-param-div-iconfilter').length > 0;
            var type = isIcon ? "icon" : isDate ? "date_" : isNumber ? "number_" : isText ? "freetext_" : "select_";
            var selects = undefined;
            if (isSelect || isIcon) {
                selects = self.getSelectFilterOptions(field, isIcon, self);
            }

            var value = self.defaults[type + field.text()] || '';
            if ((isDate || isNumber) && value) {
                var values = value.split('->');
                value = values[0];
                valueTo = values[1];
                if (isDate) {
                    value = TableUtils.convertToDate(value, self.dateFormatInput.val(), self.locale);
                    valueTo = TableUtils.convertToDate(valueTo, self.dateFormatInput.val(), self.locale);
                }
            }

            inputs.push({
                'field': field.text(),
                'value': value,
                'valueTo': valueTo,
                'selects': selects,
                'fakeSelects': [],
                isDate: isDate,
                isNumber: isNumber,
                isIcon: isIcon,
                type: type
            });
        });

        if ($('#macro-param-globalFilter').prop('checked')) {
            var globalFilterName = "Global filter";
            inputs.push({'field': globalFilterName, 'value': self.defaults["global"] || '', type: "global"});
        }

        if ($('#macro-param-hideColumns').prop('checked')) {
            var hideColumnsName = "Columns to hide";
            inputs.push({'field': hideColumnsName, 'value': self.defaults["hideColumns"] || '', selects: self.tableColumns, 'fakeSelects': [], type: "hideColumns"});
        }

        $.each(inputs, function (i, input) {
            if (this.selects) {
                this.value = this.value.split('&&');
                var selects = this.selects;

                $.each(selects, function(i, select) {
                    if (select == '') {
                        selects[i] = "-Empty-";
                    }
                });

                $.each(this.value, function (i, elem) {
                    if (elem != '' && $.inArray(elem, selects) == -1) {
                        input.fakeSelects.push(elem);
                    }
                });
            }
        });

        return inputs;
    };

    TablefilterMacro.prototype.getInputsWithInverseValue = function() {
        var self = this;
        var inputs = [];

        $('div.macro-param-div:not(#macro-param-div-sort) .chosen-choices .search-choice').each(function () {
            var field = $(this);
            var isDate = field.parents('#macro-param-div-datefilter').length > 0;
            var isNumber = field.parents('#macro-param-div-numberfilter').length > 0;
            var isText = field.parents('#macro-param-div-userfilter').length > 0;
            var isIcon = field.parents('#macro-param-div-iconfilter').length > 0;
            var type = isIcon ? "icon" : isDate ? "date_" : isNumber ? "number_" : isText ? "freetext_" : "select_";

            var value = self.inverse[type + field.text()] || false;

            inputs.push({
                'field': field.text(),
                'value': value,
                type: type,
                isIcon: isIcon,
                inverseType: true
            });
        });

        if ($('#macro-param-globalFilter').prop('checked')) {
            var globalFilterName = "Global filter";
            inputs.push({'field': globalFilterName, 'value': self.inverse["global"] || false, type: "global", inverseType: true});
        }

        if ($('#macro-param-hideColumns').prop('checked')) {
            var hideColumnsName = "Columns to hide";
            inputs.push({'field': hideColumnsName, 'value': self.inverse["hideColumns"] || false, type: "hideColumns", inverseType: true});
        }

        return inputs;
    };

    TablefilterMacro.prototype.getSelectFilterOptions = function(filter, isIcon) {
        var self = this;
        var title = filter.text();
        var allSelects = [];
        var lookForCheckboxes = true;

        $.each(self.tables, function (n, table) {
            $.each(table[0], function (i, td) {
                if (title.toLocaleLowerCase() == td.toLowerCase()) {
                    var tableData = isIcon ? self.iconTables[n] : table;
                    $.each(tableData, function (j, elem) {
                        if (j != 0) {
                            if (isIcon && $(elem[i]).is('ul')) {
                                if (lookForCheckboxes) {
                                    lookForCheckboxes = false;
                                    if ($.inArray('<ul class="inline-task-list"><li></li></ul>', allSelects) == -1) {
                                        allSelects.push('<ul class="inline-task-list"><li></li></ul>');
                                    }
                                    if ($.inArray('<ul class="inline-task-list"><li class="checked"></li></ul>', allSelects) == -1) {
                                        allSelects.push('<ul class="inline-task-list"><li class="checked"></li></ul>');
                                    }
                                }
                            } else if (elem[i] != undefined && $.inArray(elem[i].replace(/\,/g, '‚'), allSelects) == -1) {
                                allSelects.push(elem[i].replace(/\,/g, '‚'));
                            }
                        }
                    });

                    if (!isIcon && $.inArray("@currentUser", allSelects) == -1) {
                        var tdNumber = i + 1;
                        if (self.body.find('table').eq(n).find('tr td:nth-child(' + tdNumber + '), tr th:nth-child(' + tdNumber + ')').find('.confluence-userlink, ri\\:user').length) {
                            allSelects.push("@currentUser");
                        }
                    }
                }
            });
        });

        return allSelects;
    };

    TablefilterMacro.prototype.updateColumns = function(body) {
        var self = this;
        self.body = body;
        var hasNew = false;
        var hasNewSort = false;

        var updateColumns = function() {
            TableUtils.normalizeTable(body);
            self.tables = TableUtils.getTables(body, '.tablefilter-table-wrapper');
            self.iconTables = TableUtils.getTables(body, '.tablefilter-table-wrapper', true);

            $.each(self.tables, function(i, table) {
                $.each(table[0], function(i, cell) {
                    if ($.inArray(cell, self.tableColumns) == -1) {
                        hasNew = true;
                        self.tableColumns.push(cell);
                        self.columnSelect.append($('<option></option>').val(cell).text(cell));
                        self.userfilterSelect.append($('<option></option>').val(cell).text(cell));
                        self.datefilterSelect.append($('<option></option>').val(cell).text(cell));
                        self.numberfilterSelect.append($('<option></option>').val(cell).text(cell));
                        self.iconfilterSelect.append($('<option></option>').val(cell).text(cell));
                    }
                    if ($.trim(cell).length) {
                        if ($.inArray(cell + ' (\u200DAsc)', self.sortColumns) == -1) {
                            hasNewSort = true;
                            self.sortColumns.push(cell + " (\u200DAsc)");
                            var css = '';
                            if ($('#macro-param-sort').val().indexOf(cell + ' (\u200DDesc)') != -1) {
                                css = ' style="display:none"';
                            }
                            self.sortSelect.append($('<option' + css + '></option>').val(cell + ' (\u200DAsc)').text(cell + ' (\u200DAsc)'));
                        }
                        if ($.inArray(cell + ' (\u200DDesc)', self.sortColumns) == -1) {
                            hasNewSort = true;
                            self.sortColumns.push(cell + " (\u200DDesc)");
                            var css = '';
                            if ($('#macro-param-sort').val().indexOf(cell + ' (\u200DAsc)') != -1) {
                                css = ' style="display:none"';
                            }
                            self.sortSelect.append($('<option' + css + '></option>').val(cell + ' (\u200DDesc)').text(cell + ' (\u200DDesc)'));
                        }
                    }
                });
            });

            if (hasNew) {
                self.columnSelect.trigger('chosen:updated');
                self.userfilterSelect.trigger('chosen:updated');
                self.datefilterSelect.trigger('chosen:updated');
                self.numberfilterSelect.trigger('chosen:updated');
                self.iconfilterSelect.trigger('chosen:updated');
                TableUtils.sortSelectedOptions(self.columnSelect, self.columnParams);
                TableUtils.sortSelectedOptions(self.userfilterSelect, self.userfilterParams);
                TableUtils.sortSelectedOptions(self.datefilterSelect, self.datefilterParams);
                TableUtils.sortSelectedOptions(self.numberfilterSelect, self.numberfilterParams);
                TableUtils.sortSelectedOptions(self.iconfilterSelect, self.iconfilterParams);
            }
            if (hasNewSort) {
                self.sortSelect.trigger('chosen:updated');
                TableUtils.sortSelectedOptions(self.sortSelect, self.sortParams);
            }
        };

        self.body.find('.tablefilter-table-wrapper').bind('DOMNodeInserted', function(e) {
            var target = $(e.target);
            if (e.relatedNode.id == this.id || target.hasClass("aui-nav-pagination") ||
                target.hasClass("user-profile-list-result") || target.attr('id') == "teamcity") {
                updateColumns();
            }
        });

        updateColumns();
    };

    TablefilterMacro.prototype.getComputationalParams = function(params) {
        var self = this;
        var columnParams = params["column"] != undefined ? params["column"].split(',') : [];
        var userfilterParams = params["userfilter"] != undefined ? params["userfilter"].split(',') : [];
        var numberfilterParams = params["numberfilter"] != undefined ? params["numberfilter"].split(',') : [];
        var datefilterParams = params["datefilter"] != undefined ? params["datefilter"].split(',') : [];
        var iconfilterParams = params["iconfilter"] != undefined ? params["iconfilter"].split(',') : [];
        var defValues = [];
        var widthValues = [];
        var inverseValues = [];
        var labelsValues = [];
        var orders = [];
        var orderIncrement = {};
        var added = 0;

        added = processFilterOptions(self, columnParams, "select_");
        added = processFilterOptions(self, userfilterParams, "freetext_");
        added = processFilterOptions(self, numberfilterParams, "number_");
        added = processFilterOptions(self, datefilterParams, "date_");
        if (params["globalFilter"]) {
            added = processFilterOptions(self, [""], "global");
        }
        if (params["hideColumns"]) {
            added = processFilterOptions(self, [""], "hideColumns");
        }
        var globalAndHideLength = (params["globalFilter"] ? 1 : 0) + (params["hideColumns"] ? 1 : 0);
        processFilterOptions(self, iconfilterParams, "icon", globalAndHideLength);

        var sortedOrders = orders.slice().sort(function(a, b) { return a - b;});
        sortedOrders.forEach(function(n, i) {
            orders[$.inArray(n, orders)] = i;
        });

        params["default"] = defValues.join(",");
        params["cell-width"] = widthValues.join(",");
        params["inverse"] = inverseValues.join(",");
        params["labels"] = labelsValues.join("‚");
        params["order"] = orders.join(",");

        return params;

        function processFilterOptions(self, params, type, globalAndHideLength) {
            $.each(params, function(i, param) {
                defValues.push(self.defaults[type + $.trim(param.replace(/\‚/g, ','))]);
                if (type != "number_" && type != "date_")
                    widthValues.push(self.widths[type + $.trim(param.replace(/\‚/g, ','))]);
                inverseValues.push(self.inverse[type + $.trim(param.replace(/\‚/g, ','))]);
                labelsValues.push(self.labels[type + $.trim(param.replace(/\‚/g, ','))]);
                var order = self.order[type][i];
                if (order == undefined) {
                    if (orders.length == 0) {
                        order = 0;
                    } else {
                        added = 0;
                        var prev = orders.length - 1;
                        if (type == 'icon' && i == 0) {
                            prev -= globalAndHideLength;
                        }
                        order = prev >= 0 ? orders[prev] + 1 : 0;
                    }
                    var rootOrder = order - added;
                    var tmpRootOrdr = rootOrder;
                    $.each(orderIncrement, function(i, n) {
                        if (tmpRootOrdr > i) {
                            rootOrder -= n;
                        }
                    });

                    orderIncrement[rootOrder] ? orderIncrement[rootOrder]++ : orderIncrement[rootOrder] = 1;
                    orders.forEach(function(o, i) {
                        if (o >= order - added && i < orders.length - added) {
                            orders[i] += 1;
                        }
                    });
                    added++;
                } else {
                    added = 0;
                    for (var j = order; j >= 0; j--) {
                        if (orderIncrement[j]) {
                            order += orderIncrement[j];
                        }
                    }
                }
                orders.push(order);
            });

            return added;
        }
    };

    function initChosen(content) {
        content.find('option:contains(' + "-Empty-" + ')').addClass('tf-empty-option');

        content.find("select").each(function() {
            var select = $(this);
            select.chosen({
                no_results_text: "No matches found. Press \u0027Enter\u0027 to add",
                placeholder_text_multiple: select.data('type') == 'icon' ? "Select items..." : "Click or start typing...",
                display_selected_options: false,
                width: "250px",
                addUserOption: true,
                search_contains: true,
                isHtml: select.data('html')
            }).change(function (e, data) {
                    if (data.selected) {
                        $(this).find("option[value='" + data.selected + "']").prop('selected', true);
                    } else if (data.deselected) {
                        $(this).find("option[value='" + data.deselected + "']").prop('selected', false);
                    }
                    content.find('.search-choice:contains(' + "-Empty-" + ')').addClass('tf-empty-option');
                });
        });

        content.find('.search-choice:contains(' + "-Empty-" + ')').addClass('tf-empty-option');
    }

    function getFormValue(form, storage, newSeparator) {
        form.find('div.field-group').find('select, .simple-input, .range, .checkbox').each(function () {
            var field = $(this);
            var type = field.data('type');
            var name = type == "global" || type == "hideColumns" ? "" : field.prev().text();

            if (field.hasClass('checkbox')) {
                storage.values[type + name] = field.is(':checked');
            } else if (field.hasClass('simple-input')) {
                storage.values[type + name] = newSeparator ? field.val() : field.val().replace(/\,/g, '‚');
            } else if (field.hasClass('range')) {
                var from = field.find('.from').val();
                var to = field.find('.to').val();
                if (storage.locale && field.hasClass('dates')) {
                    try {
                        var date = TableUtils.parseDate(from, storage.dateFormat, storage.locale);
                        var timestamp = date.getTime() - date.getTimezoneOffset() * 60000;
                        from = timestamp + '';
                        date = TableUtils.parseDate(to, storage.dateFormat, storage.locale);
                        timestamp = date.getTime() - date.getTimezoneOffset() * 60000;
                        to = timestamp + '';
                    } catch (e) {}
                }
                storage.values[type + name] = (from + "->" + to).replace(/\,/g, '‚');
            } else {
                var temp = [];
                field.find('option:selected').each(function () {
                    temp.push($(this).text().replace(/\,/g, '‚'));
                });
                storage.values[type + name] = temp.join('&&');
            }
        });
    }

    function bindHideMacroDialogEvents(uniqueDialogName) {
        $('#macro-insert-container .macro-input-fields').scroll(function () {
            $("#inline-dialog-" + uniqueDialogName).hide();
        });

        var ev = 'hide';
        var el = $.fn[ev];
        $.fn[ev] = function () {
            if (this.attr('id') == 'macro-browser-dialog') {
                $("#inline-dialog-" + uniqueDialogName).hide();
            }
            return el.apply(this, arguments);
        };
    }

    function setSavedParams(params) {
        if (params['globalFilter']) {
            $('#macro-param-globalFilter').prop('checked', true);
        }

        if (params['hideColumns']) {
            $('#macro-param-hideColumns').prop('checked', true);
        }

        if (params['hidePane']) {
            $('#macro-param-hidePane').prop('checked', true);
        }

        if (params['hidelabels']) {
            $('#macro-param-hidelabels').prop('checked', true);
        }

        if (params['hideControls']) {
            $('#macro-param-hideControls').prop('checked', true);
        }

        if (params['disableSave']) {
            $('#macro-param-disableSave').prop('checked', true);
        }

        if (params['separator']) {
            $('#macro-param-separator').val(params['separator']);
        }

        if (params['showNRows'] && params['showNRows'] != 'all') {
            $('#macro-param-showNRows').val(params['showNRows']);
        }

        if (params['numbering']) {
            $('#macro-param-numbering').val(params['numbering']);
        }

        if (params['limitHeight']) {
            $('#macro-param-limitHeight').prop('checked', true);
        }

        if (params['ddSeparator']) {
            $('#macro-param-ddSeparator').val(params['ddSeparator']);
        }

        if (params['isOR']) {
            $('#macro-param-isOR').val(params['isOR']);
        }
    }

    function appendTabs(isCloud) {
        $('#macro-param-div-column').before(TableFilterTemplate.macrobrowserTabs());

        var filters = [$('#macro-param-div-column'), $('#macro-param-div-userfilter'), $('#macro-param-div-numberfilter'),
            $('#macro-param-div-datefilter'), $('#macro-param-div-iconfilter'), $('#macro-param-div-globalFilter'),
            $('#macro-param-div-hideColumns')];
        var filterOptions = [$('#macro-param-div-separator'), $('#macro-param-div-datepattern'), $('#macro-param-div-default'),
            $('#macro-param-div-cell-width'), $('#macro-param-div-inverse'), $('#macro-param-div-disableSave'),
            $('#macro-param-div-hidelabels'), $('#macro-param-div-hidePane'), $('#macro-param-div-hideControls'),
            $('#macro-param-div-isOR'), $('#macro-param-div-ddSeparator'), $('#macro-param-div-labels')];
        var tableOptions = [$('#macro-param-div-showNRows'), $('#macro-param-div-numbering'), $('#macro-param-div-sort'),
            $('#macro-param-div-limitHeight'), $('#macro-param-div-sparkline'), $('#macro-param-div-sparkName')];

        if (isCloud) {
            tableOptions.push($('#macro-param-div-heightValue'));
        }

        $('.tf-macrobrowser-tabs a').attr('disabled', 'disabled');

        var filtersTab = $('#tab-filters');
        var filterOptionsTab = $('#tab-filter-options');
        var tableOptionsTab = $('#tab-table-options');
        var tabs = [filtersTab, filterOptionsTab, tableOptionsTab];

        filtersTab.click(function() {
            setActive(filtersTab);
            fadeElements([filters, filterOptions, tableOptions]);
        });

        filterOptionsTab.click(function() {
            setActive(filterOptionsTab);
            fadeElements([filterOptions, filters, tableOptions]);
        });

        tableOptionsTab.click(function() {
            setActive(tableOptionsTab);
            fadeElements([tableOptions, filters, filterOptions]);
        });

        filtersTab.trigger('click');

        function fadeElements(elements) {
            fade(elements[0], 'in');
            for (var i = 1; i < elements.length; i++) {
                fade(elements[i], 'out');
            }

            function fade(elements, state) {
                for (var i = 0; i < elements.length; i++) {
                    if (state == 'in') {
                        elements[i].show();
                    } else {
                        elements[i].hide();
                    }
                }
            }
        }

        function setActive(tab) {
            if (!tab.hasClass('active-tab')) {
                tabs.forEach(function(_tab) {
                    _tab.removeClass('active-tab');
                });
                tab.addClass('active-tab');
            }
        }
    }
})(jQuery);
}catch(e){WRMCB(e)};
;
