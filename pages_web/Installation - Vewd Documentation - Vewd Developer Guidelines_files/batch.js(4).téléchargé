TableFilterMacro.inlineDialog = function(opt_data, opt_ignored) {
  var output = '<form class="aui top-label inline-dialog-form" id="' + soy.$$escapeHtml(opt_data.idDialog) + '">';
  if (opt_data.inputs != '') {
    output += '<fieldset>';
    var inputList15 = opt_data.inputs;
    var inputListLen15 = inputList15.length;
    for (var inputIndex15 = 0; inputIndex15 < inputListLen15; inputIndex15++) {
      var inputData15 = inputList15[inputIndex15];
      output += '<div class="field-group"><label for="input-' + soy.$$escapeHtml(inputData15.field) + '-' + soy.$$escapeHtml(inputIndex15) + '" ' + ((inputData15.inverseType) ? 'class="inverse-label"' : '') + '>' + soy.$$escapeHtml(inputData15.field) + '</label>';
      if (inputData15.inverseType) {
        output += '<input class="checkbox inverse-input" type="checkbox" name="' + soy.$$escapeHtml(inputData15.field) + '-' + soy.$$escapeHtml(inputIndex15) + '" data-type="' + soy.$$escapeHtml(inputData15.type) + '" ' + ((inputData15.value) ? 'checked="checked"' : '') + '>';
      } else if (inputData15.selects) {
        output += '<select class="select" name="' + soy.$$escapeHtml(inputData15.field) + '-' + soy.$$escapeHtml(inputIndex15) + '" data-type="' + soy.$$escapeHtml(inputData15.type) + '" style="width: 150px" multiple ' + ((inputData15.isIcon) ? 'data-html="true"' : '') + '>';
        var selectList52 = inputData15.selects;
        var selectListLen52 = selectList52.length;
        for (var selectIndex52 = 0; selectIndex52 < selectListLen52; selectIndex52++) {
          var selectData52 = selectList52[selectIndex52];
          output += '<option value="' + soy.$$escapeHtml(selectData52) + '"';
          var valueList56 = inputData15.value;
          var valueListLen56 = valueList56.length;
          for (var valueIndex56 = 0; valueIndex56 < valueListLen56; valueIndex56++) {
            var valueData56 = valueList56[valueIndex56];
            output += (valueData56 == selectData52) ? ' selected="selected" ' : '';
          }
          output += '>' + soy.$$escapeHtml(selectData52) + '</option>';
        }
        var selectList65 = inputData15.fakeSelects;
        var selectListLen65 = selectList65.length;
        for (var selectIndex65 = 0; selectIndex65 < selectListLen65; selectIndex65++) {
          var selectData65 = selectList65[selectIndex65];
          output += '<option value="' + soy.$$escapeHtml(selectData65) + '" selected="selected">' + soy.$$escapeHtml(selectData65) + '</option>';
        }
        output += '</select>';
      } else if (inputData15.isDate || inputData15.isNumber) {
        var class__soy74 = '' + ((inputData15.isDate) ? 'date' : 'number');
        output += '<div class="' + soy.$$escapeHtml(class__soy74) + 's range" data-type="' + soy.$$escapeHtml(inputData15.type) + '">' + soy.$$escapeHtml("from") + '<input type="text" class="text ' + soy.$$escapeHtml(class__soy74) + ' from" name="' + soy.$$escapeHtml(inputData15.field) + '-from-' + soy.$$escapeHtml(inputIndex15) + '" id="input-' + soy.$$escapeHtml(inputData15.field) + '-from-' + soy.$$escapeHtml(inputIndex15) + '" ' + ((inputData15.value) ? ' value="' + soy.$$escapeHtml(inputData15.value) + '" ' : '') + ((inputData15.isDate) ? ' placeholder="' + soy.$$escapeHtml("Pick date") + '" ' : '') + '/>' + soy.$$escapeHtml("to") + '<input type="text" class="text ' + soy.$$escapeHtml(class__soy74) + ' to" name="' + soy.$$escapeHtml(inputData15.field) + '-to-' + soy.$$escapeHtml(inputIndex15) + '" id="input-' + soy.$$escapeHtml(inputData15.field) + '-to-' + soy.$$escapeHtml(inputIndex15) + '" ' + ((inputData15.valueTo) ? ' value="' + soy.$$escapeHtml(inputData15.valueTo) + '" ' : '') + ((inputData15.isDate) ? ' placeholder="' + soy.$$escapeHtml("Pick date") + '" ' : '') + '/></div>';
      } else {
        output += '<input type="text" class="text simple-input" name="' + soy.$$escapeHtml(inputData15.field) + '-' + soy.$$escapeHtml(inputIndex15) + '" id="input-' + soy.$$escapeHtml(inputData15.field) + '-' + soy.$$escapeHtml(inputIndex15) + '" ' + ((inputData15.value) ? ' value="' + soy.$$escapeHtml(inputData15.value) + '" ' : '') + ' data-type="' + soy.$$escapeHtml(inputData15.type) + '"/>';
      }
      output += '</div>';
    }
    output += '</fieldset><div class="buttons-container"><div class="buttons"><input class="aui-button" type="button" value="' + soy.$$escapeHtml("Save") + '" id="save' + soy.$$escapeHtml(opt_data.idDialog) + '"/><a class="cancel" id="cancel' + soy.$$escapeHtml(opt_data.idDialog) + '">' + soy.$$escapeHtml("Cancel") + '</a></div></div>';
  } else {
    output += '<div class="aui-message info"><span class="aui-icon icon-info"></span>' + soy.$$escapeHtml("Filters are not specified") + '</div><div class="buttons-container"><div class="buttons"><a class="cancel" id="cancel' + soy.$$escapeHtml(opt_data.idDialog) + '">' + soy.$$escapeHtml("Cancel") + '</a></div></div>';
  }
  output += '</form>';
  return output;
};
