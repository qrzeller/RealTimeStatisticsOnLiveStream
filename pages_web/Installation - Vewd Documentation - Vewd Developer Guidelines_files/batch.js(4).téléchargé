ChartTemplate.view = function(opt_data, opt_ignored) {
  return ((opt_data.hidecontrols) ? '<style type="text/css">#chart' + soy.$$escapeHtml(opt_data.macroId) + ' .chart-controls {display: none;}</style>' : '') + '<div id="chart' + soy.$$escapeHtml(opt_data.macroId) + '" class="table-chart-contents" data-id="' + soy.$$escapeHtml(opt_data.macroId) + '" data-hidden="' + soy.$$escapeHtml(opt_data.hide) + '" data-pageid="' + soy.$$escapeHtml(opt_data.pageId) + '">' + ((! opt_data.isEmail) ? '<div style="display: none"><canvas></canvas><div id="pngdataurl"></div></div><div class="chart-wrapper" style="text-align: ' + soy.$$escapeHtml(opt_data.align) + '"><div class="tf-chart-message no-table-message" style="display: none;"><div class="aui-message warning"><p class="title"><span class="aui-icon icon-warning"></span></p>' + soy.$$escapeHtml("Probably, there is no table within the Chart from Table macro. Please place the table or the macro generating a table within it. Once this is done, configure parameters of chart generation.") + '</div></div><div class="tf-chart-message no-data-message" style="display: none;"><div class="aui-message warning"><p class="title"><span class="aui-icon icon-warning"></span></p>' + soy.$$escapeHtml("Probably, there is no data in the Values column. Please, select the column with data values for chart generation.") + '</div></div><div class="tf-chart-message wrong-date-message" style="display: none;"><div class="aui-message warning"><p class="title"><span class="aui-icon icon-warning"></span></p>' + soy.$$escapeHtml("Probably, there is wrong date format specified. Please, check the date format in macro parameter.") + '</div></div><div class="tf-chart-message bad-settings-message" style="display: none;"><div class="aui-message warning"><p class="title"><span class="aui-icon icon-warning"></span></p>' + soy.$$escapeHtml("Please select the column with labels and the column with data values for chart generation.") + '</div></div><div class="tf-chart-message log-zero-message" style="display: none;"><div class="aui-message warning"><p class="title"><span class="aui-icon icon-warning"></span></p>' + soy.$$escapeHtml("Logarithmic charts can not contain zero values.") + '</div></div><span id="chart-loading"></span><div class="chart-controls"><form class="aui" name="inlinecommentform" onsubmit="return false;"><div class="chart-menu-buttons aui-toolbar2" role="toolbar"><div id="chart-menu-row-' + soy.$$escapeHtml(opt_data.macroId) + '" class="aui-toolbar2-inner"><div class="aui-toolbar2-primary" style="float:right;"><button class="chart-settings aui-button aui-dropdown2-trigger" aria-owns="chart-menu-view-' + soy.$$escapeHtml(opt_data.macroId) + '" aria-haspopup="true" data-container="" resolved="" aria-expanded="false" href="#"><span class="aui-icon aui-icon-small aui-iconfont-configure"></span></button><div id="chart-menu-view-' + soy.$$escapeHtml(opt_data.macroId) + '" class="aui-style-default aui-dropdown2" style="display: none;"><div class="aui-dropdown2-section"><ul class="aui-list-truncate">' + ((opt_data.editable) ? '<li><a class="chart-save tf-disabled-link cursor-pointer">' + soy.$$escapeHtml("Save changes") + '</a></li>' : '') + '<li><a class="save-as-png" download="Chart.png" href="">' + soy.$$escapeHtml("Chart as image") + '</a></li><li><a class="export-file-pdf cursor-pointer">' + soy.$$escapeHtml("Export to PDF") + '</a></li><li><a class="export-file-doc cursor-pointer">' + soy.$$escapeHtml("Export to Word") + '</a></li>' + ((opt_data.editable) ? '<li><a class="chart-change-color cursor-pointer">' + soy.$$escapeHtml("Select colors") + '</a></li>' : '') + '<li ' + ((! opt_data.hide) ? 'style="display: none;"' : '') + '><a class="chart-showrawdata cursor-pointer">' + soy.$$escapeHtml("Show source table") + '</a></li><li ' + ((opt_data.hide) ? 'style="display: none;"' : '') + '><a class="chart-hiderawdata cursor-pointer">' + soy.$$escapeHtml("Hide source table") + '</a></li></ul></div><div class="aui-dropdown2-section"><ul class="aui-list-truncate"><li><a target="_blank" href="https://docs.stiltsoft.com/display/TableFilter/How+to+use+Chart+from+Table+macro?from=tf-view">' + soy.$$escapeHtml("Documentation") + '</a></li><li><a target="_blank" href="https://docs.stiltsoft.com/display/TableFilter/Table+Filter+and+Charts+3.9.0?from=tf-view">' + soy.$$escapeHtml("What\x27s new") + '</a></li></ul></div></div></div></div></div><div><label for="chart-column" class="simple-text">' + soy.$$escapeHtml("Labels column") + '</label><label for="chart-column" class="time-text" style="display: none;">' + soy.$$escapeHtml("Dates column") + '</label><label for="chart-column" class="series-text" style="display: none;">' + soy.$$escapeHtml("X-axis value column") + '</label><select class="select chart-column" name="chart-column" multiple></select></div><div><label for="chart-aggregation" class="simple-text">' + soy.$$escapeHtml("Values column") + '</label><label for="chart-aggregation" class="time-text" style="display: none;">' + soy.$$escapeHtml("Values column") + '</label><label for="chart-aggregation" class="series-text" style="display: none;">' + soy.$$escapeHtml("Y-axis value column") + '</label><select class="select chart-aggregation" name="chart-aggregation" multiple></select></div><div><label for="chart-type">' + soy.$$escapeHtml("Type") + '</label><select class="select chart-type" name="chart-type"><option value="Pie" ' + ((opt_data.type == 'Pie') ? 'selected="selected"' : '') + '>Pie</option><option value="Donut" ' + ((opt_data.type == 'Donut') ? 'selected="selected"' : '') + '>Donut</option><option value="3D Donut" ' + ((opt_data.type == '3D Donut') ? 'selected="selected"' : '') + '>3D Donut</option><option value="Column" ' + ((opt_data.type == 'Column') ? 'selected="selected"' : '') + '>Column</option><option value="Stacked Column" ' + ((opt_data.type == 'Stacked Column') ? 'selected="selected"' : '') + '>Stacked Column</option><option value="Bar" ' + ((opt_data.type == 'Bar') ? 'selected="selected"' : '') + '>Bar</option><option value="Stacked Bar" ' + ((opt_data.type == 'Stacked Bar') ? 'selected="selected"' : '') + '>Stacked Bar</option><option value="Line" ' + ((opt_data.type == 'Line') ? 'selected="selected"' : '') + '>Line</option><option value="Area" ' + ((opt_data.type == 'Area') ? 'selected="selected"' : '') + '>Area</option><option value="Time Line" ' + ((opt_data.type == 'Time Line') ? 'selected="selected"' : '') + '>Time Line</option><option value="Time Area" ' + ((opt_data.type == 'Time Area') ? 'selected="selected"' : '') + '>Time Area</option></select></div><div><span class="chart-dimension"><label for="chart-height">' + soy.$$escapeHtml("Height") + '</label><input class="text chart-height" name="chart-height" value="' + soy.$$escapeHtml(opt_data.height) + '"></span><span class="chart-dimension"><label for="chart-width">' + soy.$$escapeHtml("Width") + '</label><input class="text chart-width" name="chart-width" value="' + soy.$$escapeHtml(opt_data.width) + '"></span></div></form></div></div>' : '') + '<div class="chart-body ' + ((opt_data.isCloud) ? 'tf-cloud-table' : '') + '" ' + ((opt_data.hide) ? 'style="display: none;"' : '') + '>' + soy.$$filterNoAutoescape(opt_data.body) + '</div></div>';
};
if (goog.DEBUG) {
  ChartTemplate.view.soyTemplateName = 'ChartTemplate.view';
}


ChartTemplate.macrobrowserTabs = function(opt_data, opt_ignored) {
  return '<div class="aui-tabs horizontal-tabs tf-macrobrowser-tabs"><ul class="tabs-menu"><li id="tab-columns" class="menu-item"><a href="#">' + "Settings" + '</a></li><li id="tab-display-options" class="menu-item"><a href="#">' + "Look" + '</a></li><li id="tab-advanced-options" class="menu-item"><a href="#">' + "Adjustments" + '</a></li></ul><div class="tabs-pane active-pane"></div></div>';
};
if (goog.DEBUG) {
  ChartTemplate.macrobrowserTabs.soyTemplateName = 'ChartTemplate.macrobrowserTabs';
}


ChartTemplate.macro = function(opt_data, opt_ignored) {
  return '<div class="dialog-panel-body macro-input-panel" style="height: 423px; padding: 0px;"><div id="macro-insert-container" style="display: block;"><input class="macro-name" type="hidden" value="table-chart"><div class="macro-input-fields dialog-panel"><form action="#" class="aui"><div class="macro-desc"><p>' + "Create bright and dynamically updated charts from table data." + '</p>' + "Post \x3ca href\x3d\x22https://stiltsoft.customercase.com/forums/table-filter-for-confluence/ideas/create?from\x3dmacro\x22\x3eIdea\x3c/a\x3e or \x3ca href\x3d\x22https://stiltsoft.customercase.com/forums/support-requests/ideas/create?from\x3dmacro\x26category\x3dTable%2BFilter%2Band%2BCharts%2Bfor%2BConfluence\x22\x3eIssue\x3c/a\x3e." + ' ' + "\x3ca class\x3d\x22macro-doco-link\x22 href\x3d\x22https://docs.stiltsoft.com/display/TableFilter/How+to+use+Table+chart+macro?from\x3dmacro\x22\x3eDocumentation\x3c/a\x3e" + '</div><div class="macro-param-div" id="macro-param-div-column"><label for="macro-param-column">' + "Labels column" + '</label><input type="text" class="text macro-param-input" id="macro-param-column"></div><div class="macro-param-div" id="macro-param-div-aggregation"><label for="macro-param-aggregation">' + "Values column" + '</label><input type="text" class="text macro-param-input" id="macro-param-aggregation"></div><div class="macro-param-div required" id="macro-param-div-type"><label for="macro-param-type">' + "Type" + ' *</label><select class="select macro-param-input" id="macro-param-type"><option value="Pie">Pie</option><option value="Donut">Donut</option><option value="3D Donut">3D Donut</option><option value="Column">Column</option><option value="Stacked Column">Stacked Column</option><option value="Bar">Bar</option><option value="Stacked Bar">Stacked Bar</option><option value="Line">Line</option><option value="Area">Area</option><option value="Time Line">Time Line</option><option value="Time Area">Time Area</option></select></div><div class="macro-param-div" id="macro-param-div-datepattern"><label for="macro-param-datepattern">' + "Date format" + '</label><input type="text" class="text macro-param-input" id="macro-param-datepattern"><div class="macro-param-desc">' + "Specify the \x3ca href\x3d\x22https://docs.stiltsoft.com/display/TableFilter/FAQ#FAQ-Supporteddateformats\x22 target\x3d\x22_blank\x22\x3edate format\x3c/a\x3e that will be used to read date values." + '</div></div><div class="macro-param-div required" id="macro-param-div-separator"><label for="macro-param-separator">' + "Decimal separator" + ' *</label><select class="select macro-param-input" id="macro-param-separator"><option value="Point (.)">Point (.)</option><option value="Comma (,)">Comma (,)</option></select></div><div class="macro-param-div" id="macro-param-div-tfc-height"><label for="macro-param-tfc-height">' + "Height" + '</label><input type="text" class="text macro-param-input" id="macro-param-tfc-height"></div><div class="macro-param-div" id="macro-param-div-tfc-width"><label for="macro-param-tfc-width">' + "Width" + '</label><input type="text" class="text macro-param-input" id="macro-param-tfc-width"><div class="macro-param-desc">' + "Specify the size of the chart in pixels. If empty, the chart will fit the page area." + '</div></div><div class="macro-param-div" id="macro-param-div-colors"><label for="macro-param-colors">' + "Select colors" + '</label><input type="text" class="text macro-param-input" id="macro-param-colors"></div><div class="macro-param-div required" id="macro-param-div-align"><label for="macro-param-align">' + "Align chart" + ' *</label><select class="select macro-param-input" id="macro-param-align"><option value="Center">Center</option><option value="Left">Left</option><option value="Right">Right</option></select></div><div class="macro-param-div required" id="macro-param-div-dataorientation"><label for="macro-param-dataorientation">' + "Table data orientation" + ' *</label><select class="select macro-param-input" id="macro-