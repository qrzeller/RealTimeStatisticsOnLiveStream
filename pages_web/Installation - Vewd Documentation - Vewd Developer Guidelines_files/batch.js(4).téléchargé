if (typeof PivotTemplate == 'undefined') { var PivotTemplate = {}; }


PivotTemplate.table = function(opt_data, opt_ignored) {
  var output = '<table class="confluenceTable"><thead><tr class="tf-header-hidden">' + ((opt_data.column.length == 0) ? '<th class="confluenceTh"></th>' : '');
  var cnameList7 = opt_data.column;
  var cnameListLen7 = cnameList7.length;
  for (var cnameIndex7 = 0; cnameIndex7 < cnameListLen7; cnameIndex7++) {
    var cnameData7 = cnameList7[cnameIndex7];
    output += '<th class="confluenceTh">' + soy.$$escapeHtml(cnameData7) + '</th>';
  }
  var tList12 = opt_data.type;
  var tListLen12 = tList12.length;
  for (var tIndex12 = 0; tIndex12 < tListLen12; tIndex12++) {
    var tData12 = tList12[tIndex12];
    if (tData12 != 'Count') {
      var aggrList15 = opt_data.aggregation;
      var aggrListLen15 = aggrList15.length;
      for (var aggrIndex15 = 0; aggrIndex15 < aggrListLen15; aggrIndex15++) {
        var aggrData15 = aggrList15[aggrIndex15];
        output += (opt_data.rows.length > 0) ? PivotTemplate.fakeCell({number: 0, columns: opt_data.rows, str: tData12 + ' of ' + aggrData15, total: opt_data.total}) : '<th class="confluenceTh">' + soy.$$escapeHtml(tData12) + ' of ' + soy.$$escapeHtml(aggrData15) + '</th>';
      }
    } else {
      output += (opt_data.rows.length > 0) ? PivotTemplate.fakeCell({number: 0, columns: opt_data.rows, str: tData12, total: opt_data.total}) : '<th class="confluenceTh">' + soy.$$escapeHtml(tData12) + '</th>';
    }
  }
  output += '</tr>';
  var colspan__soy44 = '' + PivotTemplate.calc({number: 0, array: opt_data.rows, value: 1});
  var antiColspan__soy49 = '' + PivotTable.calcAntiRowspan({n: 0, total: opt_data.total, cols: opt_data.rows});
  var newColspan__soy54 = colspan__soy44 - antiColspan__soy49;
  var rowspan__soy55 = 1 + opt_data.rows.length;
  output += '<tr class="pivot-header"><th class="confluenceTh" rowspan="' + soy.$$escapeHtml(rowspan__soy55) + '" colspan="' + soy.$$escapeHtml(opt_data.cols.length) + '">' + soy.$$escapeHtml(opt_data.name) + '</th>';
  var tList63 = opt_data.type;
  var tListLen63 = tList63.length;
  for (var tIndex63 = 0; tIndex63 < tListLen63; tIndex63++) {
    var tData63 = tList63[tIndex63];
    if (tData63 != 'Count') {
      var aggrList66 = opt_data.aggregation;
      var aggrListLen66 = aggrList66.length;
      for (var aggrIndex66 = 0; aggrIndex66 < aggrListLen66; aggrIndex66++) {
        var aggrData66 = aggrList66[aggrIndex66];
        output += '<th class="confluenceTh" colspan="' + soy.$$escapeHtml(newColspan__soy54) + '">' + ((opt_data.rows.length > 0) ? '<span class="aui-icon aui-icon-small aui-iconfont-expanded collapser"></span>' : '') + soy.$$escapeHtml(tData63) + ' of ' + soy.$$escapeHtml(aggrData66) + '</th>';
      }
    } else {
      output += '<th class="confluenceTh" colspan="' + soy.$$escapeHtml(newColspan__soy54) + '">' + ((opt_data.rows.length > 0) ? '<span class="aui-icon aui-icon-small aui-iconfont-expanded collapser"></span>' : '') + soy.$$escapeHtml(tData63) + '</th>';
    }
  }
  output += '</tr>';
  var rowList89 = opt_data.rows;
  var rowListLen89 = rowList89.length;
  for (var rowIndex89 = 0; rowIndex89 < rowListLen89; rowIndex89++) {
    var rowData89 = rowList89[rowIndex89];
    var colspan__soy90 = '' + PivotTemplate.calc({number: rowIndex89 + 1, array: opt_data.rows, value: 1});
    output += '<tr class="pivot-header">';
    var tList96 = opt_data.type;
    var tListLen96 = tList96.length;
    for (var tIndex96 = 0; tIndex96 < tListLen96; tIndex96++) {
      var tData96 = tList96[tIndex96];
      if (tData96 != 'Count') {
        var aggrList99 = opt_data.aggregation;
        var aggrListLen99 = aggrList99.length;
        for (var aggrIndex99 = 0; aggrIndex99 < aggrListLen99; aggrIndex99++) {
          var aggrData99 = aggrList99[aggrIndex99];
          output += PivotTemplate.headerRow({n: rowIndex89, number: 0, total: opt_data.total, row: rowData89, rows: opt_data.rows, colspan: colspan__soy90});
        }
      } else {
        output += PivotTemplate.headerRow({n: rowIndex89, number: 0, total: opt_data.total, row: rowData89, rows: opt_data.rows, colspan: colspan__soy90});
      }
    }
    output += '</tr>';
  }
  output += '</thead><tbody><tr>' + PivotTemplate.processCols({number: 0, total: opt_data.total, baseTotal: opt_data.total, cols: opt_data.cols, rows: opt_data.rows, types: opt_data.type, aggregations: opt_data.aggregation}) + '</tr></tbody></table>';
  return output;
};
if (goog.DEBUG) {
  PivotTemplate.table.soyTemplateName = 'PivotTemplate.table';
}


PivotTemplate.headerCell = function(opt_data, opt_ignored) {
  var output = '';
  var colList130 = opt_data.row;
  var colListLen130 = colList130.length;
  for (var colIndex130 = 0; colIndex130 < colListLen130; colIndex130++) {
    var colData130 = colList130[colIndex130];
    var antiColspan__soy131 = '' + PivotTable.calcAntiRowspan({n: opt_data.n + 1, total: opt_data.total[colData130], cols: opt_data.rows});
    var newColspan__soy136 = opt_data.colspan - antiColspan__soy131;
    if (opt_data.total[colData130]) {
      output += '<th class="confluenceTh" colspan="' + soy.$$escapeHtml(newColspan__soy136) + '">' + ((newColspan__soy136 > 1) ? '<span class="aui-icon aui-icon-small aui-iconfont-expanded collapser"></span>' : '');
      var isImg__soy145 = '' + PivotTable.isImg({name: colData130});
      output += ((isImg__soy145 == 'true') ? soy.$$filterNoAutoescape(colData130) : soy.$$escapeHtml(colData130)) + '</th>';
    }
  }
  var antiColspan__soy156 = '' + PivotTable.calcAntiRowspan({n: opt_data.n + 1, total: opt_data.total, cols: opt_data.rows});
  var newColspan__soy161 = opt_data.colspan - antiColspan__soy156;
  output += '<th class="confluenceTh" colspan="' + soy.$$escapeHtml(newColspan__soy161) + '">' + ((newColspan__soy161 > 1) ? '<span class="aui-icon aui-icon-small aui-iconfont-expanded collapser"></span>' : '') + soy.$$escapeHtml("Total") + '</th>';
  return output;
};
if (goog.DEBUG) {
  PivotTemplate.headerCell.soyTemplateName = 'PivotTemplate.headerCell';
}


PivotTemplate.headerRow = function(opt_data, opt_ignored) {
  var output = '';
  if (opt_data.n > opt_data.number) {
    var nameList173 = opt_data.rows[opt_data.number];
    var nameListLen173 = nameList173.length;
    for (var nameIndex173 = 0; nameIndex173 < nameListLen173; nameIndex173++) {
      var nameData173 = nameList173[nameIndex173];
      output += (opt_data.total[nameData173]) ? PivotTemplate.headerRow({n: opt_data.n, number: opt_data.number + 1, total: opt_data.total[nameData173], row: opt_data.row, rows: opt_data.rows, colspan: opt_data.colspan}) : '';
    }
    output += PivotTemplate.headerRow({n: opt_data.n, number: opt_data.number + 1, total: opt_data.total, row: opt_data.row, rows: opt_data.rows, colspan: opt_data.colspan});
  } else {
    output += PivotTemplate.headerCell(opt_data);
  }
  return output;
};
if (goog.DEBUG) {
  PivotTemplate.headerRow.soyTemplateName = 'PivotTemplate.headerRow';
}


PivotTemplate.fakeCell = function(opt_data, opt_ignored) {
  var output = '';
  var nameList199 = opt_data.columns[opt_data.number];
  var nameListLen199 = nameList199.length;
  for (var nameIndex199 = 0; nameIndex199 < nameListLen199; nameIndex199++) {
    var nameData199 = nameList199[nameIndex199];
    if (opt_data.total[nameData199]) {
      var newStr__soy202 = opt_data.str + ' ' + nameData199;
      var newNumber__soy203 = opt_data.number + 1;
      output += (newNumber__soy203 < opt_data.columns.length) ? PivotTemplate.fakeCell({number: newNumber__soy203, columns: opt_data.columns, str: newStr__soy202, total: opt_data.total[nameData199]}) : '<th class="confluenceTh">' + soy.$$escapeHtml(newStr__soy202) + '</th>';
    }
  }
  var newNumber__soy216 = opt_data.number + 1;
  output += (newNumber__soy216 < opt_data.columns.length) ? PivotTemplate.fakeCell({number: newNumber__soy216, columns: opt_data.columns, str: opt_data.str, total: opt_data.total}) : '<th class="confluenceTh pivot-total-column">' + soy.$$escapeHtml(opt_data.str) + '</th>';
  return output;
};
if (goog.DEBUG) {
  PivotTemplate.fakeCell.soyTemplateName = 'PivotTemplate.fakeCell';
}


PivotTemplate.calc = function(opt_data, opt_ignored) {
  var output = '';
  var newValue__soy229 = opt_data.number < opt_data.array.length ? opt_data.value * (opt_data.array[opt_data.number].length + 1) : opt_data.value;
  var newNumber__soy230 = opt_data.number + 1;
  output += (newNumber__soy230 < opt_data.array.length) ? PivotTemplate.calc({number: newNumber__soy230, array: opt_data.array, value: newValue__soy229}) : soy.$$escapeHtml(newValue__soy229);
  return output;
};
if (goog.DEBUG) {
  PivotTemplate.calc.soyTemplateName = 'PivotTemplate.calc';
}


PivotTemplate.processCols = function(opt_data, opt_ignored) {
  var output = '';
  var newNumber__soy240 = opt_data.number + 1;
  var rowspan__soy241 = '' + PivotTemplate.calc({number: newNumber__soy240, array: opt_data.cols, value: 1});
  if (opt_data.number < opt_data.cols.length) {
    var nameList248 = opt_data.cols[opt_data.number];
    var nameListLen248 = nameList248.length;
    for (var nameIndex248 = 0; nameIndex248 < nameListLen248; nameIndex248++) {
      var nameData248 = nameList248[nameIndex248];
      var antiRowspan__soy249 = '' + PivotTable.calcAntiRowspan({n: newNumber__soy240, total: opt_data.total[nameData248], cols: opt_data.cols});
      var newRowspan__soy254 = rowspan__soy241 - antiRowspan__soy249;
      if (opt_data.total[nameData248]) {
        output += '<th class="confluenceTh" rowspan="' + soy.$$escapeHtml(newRowspan__soy254) + '">' + ((newNumber__soy240 < opt_data.cols.length) ? '<span class="aui-icon aui-icon-small aui-iconfont-expanded collapser"></span>' : '');
        var isImg__soy263 = '' + PivotTable.isImg({name: nameData248});
        output += ((isImg__soy263 == 'true') ? soy.$$filterNoAutoescape(nameData248) : soy.$$escapeHtml(nameData248)) + '</th>' + ((newNumber__soy240 < opt_data.cols.length) ? PivotTemplate.processCols({number: newNumber__soy240, total: opt_data.total[nameData248], baseTotal: opt_data.baseTotal, cols: opt_data.cols, rows: opt_data.rows, types: opt_data.types, aggregations: opt_data.aggregations}) : PivotTemplate.processType({total: opt_data.total[nameData248], baseTotal: opt_data.baseTotal, rows: opt_data.rows, types: opt_data.types, aggregations: opt_data.aggregations}));
      }
    }
  }
  var antiRowspan__soy291 = '' + PivotTable.calcAntiRowspan({n: newNumber__soy240, total: opt_data.total, cols: opt_data.cols});
  var newRowspan__soy296 = rowspan__soy241 - antiRowspan__soy291;
  output += '<th class="confluenceTh th-total" rowspan="' + soy.$$escapeHtml(newRowspan__soy296) + '">' + ((newNumber__soy240 < opt_data.cols.length) ? '<span class="aui-icon aui-icon-small aui-iconfont-expanded collapser"></span>' : '') + soy.$$escapeHtml("Total") + '</th>' + ((newNumber__soy240 < opt_data.cols.length) ? PivotTemplate.processCols({number: newNumber__soy240, total: opt_data.total, baseTotal: opt_data.baseTotal, cols: opt_data.cols, rows: opt_data.rows, types: opt_data.types, aggregations: opt_data.aggregations}) : PivotTemplate.processType(opt_data));
  return output;
};
if (goog.DEBUG) {
  PivotTemplate.processCols.soyTemplateName = 'PivotTemplate.processCols';
}


PivotTemplate.processType = function(opt_data, opt_ignored) {
  var output = '';
  var typeList323 = opt_data.types;
  var typeListLen323 = typeList323.length;
  for (var typeIndex323 = 0; typeIndex323 < typeListLen323; typeIndex323++) {
    var typeData323 = typeList323[typeIndex323];
    if (typeData323 != 'Count') {
      var aggrList326 = opt_data.aggregations;
      var aggrListLen326 = aggrList326.length;
      for (var aggrIndex326 = 0; aggrIndex326 < aggrListLen326; aggrIndex326++) {
        var aggrData326 = aggrList326[aggrIndex326];
        output += PivotTemplate.processRows({number: 0, total: opt_data.total, baseTotal: opt_data.baseTotal, aggr: aggrData326, type: typeData323, rows: opt_data.rows});
      }
    } else {
      output += PivotTemplate.processRows({number: 0, total: opt_data.total, baseTotal: opt_data.baseTotal, aggr: opt_data.aggregations[0], type: typeData323, rows: opt_data.rows});
    }
  }
  output += '</tr><tr>';
  return output;
};
if (goog.DEBUG) {
  PivotTemplate.processType.soyTemplateName = 'PivotTemplate.processType';
}


PivotTemplate.processRows = function(opt_data, opt_ignored) {
  var output = '';
  var newNumber__soy346 = opt_data.number + 1;
  if (opt_data.number < opt_data.rows.length) {
    var nameList349 = opt_data.rows[opt_data.number];
    var nameListLen349 = nameList349.length;
    for (var nameIndex349 = 0; nameIndex349 < nameListLen349; nameIndex349++) {
      var nameData349 = nameList349[nameIndex349];
      if (opt_data.baseTotal[nameData349]) {
        var newTotal__soy352 = opt_data.total[nameData349] ? opt_data.total[nameData349] : [];
        if (newNumber__soy346 < opt_data.rows.length) {
          output += PivotTemplate.processRows({number: newNumber__soy346, total: newTotal__soy352, baseTotal: opt_data.baseTotal[nameData349], rows: opt_data.rows, type: opt_data.type, aggr: opt_data.aggr});
        } else {
          var value__soy363 = newTotal__soy352[opt_data.aggr] ? newTotal__soy352[opt_data.aggr][opt_data.type] : '';
          output += '<td class="confluenceTh pivot-number">' + soy.$$escapeHtml(value__soy363) + '</td>';
        }
      }
    }
  }
  if (newNumber__soy346 < opt_data.rows.length) {
    output += PivotTemplate.processRows({number: newNumber__soy346, total: opt_data.total, baseTotal: opt_data.baseTotal, rows: opt_data.rows, type: opt_data.type, aggr: opt_data.aggr});
  } else {
    var value__soy378 = opt_data.total[opt_data.aggr] ? opt_data.total[opt_data.aggr][opt_data.type] : '';
    output += '<td class="confluenceTh pivot-number" ' + ((opt_data.rows.length > 0) ? 'style="font-weight: bold"' : '') + '>' + soy.$$escapeHtml(value__soy378) + '</td>';
  }
  return output;
};
if (goog.DEBUG) {
  PivotTemplate.processRows.soyTemplateName = 'PivotTemplate.processRows';
}


