TableFilterTemplate.addFilterDialog = function(opt_data, opt_ignored) {
  var output = '<form class="aui top-label inline-dialog-form" style="padding: 10px">';
  if (opt_data.filters.length > 0 || opt_data.globalfilter || opt_data.columnhider) {
    var filterList20 = opt_data.filters;
    var filterListLen20 = filterList20.length;
    for (var filterIndex20 = 0; filterIndex20 < filterListLen20; filterIndex20++) {
      var filterData20 = filterList20[filterIndex20];
      output += '<div class="' + soy.$$escapeHtml(filterData20.type) + ' tf-add-filter-column"><label for="' + soy.$$escapeHtml(filterData20.type) + '">' + soy.$$escapeHtml("Filter Column") + '</label><select class="select ' + soy.$$escapeHtml(filterData20.type) + '" name="' + soy.$$escapeHtml(filterData20.type) + '">';
      var columnList32 = filterData20.columns;
      var columnListLen32 = columnList32.length;
      for (var columnIndex32 = 0; columnIndex32 < columnListLen32; columnIndex32++) {
        var columnData32 = columnList32[columnIndex32];
        output += '<option value="' + soy.$$escapeHtml(columnData32) + '">' + soy.$$escapeHtml(columnData32) + '</option>';
      }
      output += '</select></div>';
    }
    output += '<label for="filter-type">' + soy.$$escapeHtml("Filter Type") + '</label><select class="select filter-type" name="filter-type">';
    var filterList44 = opt_data.filters;
    var filterListLen44 = filterList44.length;
    for (var filterIndex44 = 0; filterIndex44 < filterListLen44; filterIndex44++) {
      var filterData44 = filterList44[filterIndex44];
      output += '<option value="' + soy.$$escapeHtml(filterData44.type) + '">' + soy.$$escapeHtml(filterData44.name) + '</option>';
    }
    output += ((opt_data.globalfilter) ? '<option value="globalfilter">' + soy.$$escapeHtml("Global Filter") + '</option>' : '') + ((opt_data.columnhider) ? '<option value="columnhider">' + soy.$$escapeHtml("Hide Columns") + '</option>' : '') + '</select><div class="buttons-container"><div class="buttons"><input class="aui-button" type="button" value="' + soy.$$escapeHtml("Add filter") + '" id="save"/><a class="cancel" id="cancel">' + soy.$$escapeHtml("Cancel") + '</a></div></div>';
  } else {
    output += '<div class="aui-message info"><span class="aui-icon icon-info"></span>' + soy.$$escapeHtml("Filters are not available") + '</div><div class="buttons-container"><div class="buttons"><a class="cancel" id="cancel">' + soy.$$escapeHtml("Cancel") + '</a></div></div>';
  }
  output += '</form>';
  return output;
};
if (goog.DEBUG) {
  TableFilterTemplate.addFilterDialog.soyTemplateName = 'TableFilterTemplate.addFilterDialog';
}


