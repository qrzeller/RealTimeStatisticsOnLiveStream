/* module-key = 'com.stiltsoft.confluence.plugin.tablefilter.tablefilter:macro-browser-resources', location = 'js/tablefilter-macro-browser.js' */
(function($) {

    var tablefilterMacroJsOverride;

    var TablefilterMacroJsOverride = function() {
        this.tableFilterMacro = new TablefilterMacro();
    };

    TablefilterMacroJsOverride.prototype.getMacroDetailsFromSelectedMacro = function(metadataList, macro) {
        var self = tablefilterMacroJsOverride.tableFilterMacro;
        self.getMacroDetails(macro.body);
    };

    TablefilterMacroJsOverride.prototype.postPreview = function(iframe) {
        var self = tablefilterMacroJsOverride.tableFilterMacro;
        self.updateColumns($(iframe).contents());
    };

    TablefilterMacroJsOverride.prototype.beforeParamsSet = function(params) {
        var self = tablefilterMacroJsOverride.tableFilterMacro;
        if (!self.gotBody) {
            tablefilterMacroJsOverride.getMacroDetailsFromSelectedMacro(null, {body: tinymce.activeEditor.selection.getContent()});
        }
        self.gotBody = false;

        if (params["numbering"] == "true") {
            params["numbering"] = "Dynamic Ascending";
        } else if (params["numbering"] == "Dynamic") {
            params["numbering"] = "Dynamic Ascending";
        }

        params['id'] = new Date().getTime() + "_" + TableUtils.hashCode(self.tfBody);

        self.transformElements(params);

        if (params["column"] == "") params["column"] = " ";
        if (params["userfilter"] == "") params["userfilter"] = " ";
        if (params["iconfilter"] == "") params["iconfilter"] = " ";
        if (params["datefilter"] == "") params["datefilter"] = " ";
        if (params["numberfilter"] == "") params["numberfilter"] = " ";

        return params;
    };

    TablefilterMacroJsOverride.prototype.beforeParamsRetrieved = function(params) {
        var self = tablefilterMacroJsOverride.tableFilterMacro;

        if (!self.chosenUpdated) {
            self.columnSelect.trigger('chosen:updated');
            self.userfilterSelect.trigger('chosen:updated');
            self.datefilterSelect.trigger('chosen:updated');
            self.numberfilterSelect.trigger('chosen:updated');
            self.iconfilterSelect.trigger('chosen:updated');
            self.sortSelect.trigger('chosen:updated');
            TableUtils.sortSelectedOptions(self.columnSelect, self.columnParams);
            TableUtils.sortSelectedOptions(self.userfilterSelect, self.userfilterParams);
            TableUtils.sortSelectedOptions(self.datefilterSelect, self.datefilterParams);
            TableUtils.sortSelectedOptions(self.numberfilterSelect, self.numberfilterParams);
            TableUtils.sortSelectedOptions(self.iconfilterSelect, self.iconfilterParams);
            TableUtils.sortSelectedOptions(self.sortSelect, self.sortParams);
            self.chosenUpdated = true;
        }

        return self.getComputationalParams(params);
    };

    tablefilterMacroJsOverride = new TablefilterMacroJsOverride();
    AJS.MacroBrowser.setMacroJsOverride("table-filter", tablefilterMacroJsOverride);

})(AJS.$);

}catch(e){WRMCB(e)};
