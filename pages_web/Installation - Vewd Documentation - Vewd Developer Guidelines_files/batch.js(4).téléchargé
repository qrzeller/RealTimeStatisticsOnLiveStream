if (typeof Confluence == 'undefined') { var Confluence = {}; }
if (typeof Confluence.Templates == 'undefined') { Confluence.Templates = {}; }
if (typeof Confluence.Templates.Editor == 'undefined') { Confluence.Templates.Editor = {}; }
if (typeof Confluence.Templates.Editor.Reliable == 'undefined') { Confluence.Templates.Editor.Reliable = {}; }


Confluence.Templates.Editor.Reliable.draftMessage = function(opt_data, opt_ignored) {
  var output = '<div id="draft-messages">';
  if (opt_data.isNewPage) {
    if (opt_data.existingDraft.title) {
      var escapedTitle__soy8 = '' + soy.$$escapeHtml(opt_data.existingDraft.title);
      output += soy.$$filterNoAutoescape(AJS.format("Une nouvelle page que vous avez essay\xe9 d\x27\x27ajouter sur {0} appel\xe9e \x26#8216;{1}\x26#8217; a \xe9t\xe9 enregistr\xe9e dans vos brouillons.",opt_data.existingDraft.date,escapedTitle__soy8));
    } else {
      output += soy.$$filterNoAutoescape(AJS.format("Une nouvelle page que vous avez ajout\xe9e \xe0 {0} a \xe9t\xe9 enregistr\xe9e dans vos brouillons.",opt_data.existingDraft.date));
    }
    output += ' ' + soy.$$filterNoAutoescape(AJS.format("Souhaitez-vous {0}reprendre les modifications{1} ou {2}les annuler{3}\x26nbsp;?",'<a href="#" class="use-draft"> ','</a>','<a href="#" class="discard-draft"> ','</a>'));
  } else {
    output += soy.$$filterNoAutoescape(AJS.format("Une version de cette page en cours de modification sur {0} a \xe9t\xe9 enregistr\xe9e dans vos brouillons.",opt_data.existingDraft.date)) + ((opt_data.mergeRequired) ? ' ' + soy.$$escapeHtml("La page a \xe9t\xe9 mise \xe0 jour depuis. Vos modifications seront fusionn\xe9es avec la derni\xe8re version.") : '') + ((opt_data.conflictFound) ? ' ' + soy.$$filterNoAutoescape(AJS.format("La page a \xe9t\xe9 mise \xe0 jour depuis. Les modifications apport\xe9es entrent en conflit avec vos modifications et ne peuvent \xeatre fusionn\xe9es. Souhaitez-vous {0}afficher les donn\xe9es conflictuelles{1} ou {2}annuler{3} vos modifications\x26nbsp;?",'<a href="?pageId=' + opt_data.pageId + '&viewConflict=true&spaceKey=' + opt_data.spaceKey + '" >','</a>','<a href="#" class="discard-draft">','</a>','<a href="' + "" + '/users/viewmydrafts.action">','</a>')) : (opt_data.mergeRequired) ? ' ' + soy.$$filterNoAutoescape(AJS.format("Souhaitez-vous {0}afficher{1}, {2}fusionner et reprendre les modifications{3} ou {4}les annuler{5} ?",'<a id="view-diff-link-notification" href="#" class="view-diff-link">','</a>','<a href="#" class="use-draft"> ','</a>','<a href="#" class="discard-draft">','</a>')) : ' ' + soy.$$filterNoAutoescape(AJS.format("Souhaitez-vous {0}afficher le changement{1}, {2}reprendre les modifications{3} ou {4}les annuler{5} ?",'<a id="view-diff-link-notification" href="#" class="view-diff-link">','</a>','<a href="#" class="use-draft"> ','</a>','<a href="#" class="discard-draft">','</a>')));
  }
  output += '</div>';
  return output;
};
if (goog.DEBUG) {
  Confluence.Templates.Editor.Reliable.draftMessage.soyTemplateName = 'Confluence.Templates.Editor.Reliable.draftMessage';
}

